local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("TRIPLE WARP LOCKER", "Default")

-- [ Services ]
local Player = game:GetService("Players").LocalPlayer

-- [ Config ]
local Config = {
    AutoTP = false,
    Delay = 0.1, -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡∏™‡∏•‡∏±‡∏ö‡∏à‡∏∏‡∏î (‡∏õ‡∏£‡∏±‡∏ö‡πÑ‡∏î‡πâ)
    Locations = {
        CFrame.new(2.11108589, 3.14999819, -894.264893),   -- ‡∏à‡∏∏‡∏î‡∏ó‡∏µ‡πà 1
        CFrame.new(12.6919117, 3.14999771, -909.917358),  -- ‡∏à‡∏∏‡∏î‡∏ó‡∏µ‡πà 2
        CFrame.new(-11.9985313, 3.14999723, -907.327576)  -- ‡∏à‡∏∏‡∏î‡∏ó‡∏µ‡πà 3
    }
}

-- [ ‡∏£‡∏∞‡∏ö‡∏ö Auto Warp ‡∏ß‡∏ô 3 ‡∏à‡∏∏‡∏î ]
task.spawn(function()
    while true do
        if Config.AutoTP then
            for _, pos in ipairs(Config.Locations) do
                if not Config.AutoTP then break end -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤‡∏õ‡∏¥‡∏î‡∏´‡∏£‡∏∑‡∏≠‡∏¢‡∏±‡∏á
                pcall(function()
                    if Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") then
                        Player.Character.HumanoidRootPart.CFrame = pos
                        Player.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                    end
                end)
                task.wait(Config.Delay) -- ‡∏î‡∏µ‡πÄ‡∏•‡∏¢‡πå‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á‡∏Å‡∏≤‡∏£‡∏™‡∏•‡∏±‡∏ö‡∏à‡∏∏‡∏î
            end
        else
            task.wait(0.5) -- ‡∏û‡∏±‡∏Å‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô
        end
    end
end)

--- [ UI Setup ] ---
local tab = DrRayLibrary.newTab("Teleport", "rbxassetid://4483345998")

tab.newToggle("Auto Warp 3 Points", "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏™‡∏•‡∏±‡∏ö 3 ‡∏û‡∏¥‡∏Å‡∏±‡∏î‡∏£‡∏±‡∏ß‡πÜ", false, function(state)
    Config.AutoTP = state
    if state then
        warn("üìç ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ß‡∏ô 3 ‡∏à‡∏∏‡∏î!")
    else
        warn("üõë ‡∏´‡∏¢‡∏∏‡∏î‡∏ß‡∏≤‡∏£‡πå‡∏õ")
    end
end)

tab.newSlider("Warp Speed", "‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏Å‡∏≤‡∏£‡∏™‡∏•‡∏±‡∏ö‡∏à‡∏∏‡∏î", 10, true, 1, function(v)
    -- ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß (‡∏¢‡∏¥‡πà‡∏á‡∏ô‡πâ‡∏≠‡∏¢‡∏¢‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß)
    Config.Delay = v / 10
end)

print("‚úÖ Triple Warp Script Loaded!")
