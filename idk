tab.newLabel("Outfits", "เปลี่ยนชุดตัวละคร")

tab.newButton("Test Basic", "ทดสอบชุดพื้นฐาน (โค้ดที่ให้มา)", function()
    applyOutfit3()
end)

tab.newButton("Wear Outfit 1", "ใส่ชุด 1 (โค้ดใหม่ที่ให้มา)", function()
    local success = applyOutfit1()
    if not success then
        print("⚠️ ลองใส่ชุดอีกครั้ง...")
        wait(0.5)
        applyOutfit1()
    end
end)

