-- ระบบเรนโบว์ที่สามารถปรับความเร็วได้
tab.newLabel("Rainbow System", "Continuous rainbow color cycling")

-- Toggle สำหรับเปิด/ปิดระบบเรนโบว์
tab.newToggle("Rainbow Mode", "Enable/Disable continuous rainbow cycling", false, function(state)
    rainbowEnabled = state
    if state then
        print("Rainbow mode enabled")
        startRainbowLoop()
    else
        print("Rainbow mode disabled")
        if rainbowThread then
            coroutine.close(rainbowThread)
            rainbowThread = nil
        end
    end
end)

-- Slider สำหรับปรับความเร็วของเรนโบว์
tab.newSlider("Rainbow Speed", "Speed of color change (0.1-5 seconds)", 5, true, 0.5, function(value)
    rainbowSpeed = value
    print("Rainbow speed set to: " .. value .. " seconds per color")
    
    -- ถ้าระบบเรนโบว์กำลังทำงานอยู่ ให้รีสตาร์ทด้วยความเร็วใหม่
    if rainbowEnabled then
        if rainbowThread then
            coroutine.close(rainbowThread)
        end
        startRainbowLoop()
    end
end)

-- ปุ่มสำหรับเริ่มระบบเรนโบว์แบบครั้งเดียว (ไม่วนลูป)
tab.newButton("Single Rainbow Cycle", "Cycle through preset colors once", function()
    local colors = {
        "Smoky grey",
        "Institutional white",
        "Carnation pink",
        "Bright orange",
        "New Yeller",
        "Lime green",
        "Cyan"
    }
    
    for i, colorName in ipairs(colors) do
        workspace.DRadio_Script.Color:FireServer(BrickColor.new(colorName))
        print("Applied " .. colorName)
        wait(0.5) -- รอครึ่งวินาทีระหว่างเปลี่ยนสี
    end
    print("Rainbow cycle complete!")
end)

-- ตัวเลือกสีแบบสุ่ม
tab.newButton("Random Color", "Apply random color", function()
    local randomColor = Color3.new(math.random(), math.random(), math.random())
    workspace.DRadio_Script.Color:FireServer(BrickColor.new(randomColor))
    print("Applied random color")
end)

-- ระบบเรนโบว์แบบ Hue (โทนสีต่อเนื่อง)
tab.newLabel("Advanced Rainbow", "Smooth rainbow hue cycling")

-- ฟังก์ชันสำหรับระบบเรนโบว์แบบ Hue
local hueRainbowEnabled = false
local hueRainbowSpeed = 0.05
local hueRainbowThread = nil

local function startHueRainbowLoop()
    if hueRainbowThread then
        coroutine.close(hueRainbowThread)
    end
    
    hueRainbowThread = coroutine.create(function()
        local hue = 0
        while hueRainbowEnabled do
            -- แปลงค่า Hue (0-1) เป็น Color3
            local color = Color3.fromHSV(hue, 1, 1)
            workspace.DRadio_Script.Color:FireServer(BrickColor.new(color))
            
            -- เพิ่มค่า Hue
            hue = hue + 0.01
            if hue > 1 then
                hue = 0
            end
            
            wait(hueRainbowSpeed)
        end
        hueRainbowThread = nil
    end)
    
    coroutine.resume(hueRainbowThread)
end

-- Toggle สำหรับเปิด/ปิดระบบเรนโบว์แบบ Hue
tab.newToggle("Smooth Rainbow Mode", "Enable/Disable smooth hue cycling", false, function(state)
    hueRainbowEnabled = state
    if state then
        print("Smooth rainbow mode enabled")
        startHueRainbowLoop()
    else
        print("Smooth rainbow mode disabled")
        if hueRainbowThread then
            coroutine.close(hueRainbowThread)
            hueRainbowThread = nil
        end
    end
end)

-- Slider สำหรับปรับความเร็วของเรนโบว์แบบ Hue
tab.newSlider("Hue Rainbow Speed", "Speed of hue change (0.01-0.5 seconds)", 0.5, true, 0.05, function(value)
    hueRainbowSpeed = value
    print("Hue rainbow speed set to: " .. value .. " seconds per update")
    
    -- ถ้าระบบเรนโบว์แบบ Hue กำลังทำงานอยู่ ให้รีสตาร์ทด้วยความเร็วใหม่
    if hueRainbowEnabled then
        if hueRainbowThread then
            coroutine.close(hueRainbowThread)
        end
        startHueRainbowLoop()
    end
end)

-- ฟังก์ชันเริ่มระบบเรนโบว์สำหรับบล็อก (จากโค้ดเดิม)
local function startRainbowLoop()
    if rainbowThread then
        coroutine.close(rainbowThread)
    end
    
    rainbowThread = coroutine.create(function()
        local colors = {
            "Smoky grey",
            "Institutional white",
            "Carnation pink",
            "Bright orange",
            "New Yeller",
            "Lime green",
            "Cyan"
        }
        
        local index = 1
        while rainbowEnabled do
            workspace.DRadio_Script.Color:FireServer(BrickColor.new(colors[index]))
            print("Rainbow: Applied " .. colors[index])
            
            index = index + 1
            if index > #colors then
                index = 1
            end
            
            wait(rainbowSpeed)
        end
        rainbowThread = nil
    end)
    
    coroutine.resume(rainbowThread)
end

-- เพิ่มปุ่มจัดการพื้นหลัง
tab.newLabel("Background Settings", "ตั้งค่าพื้นหลัง UI")

tab.newButton("เปลี่ยนภาพพื้นหลัง", "เปลี่ยนภาพพื้นหลัง UI", function()
    if backgroundGui then
        -- ใช้ Asset ID อื่นๆ ถ้าต้องการ
        local newAssetId = "137084206837999" -- ใช้เดิมหรือเปลี่ยนตามต้องการ
        local bgImage = backgroundGui:FindFirstChild("MainBackground")
        if bgImage then
            local imageLabel = bgImage:FindFirstChild("BackgroundImage")
            if imageLabel then
                imageLabel.Image = "rbxassetid://" .. newAssetId
                print("✓ เปลี่ยนภาพพื้นหลังเป็น Asset ID: " .. newAssetId)
            end
        end
    else
        print("⚠️ ไม่พบ GUI พื้นหลัง กำลังสร้างใหม่...")
        backgroundGui = setupBackground()
    end
end)

tab.newToggle("แสดง/ซ่อนพื้นหลัง", "เปิด/ปิดการแสดงผลพื้นหลัง", true, function(state)
    if backgroundGui then
        backgroundGui.Enabled = state
        if state then
            print("✓ แสดงพื้นหลังแล้ว")
        else
            print("✓ ซ่อนพื้นหลังแล้ว")
        end
    else
        print("⚠️ ไม่พบ GUI พื้นหลัง")
    end
end)

tab.newButton("ลบพื้นหลัง", "ลบภาพพื้นหลังออก", function()
    if backgroundGui then
        backgroundGui:Destroy()
        backgroundGui = nil
        print("✓ ลบพื้นหลังออกแล้ว")
    else
        print("⚠️ ไม่พบ GUI พื้นหลังที่จะลบ")
    end
end)

tab.newButton("สร้างพื้นหลังใหม่", "สร้างภาพพื้นหลังใหม่", function()
    if backgroundGui then
        backgroundGui:Destroy()
    end
    backgroundGui = setupBackground()
end)

