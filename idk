        -- ‡∏£‡∏≠‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡∏£‡∏∞‡∏°‡∏ß‡∏•‡∏ú‡∏•
        wait(0.5)
        
        -- ‡∏•‡∏≠‡∏á‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏ï‡∏¥‡∏°‡∏ñ‡πâ‡∏≤ Remote ‡∏ô‡∏µ‡πâ‡∏°‡∏µ
        pcall(function()
            if game:GetService("ReplicatedStorage"):FindFirstChild("BloxbizRemotes") then
                -- ‡∏™‡∏µ‡∏à‡∏≤‡∏Å‡πÇ‡∏Ñ‡πâ‡∏î (‡πÅ‡∏õ‡∏•‡∏á‡∏à‡∏≤‡∏Å RGB 0-255 ‡πÄ‡∏õ‡πá‡∏ô 0-1)
                local r = 234.00000125169754 / 255
                local g = 184.00000423192978 / 255
                local b = 146.00000649690628 / 255
                
                game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyToRealHumanoid:FireServer({
                    ["BodyColor"] = {
                        ["HeadColor"] = Color3.new(r, g, b),
                        ["TorsoColor"] = Color3.new(r, g, b),
                        ["LeftArmColor"] = Color3.new(r, g, b),
                        ["RightLegColor"] = Color3.new(r, g, b),
                        ["LeftLegColor"] = Color3.new(r, g, b),
                        ["RightArmColor"] = Color3.new(r, g, b)
                    }
                })
                print("‚úì ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏ï‡∏¥‡∏°‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ä‡∏∏‡∏î 5")
            end
        end)
    else
        print("‚úó ‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 5 ‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à: " .. tostring(errorMsg))
        
        -- ‡∏•‡∏≠‡∏á‡πÉ‡∏ä‡πâ‡∏ß‡∏¥‡∏ò‡∏µ‡πÄ‡∏Å‡πà‡∏≤‡∏ñ‡πâ‡∏≤‡∏ß‡∏¥‡∏ò‡∏µ‡πÉ‡∏´‡∏°‡πà‡πÑ‡∏°‡πà‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
        print("‚ö†Ô∏è ‡∏•‡∏≠‡∏á‡πÉ‡∏ä‡πâ‡∏ß‡∏¥‡∏ò‡∏µ‡πÄ‡∏Å‡πà‡∏≤‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ä‡∏∏‡∏î 5...")
        wait(0.5)
        
        local success2, error2 = pcall(function()
            -- ‡πÅ‡∏õ‡∏•‡∏á‡∏™‡∏µ‡∏à‡∏≤‡∏Å‡πÇ‡∏Ñ‡πâ‡∏î (RGB 0-255 ‡πÄ‡∏õ‡πá‡∏ô Color3 0-1)
            local r = 234.00000125169754 / 255
            local g = 184.00000423192978 / 255
            local b = 146.00000649690628 / 255
            
            game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyOutfit:FireServer({
                ["WalkAnimation"] = 0,
                ["Accessories"] = {
                    {
                        ["Rotation"] = Vector3.new(0, 0, 0),
                        ["AssetId"] = 323476364, -- ‡πÄ‡∏™‡πâ‡∏ô‡∏ú‡∏°‡∏à‡∏≤‡∏Å HairAccessory
                        ["Position"] = Vector3.new(0, 0, 0),
                        ["Scale"] = Vector3.new(1, 1, 1),
                        ["IsLayered"] = false,
                        ["AccessoryType"] = Enum.AccessoryType.Hair
                    }
                },
                ["RightLegColor"] = Color3.new(r, g, b),
                ["MoodAnimation"] = 14618207727,
                ["LeftLegColor"] = Color3.new(r, g, b),
                ["JumpAnimation"] = 0,
                ["ProportionScale"] = 1,
                ["BodyTypeScale"] = 1,
                ["ClimbAnimation"] = 0,
                ["LeftArmColor"] = Color3.new(r, g, b),
                ["Face"] = 0,
                ["Pants"] = 14764019035,
                ["RightArmColor"] = Color3.new(r, g, b),
                ["DepthScale"] = 1,
                ["WidthScale"] = 0.8500000238418579,
                ["SwimAnimation"] = 0,
                ["RightArm"] = 32357584,
                ["GraphicTShirt"] = 0,
                ["Head"] = 18557883987,
                ["Shirt"] = 14401847977,
                ["RightLeg"] = 27493718,
                ["RunAnimation"] = 0,
                ["Torso"] = 27493604,
                ["FallAnimation"] = 0,
                ["TorsoColor"] = Color3.new(r, g, b),
                ["IdleAnimation"] = 0,
                ["LeftArm"] = 32357619,
                ["HeadScale"] = 1,
                ["HeightScale"] = 0.800000011920929,
                ["HeadColor"] = Color3.new(r, g, b),
                ["LeftLeg"] = 27493683
            })
        end)
        
        if success2 then
            print("‚úì ‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 5 ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡∏î‡πâ‡∏ß‡∏¢‡∏ß‡∏¥‡∏ò‡∏µ‡πÄ‡∏Å‡πà‡∏≤")
            wait(0.3)
            
            pcall(function()
                -- ‡∏™‡∏µ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ä‡∏∏‡∏î 5
                local r = 234.00000125169754 / 255
                local g = 184.00000423192978 / 255
                local b = 146.00000649690628 / 255
                
                game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyToRealHumanoid:FireServer({
                    ["BodyColor"] = {
                        ["HeadColor"] = Color3.new(r, g, b),
                        ["TorsoColor"] = Color3.new(r, g, b),
                        ["LeftArmColor"] = Color3.new(r, g, b),
                        ["RightLegColor"] = Color3.new(r, g, b),
                        ["LeftLegColor"] = Color3.new(r, g, b),
                        ["RightArmColor"] = Color3.new(r, g, b)
                    }
                })
                print("‚úì ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ä‡∏∏‡∏î 5")
            end)
        else
            print("‚úó ‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 5 ‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á‡∏ß‡∏¥‡∏ò‡∏µ: " .. tostring(error2))
        end
    end
    
    return success
end

