        player.Character:BreakJoints()
        print("‚úì ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÅ‡∏•‡πâ‡∏ß")
        wait(2)
        print("‚úì ‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ‡∏•‡∏≠‡∏á‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î‡πÉ‡∏´‡∏°‡πà‡πÑ‡∏î‡πâ")
    end
end)

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏°‡∏µ RemoteEvent ‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
tab.newButton("Check Remotes", "‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö RemoteEvent ‡∏ó‡∏µ‡πà‡∏°‡∏µ", function()
    print("=== ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö RemoteEvent ‡πÉ‡∏ô ReplicatedStorage ===")
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö CatalogGuiRemote
    local CatalogGuiRemote = game:GetService("ReplicatedStorage"):FindFirstChild("CatalogGuiRemote")
    if CatalogGuiRemote then
        print("‚úì ‡∏û‡∏ö CatalogGuiRemote (" .. CatalogGuiRemote.ClassName .. ")")
    else
        print("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö CatalogGuiRemote")
    end
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö BloxbizRemotes
    local BloxbizRemotes = game:GetService("ReplicatedStorage"):FindFirstChild("BloxbizRemotes")
    if BloxbizRemotes then
        print("‚úì ‡∏û‡∏ö BloxbizRemotes")
        for _, child in ipairs(BloxbizRemotes:GetChildren()) do
            print("  - " .. child.Name .. " (" .. child.ClassName .. ")")
        end
    else
        print("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö BloxbizRemotes")
    end
    print("=====================================")
end)

-- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö RGB Color Picker
tab.newLabel("RGB Color Picker", "Adjust RGB values to change block color")

tab.newSlider("Red", "Red value (0-255)", 255, false, function(value)
    colorValues.r = value / 255
    print("Red set to: " .. value)
end)

tab.newSlider("Green", "Green value (0-255)", 255, false, function(value)
    colorValues.g = value / 255
    print("Green set to: " .. value)
end)

tab.newSlider("Blue", "Blue value (0-255)", 255, false, function(value)
    colorValues.b = value / 255
    print("Blue set to: " .. value)
end)

tab.newButton("Apply Color", "Apply current RGB color to block", function()
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Color3 ‡∏à‡∏≤‡∏Å‡∏Ñ‡πà‡∏≤ RGB ‡πÅ‡∏•‡∏∞‡πÅ‡∏õ‡∏•‡∏á‡πÄ‡∏õ‡πá‡∏ô BrickColor
    local color = Color3.new(colorValues.r, colorValues.g, colorValues.b)
    workspace.DRadio_Script.Color:FireServer(BrickColor.new(color))
    print("Applied color: R=" .. math.floor(colorValues.r * 255) .. " G=" .. math.floor(colorValues.g * 255) .. " B=" .. math.floor(colorValues.b * 255))
end)

