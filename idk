game:GetService("ReplicatedStorage").NameTagRemoteEvent:FireServer("name",Color3.new(0.755553, 0.967509, 0.211566),38)

local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("DrRay", "Default")

-- สร้างฟังก์ชันสำหรับเพิ่มพื้นหลังให้กับ GUI
local function setupBackground()
    -- ตรวจสอบว่าเป็นเกม Roblox หรือไม่
    if game:GetService("RunService"):IsStudio() then
        print("อยู่ใน Roblox Studio - พื้นหลังอาจไม่แสดงผล")
    end
    
    -- ตรวจสอบว่า CoreGui มีอยู่หรือไม่
    local coreGui = game:GetService("CoreGui")
    if not coreGui then
        warn("ไม่พบ CoreGui Service")
        return
    end
    
    -- Asset ID จากลิงก์ที่คุณให้มา
    local backgroundAssetId = "137084206837999"
    
    -- สร้าง ScreenGui สำหรับพื้นหลัง
    local backgroundGui = Instance.new("ScreenGui")
    backgroundGui.Name = "RayUIBackground"
    backgroundGui.DisplayOrder = -1 -- ทำให้อยู่ด้านหลัง
    backgroundGui.IgnoreGuiInset = true -- ขยายเต็มหน้าจอ
    backgroundGui.ResetOnSpawn = false
    
    -- สร้าง Frame เป็นพื้นหลังหลัก
    local backgroundFrame = Instance.new("Frame")
    backgroundFrame.Name = "MainBackground"
    backgroundFrame.Size = UDim2.new(1, 0, 1, 0) -- ขนาดเต็มหน้าจอ
    backgroundFrame.Position = UDim2.new(0, 0, 0, 0)
    backgroundFrame.BackgroundTransparency = 1 -- โปร่งใส
    
    -- สร้าง ImageLabel สำหรับภาพพื้นหลัง
    local backgroundImage = Instance.new("ImageLabel")
    backgroundImage.Name = "BackgroundImage"
    backgroundImage.Size = UDim2.new(1, 0, 1, 0) -- ขนาดเต็ม Frame
    backgroundImage.Position = UDim2.new(0, 0, 0, 0)
    backgroundImage.Image = "rbxassetid://" .. backgroundAssetId
    backgroundImage.BackgroundTransparency = 1
    backgroundImage.ScaleType = Enum.ScaleType.Crop -- ตัดให้พอดี
    backgroundImage.Parent = backgroundFrame
    
    -- สร้าง Gradient สำหรับให้สีสวยขึ้น
    local gradient = Instance.new("UIGradient")
    gradient.Name = "BackgroundGradient"
    gradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 0, 0)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(20, 20, 20)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 0, 0))
    })
    gradient.Rotation = 90
    gradient.Transparency = NumberSequence.new(0.3) -- ความโปร่งใส
    gradient.Parent = backgroundFrame
    
    -- เพิ่มเอฟเฟกต์เบลอ (ถ้าเกมรองรับ)
    local blurEffect = Instance.new("BlurEffect")
    blurEffect.Name = "BackgroundBlur"
    blurEffect.Size = 2 -- ความเบลอ
    blurEffect.Parent = backgroundFrame
    
    backgroundFrame.Parent = backgroundGui
    backgroundGui.Parent = coreGui
    
    print("✓ สร้างพื้นหลัง Ray UI สำเร็จ (Asset ID: " .. backgroundAssetId .. ")")
    
    return backgroundGui
end

-- เรียกใช้ฟังก์ชันสร้างพื้นหลัง
local backgroundGui = setupBackground()

local tab = DrRayLibrary.newTab("My Tab", "ImageIdHere")

-- ตัวแปรเก็บค่าสี RGB
local colorValues = {r = 1, g = 1, b = 1}

-- ตัวแปรสำหรับระบบเรนโบว์
local rainbowEnabled = false
local rainbowSpeed = 0.5  -- ค่าเริ่มต้น 0.5 วินาทีต่อสี
local rainbowThread = nil

-- ฟังก์ชันสำหรับระบบเรนโบว์ NameTag
local nameTagRainbow = false
local nameTagRainbowSpeed = 0.2
local nameTagRainbowThread = nil

-- ตัวแปรสำหรับระบบวิ่งเร็วและกระโดดสูง
local speedEnabled = false
local jumpEnabled = false
local currentWalkSpeed = 16  -- ค่าเริ่มต้นปกติ
local currentJumpPower = 50  -- ค่าเริ่มต้นปกติ
local speedValue = 50  -- ค่าความเร็วเริ่มต้น
local jumpValue = 100  -- ค่ากระโดดเริ่มต้น
local speedLoop = nil
local jumpLoop = nil

-- ฟังก์ชันเริ่มระบบเรนโบว์ NameTag
local function startNameTagRainbow()
    if nameTagRainbowThread then
        coroutine.close(nameTagRainbowThread)
    end
    
    nameTagRainbowThread = coroutine.create(function()
        local hue = 0
        while nameTagRainbow do
            -- คำนวณสีจากค่า Hue (0-1)
            local color = Color3.fromHSV(hue, 1, 1)
            
            -- ส่งสีไปยัง NameTag (ชื่อคงที่ที่คุณบอกว่ามีให้เปลี่ยนในเกมแล้ว)
            -- ใช้เลข 38 ตามที่คุณให้มา
            game:GetService("ReplicatedStorage").NameTagRemoteEvent:FireServer("name", color, 38)
            print("เปลี่ยนสี NameTag เป็น: " .. tostring(color))
            
            -- เปลี่ยนค่า Hue สำหรับสีถัดไป
            hue = hue + 0.01
            if hue > 1 then
                hue = 0
            end
            
            wait(nameTagRainbowSpeed)
        end
        nameTagRainbowThread = nil
    end)
    
    coroutine.resume(nameTagRainbowThread)
end

-- ฟังก์ชันตั้งค่าความเร็ว
local function applySpeed()
    if speedLoop then
        speedLoop:Disconnect()
        speedLoop = nil
    end
    
    speedLoop = game:GetService("RunService").Heartbeat:Connect(function()
        if speedEnabled then
            local player = game.Players.LocalPlayer
            if player and player.Character then
                local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid.WalkSpeed = speedValue
                end
            end
        end
    end)
end

-- ฟังก์ชันตั้งค่ากระโดดสูง
local function applyJump()
    if jumpLoop then
        jumpLoop:Disconnect()
        jumpLoop = nil
    end
    
    jumpLoop = game:GetService("RunService").Heartbeat:Connect(function()
        if jumpEnabled then
            local player = game.Players.LocalPlayer
            if player and player.Character then
                local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid.JumpPower = jumpValue
                end
            end
        end
    end)
end

-- ฟังก์ชันรีเซ็ตตัวละครแล้วตั้งค่าความเร็ว/กระโดดใหม่
local function resetCharacterSpeedJump()
    if speedEnabled then
        applySpeed()
    end
    if jumpEnabled then
        applyJump()
    end
end

-- เชื่อมต่อกับเหตุการณ์เมื่อตัวละครเกิดใหม่
game.Players.LocalPlayer.CharacterAdded:Connect(function()
    wait(1)  -- รอให้ตัวละครโหลดเสร็จ
    resetCharacterSpeedJump()
end)

tab.newButton("Button", "Prints Hello!", function()
    print('Hello!')
end)

tab.newToggle("Toggle", "Toggle! (prints the state)", true, function(toggleState)
    if toggleState then
        print("On")
    else
        print("Off")
    end
end)

tab.newInput("Input", "Prints your input.", function(text)
    print("Entered text: " .. text)
end)

tab.newKeybind("Input Key", "Press the key to start; it will be printed out.", function(key)
    print(key)
end)

tab.newSlider("Slider", "Epic slider", 1000, false, function(num)
    print(num)
end)

-- เพิ่มระบบเรนโบว์สำหรับ NameTag
tab.newLabel("Rainbow NameTag", "ระบบเปลี่ยนสี NameTag แบบเรนโบว์")

-- Toggle สำหรับเปิด/ปิดระบบเรนโบว์ NameTag
tab.newToggle("เปิดเรนโบว์ NameTag", "เปิด/ปิดระบบเปลี่ยนสี NameTag แบบเรนโบว์", false, function(state)
    nameTagRainbow = state
    if state then
        print("เปิดระบบเรนโบว์ NameTag แล้ว!")
        startNameTagRainbow()
    else
        print("ปิดระบบเรนโบว์ NameTag แล้ว")
        if nameTagRainbowThread then
            coroutine.close(nameTagRainbowThread)
            nameTagRainbowThread = nil
        end
    end
end)

-- Slider สำหรับปรับความเร็ว
tab.newSlider("ความเร็วเรนโบว์", "ปรับความเร็วในการเปลี่ยนสี (0.05-1 วินาที)", 1, true, 0.2, function(value)
    nameTagRainbowSpeed = value
    print("ตั้งค่าความเร็วเรนโบว์ NameTag เป็น: " .. value .. " วินาที")
    
    -- ถ้าระบบเรนโบว์กำลังทำงานอยู่ ให้รีสตาร์ทด้วยความเร็วใหม่
    if nameTagRainbow then
        if nameTagRainbowThread then
            coroutine.close(nameTagRainbowThread)
        end
        startNameTagRainbow()
    end
end)

-- ปุ่มเปลี่ยนสี NameTag อย่างเดียว
tab.newButton("เปลี่ยนสี NameTag", "เปลี่ยนสี NameTag เป็นสีลานโบเชยๆ", function()
    local color = Color3.fromHSV(math.random(), 1, 1)
    game:GetService("ReplicatedStorage").NameTagRemoteEvent:FireServer("name", color, 38)
    print("เปลี่ยนสี NameTag เป็น: " .. tostring(color))
end)

-- ปุ่มปิดเรนโบว์ NameTag
tab.newButton("ปิดเรนโบว์ NameTag", "ปิดระบบเรนโบว์ NameTag", function()
    nameTagRainbow = false
    if nameTagRainbowThread then
        coroutine.close(nameTagRainbowThread)
        nameTagRainbowThread = nil
    end
    print("ปิดระบบเรนโบว์ NameTag แล้ว")
end)

-- เพิ่มระบบวิ่งเร็วและกระโดดสูง
tab.newLabel("Movement Hacks", "ระบบวิ่งเร็วและกระโดดสูง")

-- Toggle สำหรับเปิด/ปิดวิ่งเร็ว
tab.newToggle("วิ่งเร็ว", "เปิด/ปิดระบบวิ่งเร็ว (ค่าเริ่มต้น 50)", false, function(state)
    speedEnabled = state
    if state then
        print("เปิดระบบวิ่งเร็วแล้ว! ความเร็ว: " .. speedValue)
        applySpeed()
    else
        print("ปิดระบบวิ่งเร็วแล้ว")
        if speedLoop then
            speedLoop:Disconnect()
            speedLoop = nil
        end
        -- รีเซ็ตความเร็วเป็นปกติ
        local player = game.Players.LocalPlayer
        if player and player.Character then
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 16
            end
        end
    end
end)

-- Slider สำหรับปรับความเร็ว
tab.newSlider("ปรับความเร็ววิ่ง", "ปรับความเร็วในการวิ่ง (16-200)", 200, false, 50, function(value)
    speedValue = value
    print("ตั้งค่าความเร็ววิ่งเป็น: " .. value)
    
    -- ถ้าระบบวิ่งเร็วกำลังทำงานอยู่ ให้ตั้งค่าใหม่
    if speedEnabled then
        local player = game.Players.LocalPlayer
        if player and player.Character then
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = speedValue
            end
        end
    end
end)

-- Toggle สำหรับเปิด/ปิดกระโดดสูง
tab.newToggle("กระโดดสูง", "เปิด/ปิดระบบกระโดดสูง (ค่าเริ่มต้น 100)", false, function(state)
    jumpEnabled = state
    if state then
        print("เปิดระบบกระโดดสูงแล้ว! ความสูง: " .. jumpValue)
        applyJump()
    else
        print("ปิดระบบกระโดดสูงแล้ว")
        if jumpLoop then
            jumpLoop:Disconnect()
            jumpLoop = nil
        end
        -- รีเซ็ตการกระโดดเป็นปกติ
        local player = game.Players.LocalPlayer
        if player and player.Character then
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.JumpPower = 50
            end
        end
    end
end)

-- Slider สำหรับปรับความสูงกระโดด
tab.newSlider("ปรับความสูงกระโดด", "ปรับความสูงในการกระโดด (50-200)", 200, false, 100, function(value)
    jumpValue = value
    print("ตั้งค่าความสูงกระโดดเป็น: " .. value)
    
    -- ถ้าระบบกระโดดสูงกำลังทำงานอยู่ ให้ตั้งค่าใหม่
    if jumpEnabled then
        local player = game.Players.LocalPlayer
        if player and player.Character then
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.JumpPower = jumpValue
            end
        end
    end
end)

-- ปุ่มรีเซ็ตค่าการเคลื่อนไหว
tab.newButton("รีเซ็ตการเคลื่อนไหว", "รีเซ็ตความเร็วและการกระโดดเป็นค่าปกติ", function()
    speedEnabled = false
    jumpEnabled = false
    
    if speedLoop then
        speedLoop:Disconnect()
        speedLoop = nil
    end
    
    if jumpLoop then
        jumpLoop:Disconnect()
        jumpLoop = nil
    end
    
    local player = game.Players.LocalPlayer
    if player and player.Character then
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = 16
            humanoid.JumpPower = 50
        end
    end
    
    print("รีเซ็ตการเคลื่อนไหวเป็นค่าปกติแล้ว")
end)

-- ปุ่มตั้งค่าความเร็ว/กระโดดใหม่เมื่อตาย
tab.newButton("รีเซ็ตเมื่อตาย", "ตั้งค่าความเร็วและกระโดดใหม่เมื่อตาย", function()
    resetCharacterSpeedJump()
    print("ตั้งค่าความเร็ว/กระโดดใหม่สำเร็จ (เมื่อตายจะรีเซ็ตอัตโนมัติ)")
end)

