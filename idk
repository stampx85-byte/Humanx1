local bodyRainbowEnabled = false
local bodyRainbowSpeed = 0.2
local bodyRainbowThread = nil

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö RGB Rainbow ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
local function startBodyRainbowLoop()
    if bodyRainbowThread then
        coroutine.close(bodyRainbowThread)
    end
    
    bodyRainbowThread = coroutine.create(function()
        local hue = 0
        while bodyRainbowEnabled do
            -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏™‡∏µ‡∏à‡∏≤‡∏Å‡∏Ñ‡πà‡∏≤ Hue (0-1)
            local color = Color3.fromHSV(hue, 1, 1)
            
            -- ‡∏™‡πà‡∏á‡∏™‡∏µ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÇ‡∏î‡∏¢‡πÉ‡∏ä‡πâ RemoteEvent ‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô‡∏Å‡∏±‡∏ö‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ‡πÉ‡∏ô Outfit
            pcall(function()
                game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyToRealHumanoid:FireServer({["BodyColor"] = color})
            end)
            
            -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏Ñ‡πà‡∏≤ Hue ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏™‡∏µ‡∏ñ‡∏±‡∏î‡πÑ‡∏õ
            hue = hue + 0.01
            if hue > 1 then
                hue = 0
            end
            
            wait(bodyRainbowSpeed)
        end
        bodyRainbowThread = nil
    end)
    
    coroutine.resume(bodyRainbowThread)
end

-- ================================
-- ‡πÄ‡∏û‡∏¥‡πà‡∏° Firefly Script ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà
-- ================================
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local stopAllActions = false

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏™‡∏Å Fireflies (‡∏´‡∏±‡∏ß‡πÉ‡∏à‡∏´‡∏•‡∏±‡∏Å‡∏Ç‡∏≠‡∏á‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå)
local function spawnFireflies(count)
    if stopAllActions then return end
    
    -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÑ‡∏≠‡πÄ‡∏ó‡∏° Fireflies ‡πÉ‡∏ô‡∏ï‡∏±‡∏ß‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
    local fireflies = player.Backpack:FindFirstChild("Fireflies") or 
                     player.Character:FindFirstChild("Fireflies")
    
    if not fireflies then
        warn("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡∏° Fireflies ‡πÉ‡∏ô‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤‡∏´‡∏£‡∏∑‡∏≠‡πÉ‡∏ô‡∏ï‡∏±‡∏ß")
        return
    end
    
    local remote = fireflies:FindFirstChild("RemoteEvent")
    if not remote then
        warn("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö RemoteEvent ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏™‡∏Å")
        return
    end
    
    print("‚ö° ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÄ‡∏™‡∏Å " .. count .. " ‡∏ï‡∏±‡∏ß...")
    
    task.spawn(function()
        for i = 1, count do
            if stopAllActions then break end
            
            -- ‡∏™‡πà‡∏á‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡πÑ‡∏õ‡∏ó‡∏µ‡πà Server
            pcall(function()
                remote:FireServer()
            end)
            
            -- ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡πÄ‡∏™‡∏Å (‡∏ñ‡πâ‡∏≤‡∏ô‡πâ‡∏≠‡∏¢‡πÑ‡∏õ‡∏≠‡∏≤‡∏à‡∏à‡∏∞‡πÇ‡∏î‡∏ô‡πÄ‡∏ï‡∏∞ ‡∏´‡∏£‡∏∑‡∏≠‡∏ñ‡πâ‡∏≤‡∏°‡∏≤‡∏Å‡πÑ‡∏õ‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏à‡∏∞‡∏Ñ‡πâ‡∏≤‡∏á)
            if i % 100 == 0 then
                task.wait() -- ‡∏û‡∏±‡∏Å‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡∏ó‡∏∏‡∏Å‡πÜ 100 ‡∏ï‡∏±‡∏ß‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏•‡∏î‡∏†‡∏≤‡∏£‡∏∞‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á
            end
        end
        print("‚úÖ ‡πÄ‡∏™‡∏Å‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô!")
    end)
end

-- ‡∏£‡∏∞‡∏ö‡∏ö‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡πÅ‡∏ä‡∏ó (‡πÑ‡∏ß‡πâ‡∏û‡∏¥‡∏°‡∏û‡πå‡∏™‡∏±‡πà‡∏á‡∏Å‡∏≤‡∏£)
player.Chatted:Connect(function(message)
    local msg = string.lower(message)
    local parts = string.split(msg, " ")
    local cmd = parts[1]
    
    if cmd == "!firefly" then
        local amount = tonumber(parts[2]) or 100 -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô ‡∏à‡∏∞‡πÄ‡∏™‡∏Å 100
        spawnFireflies(amount)
        
    elseif cmd == "!fireflystop" then
        stopAllActions = true
        task.wait(0.5)
        stopAllActions = false
        print("‚èπÔ∏è ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡πÄ‡∏™‡∏Å‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡πÅ‡∏•‡πâ‡∏ß")
        
    elseif cmd == "!fireflyclear" then
        local cleared = 0
        for _, obj in pairs(workspace:GetDescendants()) do
            if string.find(string.lower(obj.Name), "firefly") then
                obj:Destroy()
                cleared = cleared + 1
            end
        end
        print("üßπ ‡∏•‡πâ‡∏≤‡∏á‡∏´‡∏¥‡πà‡∏á‡∏´‡πâ‡∏≠‡∏¢‡∏≠‡∏≠‡∏Å‡πÑ‡∏õ " .. cleared .. " ‡∏ï‡∏±‡∏ß")
    end
end)

print("--------------------------------------")
print("üî• Firefly Dub Script Loaded!")
print("!firefly [‡∏à‡∏≥‡∏ô‡∏ß‡∏ô] - ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏™‡∏Å")
print("!fireflystop     - ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏´‡∏¢‡∏∏‡∏î")
print("!fireflyclear    - ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏•‡∏ö‡∏´‡∏¥‡πà‡∏á‡∏´‡πâ‡∏≠‡∏¢‡∏ó‡∏µ‡πà‡πÄ‡∏™‡∏Å‡∏°‡∏≤‡πÅ‡∏•‡πâ‡∏ß")
print("--------------------------------------")
