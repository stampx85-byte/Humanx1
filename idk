    if player and player.Character then
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = 16
            humanoid.JumpPower = 50
        end
    end
    
    print("‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏´‡∏ß‡πÄ‡∏õ‡πá‡∏ô‡∏Ñ‡πà‡∏≤‡∏õ‡∏Å‡∏ï‡∏¥‡πÅ‡∏•‡πâ‡∏ß")
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß/‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏≤‡∏¢
tab.newButton("‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏≤‡∏¢", "‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏•‡∏∞‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏≤‡∏¢", function()
    resetCharacterSpeedJump()
    print("‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß/‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÉ‡∏´‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à (‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏≤‡∏¢‡∏à‡∏∞‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥)")
end)

-- ================================
-- ‡πÄ‡∏û‡∏¥‡πà‡∏° Firefly UI Tab ‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà
-- ================================
local fireflyTab = DrRayLibrary.newTab("Firefly", "ImageIdHere")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Firefly
local fireflyCount = 100
local fireflySpawning = false

fireflyTab.newLabel("Firefly Spawner", "‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏™‡∏Å Fireflies ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥")

-- Input ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏à‡∏≥‡∏ô‡∏ß‡∏ô
fireflyTab.newInput("‡∏à‡∏≥‡∏ô‡∏ß‡∏ô Fireflies", "‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô Fireflies ‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏™‡∏Å", function(text)
    local num = tonumber(text)
    if num and num > 0 then
        fireflyCount = num
        print("‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏à‡∏≥‡∏ô‡∏ß‡∏ô Fireflies ‡πÄ‡∏õ‡πá‡∏ô: " .. fireflyCount)
    else
        print("‚ö†Ô∏è ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÉ‡∏™‡πà‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á")
    end
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÄ‡∏™‡∏Å
fireflyTab.newButton("‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÄ‡∏™‡∏Å Fireflies", "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡πÄ‡∏™‡∏Å Fireflies ‡∏ï‡∏≤‡∏°‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡∏±‡πâ‡∏á‡πÑ‡∏ß‡πâ", function()
    if fireflySpawning then
        print("‚ö†Ô∏è ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏™‡∏Å‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏£‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô‡∏Å‡πà‡∏≠‡∏ô")
        return
    end
    
    fireflySpawning = true
    print("‚ö° ‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÄ‡∏™‡∏Å Fireflies " .. fireflyCount .. " ‡∏ï‡∏±‡∏ß...")
    
    spawnFireflies(fireflyCount)
    
    -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏™‡∏£‡πá‡∏à
    wait(2)
    fireflySpawning = false
    print("‚úÖ ‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÄ‡∏™‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ï‡πà‡∏≠‡πÑ‡∏õ‡πÅ‡∏•‡πâ‡∏ß")
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏´‡∏¢‡∏∏‡∏î
fireflyTab.newButton("‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡πÄ‡∏™‡∏Å", "‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡πÄ‡∏™‡∏Å‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏ó‡∏µ‡πà‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏≠‡∏¢‡∏π‡πà", function()
    stopAllActions = true
    task.wait(0.5)
    stopAllActions = false
    fireflySpawning = false
    print("‚èπÔ∏è ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡πÄ‡∏™‡∏Å‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡πÅ‡∏•‡πâ‡∏ß")
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏•‡πâ‡∏≤‡∏á
fireflyTab.newButton("‡∏•‡πâ‡∏≤‡∏á Fireflies", "‡∏•‡∏ö Fireflies ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡πÉ‡∏ô‡πÄ‡∏Å‡∏°", function()
    local cleared = 0
    for _, obj in pairs(workspace:GetDescendants()) do
        if string.find(string.lower(obj.Name), "firefly") then
            obj:Destroy()
            cleared = cleared + 1
        end
    end
    print("üßπ ‡∏•‡πâ‡∏≤‡∏á‡∏´‡∏¥‡πà‡∏á‡∏´‡πâ‡∏≠‡∏¢‡∏≠‡∏≠‡∏Å‡πÑ‡∏õ " .. cleared .. " ‡∏ï‡∏±‡∏ß")
end)

-- Slider ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏±‡∏ö‡∏à‡∏≥‡∏ô‡∏ß‡∏ô (‡∏ó‡∏≤‡∏á‡πÄ‡∏•‡∏∑‡∏≠‡∏Å)
fireflyTab.newSlider("‡∏õ‡∏£‡∏±‡∏ö‡∏à‡∏≥‡∏ô‡∏ß‡∏ô", "‡∏õ‡∏£‡∏±‡∏ö‡∏à‡∏≥‡∏ô‡∏ß‡∏ô Fireflies ‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏™‡∏Å (1-1000)", 1000, false, 100, function(value)
    fireflyCount = value
    print("‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏à‡∏≥‡∏ô‡∏ß‡∏ô Fireflies ‡πÄ‡∏õ‡πá‡∏ô: " .. fireflyCount)
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏™‡∏Å‡πÅ‡∏ö‡∏ö‡πÄ‡∏£‡πá‡∏ß (500 ‡∏ï‡∏±‡∏ß)
fireflyTab.newButton("‡πÄ‡∏™‡∏Å‡πÄ‡∏£‡πá‡∏ß (500 ‡∏ï‡∏±‡∏ß)", "‡πÄ‡∏™‡∏Å Fireflies 500 ‡∏ï‡∏±‡∏ß‡πÉ‡∏ô‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏µ‡∏¢‡∏ß", function()
    if fireflySpawning then
        print("‚ö†Ô∏è ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏™‡∏Å‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏£‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô‡∏Å‡πà‡∏≠‡∏ô")
        return
    end
    
    fireflySpawning = true
    spawnFireflies(500)
    
    wait(2)
    fireflySpawning = false
    print("‚úÖ ‡πÄ‡∏™‡∏Å 500 ‡∏ï‡∏±‡∏ß‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô")
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏™‡∏Å‡πÅ‡∏ö‡∏ö‡∏°‡∏≤‡∏Å (1000 ‡∏ï‡∏±‡∏ß)
fireflyTab.newButton("‡πÄ‡∏™‡∏Å‡∏°‡∏≤‡∏Å (1000 ‡∏ï‡∏±‡∏ß)", "‡πÄ‡∏™‡∏Å Fireflies 1000 ‡∏ï‡∏±‡∏ß‡πÉ‡∏ô‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏µ‡∏¢‡∏ß", function()
    if fireflySpawning then
        print("‚ö†Ô∏è ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏™‡∏Å‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏£‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô‡∏Å‡πà‡∏≠‡∏ô")
        return
    end
    
    fireflySpawning = true
    spawnFireflies(1000)
    
    wait(3)
    fireflySpawning = false
    print("‚úÖ ‡πÄ‡∏™‡∏Å 1000 ‡∏ï‡∏±‡∏ß‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô")
end)

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡∏° Fireflies
fireflyTab.newButton("‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö Fireflies", "‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏°‡∏µ‡πÑ‡∏≠‡πÄ‡∏ó‡∏° Fireflies ‡πÉ‡∏ô‡∏ï‡∏±‡∏ß‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà", function()
    local fireflies = player.Backpack:FindFirstChild("Fireflies") or 
                     player.Character:FindFirstChild("Fireflies")
    
    if fireflies then
        local remote = fireflies:FindFirstChild("RemoteEvent")
        if remote then
            print("‚úÖ ‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡∏° Fireflies ‡πÅ‡∏•‡∏∞ RemoteEvent ‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô!")
        else
            print("‚ö†Ô∏è ‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡∏° Fireflies ‡πÅ‡∏ï‡πà‡πÑ‡∏°‡πà‡∏û‡∏ö RemoteEvent")
        end
    else
        print("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡∏° Fireflies ‡πÉ‡∏ô‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤‡∏´‡∏£‡∏∑‡∏≠‡πÉ‡∏ô‡∏ï‡∏±‡∏ß")
        print("üí° ‡∏Ñ‡∏≥‡πÅ‡∏ô‡∏∞‡∏ô‡∏≥: ‡πÇ‡∏õ‡∏£‡∏î‡πÄ‡∏Å‡πá‡∏ö Fireflies ‡πÑ‡∏ß‡πâ‡πÉ‡∏ô‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤‡∏´‡∏£‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠‡πÑ‡∏ß‡πâ‡πÉ‡∏ô‡∏°‡∏∑‡∏≠‡∏Å‡πà‡∏≠‡∏ô‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
    end
end)

-- ================================
-- ‡∏à‡∏ö Firefly UI Tab
-- ================================

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 1 (‡πÉ‡∏ä‡πâ‡πÇ‡∏Ñ‡πâ‡∏î‡∏ó‡∏µ‡πà‡∏Ñ‡∏∏‡∏ì‡πÉ‡∏´‡πâ‡∏°‡∏≤‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á)
local function applyOutfit1()
    print("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 1 (‡∏à‡∏≤‡∏Å‡πÇ‡∏Ñ‡πâ‡∏î‡∏ó‡∏µ‡πà‡πÉ‡∏´‡πâ‡∏°‡∏≤)...")
    
    local success, errorMsg = pcall(function()
        -- Script generated by TurtleSpy, made by Intrer#0421
        game:GetService("ReplicatedStorage").CatalogGuiRemote:InvokeServer({
            ["Properties"] = {
                ["WalkAnimation"] = 0,
                ["MoodAnimation"] = 10647852134,
                ["Face"] = 0,
                ["ProportionScale"] = 0.5,
                ["ClimbAnimation"] = 0,
                ["Shirt"] = 3334218182,
                ["FaceAccessory"] = "4085923851",
                ["RightArmColor"] = {["b"] = 246.0000005364418, ["IsRGBTable"] = true, ["g"] = 246.0000005364418, ["r"] = 246.0000005364418},
                ["HairAccessory"] = "",
                ["RightArm"] = 32357584,
                ["Head"] = 17388507384,
                ["FallAnimation"] = 0,
                ["TorsoColor"] = {["b"] = 246.0000005364418, ["IsRGBTable"] = true, ["g"] = 246.0000005364418, ["r"] = 246.0000005364418},
                ["DepthScale"] = 0.8500000238418579,
                ["LeftArm"] = 32357619,
                ["HeightScale"] = 1.0499999523162842,
                ["LeftLeg"] = 27493683,
                ["RightLegColor"] = {["b"] = 246.0000005364418, ["IsRGBTable"] = true, ["g"] = 246.0000005364418, ["r"] = 246.0000005364418},
                ["LeftLegColor"] = {["b"] = 246.0000005364418, ["IsRGBTable"] = true, ["g"] = 246.0000005364418, ["r"] = 246.0000005364418},
                ["WidthScale"] = 0.699999988079071,
                ["BodyTypeScale"] = 0,
                ["RunAnimation"] = 0,
                ["LeftArmColor"] = {["b"] = 246.0000005364418, ["IsRGBTable"] = true, ["g"] = 246.0000005364418, ["r"] = 246.0000005364418},
                ["Pants"] = 6095618719,
                ["WaistAccessory"] = "",
                ["LayeredAccessories"] = {},
                ["AccessoryRefinements"] = {},
                ["ShouldersAccessory"] = "",
                ["NeckAccessory"] = "",
                ["HatAccessory"] = "16807152290",
                ["FrontAccessory"] = "",
                ["SwimAnimation"] = 0,
                ["HeadColor"] = {["b"] = 246.0000005364418, ["IsRGBTable"] = true, ["g"] = 246.0000005364418, ["r"] = 246.0000005364418},
                ["BackAccessory"] = "",
                ["IdleAnimation"] = 11600321661,
                ["Torso"] = 27121265,
                ["HeadScale"] = 1,
                ["JumpAnimation"] = 0,
                ["GraphicTShirt"] = 0,
                ["RightLeg"] = 27493718
            },
            ["Action"] = "CreateAndWearHumanoidDescription",
            ["RigType"] = EnumItem.new(Enum.HumanoidRigType.R615)
        })
    end)
    
    if success then
        print("‚úì ‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 1 ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à (‡πÉ‡∏ä‡πâ CatalogGuiRemote)")
        
        -- ‡∏£‡∏≠‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡∏£‡∏∞‡∏°‡∏ß‡∏•‡∏ú‡∏•
        wait(0.5)
        
        -- ‡∏•‡∏≠‡∏á‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏ï‡∏¥‡∏°‡∏ñ‡πâ‡∏≤ Remote ‡∏ô‡∏µ‡πâ‡∏°‡∏µ
        pcall(function()
            if game:GetService("ReplicatedStorage"):FindFirstChild("BloxbizRemotes") then
                game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyToRealHumanoid:FireServer({
                    ["BodyColor"] = {
                        ["HeadColor"] = Color3.new(0.964706, 0.964706, 0.964706),
                        ["TorsoColor"] = Color3.new(0.964706, 0.964706, 0.964706),
                        ["LeftArmColor"] = Color3.new(0.964706, 0.964706, 0.964706),
                        ["RightLegColor"] = Color3.new(0.964706, 0.964706, 0.964706),
                        ["LeftLegColor"] = Color3.new(0.964706, 0.964706, 0.964706),
                        ["RightArmColor"] = Color3.new(0.964706, 0.964706, 0.964706)
                    }
                })
                print("‚úì ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏ï‡∏¥‡∏°‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à")
            end
        end)
    else
        print("‚úó ‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 1 ‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à: " .. tostring(errorMsg))
        
        -- ‡∏•‡∏≠‡∏á‡πÉ‡∏ä‡πâ‡∏ß‡∏¥‡∏ò‡∏µ‡πÄ‡∏Å‡πà‡∏≤‡∏ñ‡πâ‡∏≤‡∏ß‡∏¥‡∏ò‡∏µ‡πÉ‡∏´‡∏°‡πà‡πÑ‡∏°‡πà‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
        print("‚ö†Ô∏è ‡∏•‡∏≠‡∏á‡πÉ‡∏ä‡πâ‡∏ß‡∏¥‡∏ò‡∏µ‡πÄ‡∏Å‡πà‡∏≤...")
        wait(0.5)
        
        local success2, error2 = pcall(function()
            game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyOutfit:FireServer({
                ["WalkAnimation"] = 0,
                ["Accessories"] = {
                    {
                        ["Rotation"] = Vector3.new(0, 0, 0),
                        ["AssetId"] = 1309911, -- ‡∏´‡∏°‡∏ß‡∏Å
