        pcall(function()
            if game:GetService("ReplicatedStorage"):FindFirstChild("BloxbizRemotes") then
                game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyToRealHumanoid:FireServer({
                    ["BodyColor"] = {
