end

-- ฟังก์ชันใส่ชุด 4 (ใช้โค้ดอันแรกที่คุณให้มา)
local function applyOutfit4()
    print("กำลังใส่ชุด 4 (จากโค้ดที่ให้มา)...")
    
    local success, errorMsg = pcall(function()
        -- Script generated by TurtleSpy, made by Intrer#0421
        game:GetService("ReplicatedStorage").CatalogGuiRemote:InvokeServer({
            ["Properties"] = {
                ["WalkAnimation"] = 11600331426,
                ["MoodAnimation"] = 10647852134,
                ["Face"] = 0,
                ["ProportionScale"] = 0,
                ["ClimbAnimation"] = 0,
                ["Shirt"] = 5574405836,
                ["FaceAccessory"] = "",
                ["RightArmColor"] = {["b"] = 0,["IsRGBTable"] = true,["g"] = 0,["r"] = 0},
                ["HairAccessory"] = "",
                ["RightArm"] = 32357584,
                ["Head"] = 14629530741,
                ["FallAnimation"] = 0,
                ["TorsoColor"] = {["b"] = 0,["IsRGBTable"] = true,["g"] = 0,["r"] = 0},
                ["DepthScale"] = 1,
                ["LeftArm"] = 32357619,
                ["HeightScale"] = 1,
                ["LeftLeg"] = 27493683,
                ["RightLegColor"] = {["b"] = 0,["IsRGBTable"] = true,["g"] = 0,["r"] = 0},
                ["LeftLegColor"] = {["b"] = 0,["IsRGBTable"] = true,["g"] = 0,["r"] = 0},
                ["WidthScale"] = 1,
                ["BodyTypeScale"] = 0,
                ["RunAnimation"] = 0,
                ["LeftArmColor"] = {["b"] = 0,["IsRGBTable"] = true,["g"] = 0,["r"] = 0},
                ["Pants"] = 4750575774,
                ["WaistAccessory"] = "",
                ["LayeredAccessories"] = {},
                ["AccessoryRefinements"] = {},
                ["ShouldersAccessory"] = "",
                ["NeckAccessory"] = "",
                ["HatAccessory"] = "13446702865",
                ["FrontAccessory"] = "",
                ["SwimAnimation"] = 0,
                ["HeadColor"] = {["b"] = 181.16014301776886,["IsRGBTable"] = true,["g"] = 0,["r"] = 228.50855112075806},
                ["BackAccessory"] = "",
                ["IdleAnimation"] = 11600321661,
                ["Torso"] = 27121265,
                ["HeadScale"] = 1,
                ["JumpAnimation"] = 0,
                ["GraphicTShirt"] = 0,
                ["RightLeg"] = 27493718
            },
            ["Action"] = "CreateAndWearHumanoidDescription",
            ["RigType"] = EnumItem.new(Enum.HumanoidRigType.R15)
        })
    end)
    
    if success then
        print("✓ ใส่ชุด 4 สำเร็จ (ใช้ CatalogGuiRemote)")
        
        -- รอเล็กน้อยเพื่อให้ระบบประมวลผล
        wait(0.5)
        
        -- ลองตั้งค่าสีเพิ่มเติมถ้า Remote นี้มี
        pcall(function()
            if game:GetService("ReplicatedStorage"):FindFirstChild("BloxbizRemotes") then
                -- สีจาก HeadColor ในโค้ด (แปลงจาก RGB 0-255 เป็น 0-1)
                local r = 228.50855112075806 / 255
                local g = 0 / 255
                local b = 181.16014301776886 / 255
                
                game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyToRealHumanoid:FireServer({
                    ["BodyColor"] = {
                        ["HeadColor"] = Color3.new(r, g, b),
                        ["TorsoColor"] = Color3.new(0, 0, 0),
                        ["LeftArmColor"] = Color3.new(0, 0, 0),
                        ["RightLegColor"] = Color3.new(0, 0, 0),
                        ["LeftLegColor"] = Color3.new(0, 0, 0),
                        ["RightArmColor"] = Color3.new(0, 0, 0)
                    }
                })
                print("✓ ตั้งค่าสีเพิ่มเติมสำเร็จสำหรับชุด 4")
            end
        end)
    else
        print("✗ ใส่ชุด 4 ไม่สำเร็จ: " .. tostring(errorMsg))
        
        -- ลองใช้วิธีเก่าถ้าวิธีใหม่ไม่ทำงาน
        print("⚠️ ลองใช้วิธีเก่าสำหรับชุด 4...")
        wait(0.5)
        
        local success2, error2 = pcall(function()
            -- แปลงสีจากโค้ด (RGB 0-255 เป็น Color3 0-1)
            local headR = 228.50855112075806 / 255
            local headG = 0 / 255
            local headB = 181.16014301776886 / 255
            
            game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyOutfit:FireServer({
                ["WalkAnimation"] = 11600331426,
                ["Accessories"] = {
                    {
                        ["Rotation"] = Vector3.new(0, 0, 0),
                        ["AssetId"] = 13446702865, -- หมวกจาก HatAccessory
                        ["Position"] = Vector3.new(0, 0, 0),
                        ["Scale"] = Vector3.new(1, 1, 1),
                        ["IsLayered"] = false,
                        ["AccessoryType"] = Enum.AccessoryType.Hat
                    }
                },
                ["RightLegColor"] = Color3.new(0, 0, 0),
                ["MoodAnimation"] = 10647852134,
                ["LeftLegColor"] = Color3.new(0, 0, 0),
                ["JumpAnimation"] = 0,
                ["ProportionScale"] = 0,
                ["BodyTypeScale"] = 0,
                ["ClimbAnimation"] = 0,
                ["LeftArmColor"] = Color3.new(0, 0, 0),
                ["Face"] = 0,
                ["Pants"] = 4750575774,
                ["RightArmColor"] = Color3.new(0, 0, 0),
                ["DepthScale"] = 1,
                ["WidthScale"] = 1,
                ["SwimAnimation"] = 0,
                ["RightArm"] = 32357584,
                ["GraphicTShirt"] = 0,
                ["Head"] = 14629530741,
                ["Shirt"] = 5574405836,
                ["RightLeg"] = 27493718,
                ["RunAnimation"] = 0,
                ["Torso"] = 27121265,
                ["FallAnimation"] = 0,
                ["TorsoColor"] = Color3.new(0, 0, 0),
                ["IdleAnimation"] = 11600321661,
                ["LeftArm"] = 32357619,
                ["HeadScale"] = 1,
                ["HeightScale"] = 1,
                ["HeadColor"] = Color3.new(headR, headG, headB),
                ["LeftLeg"] = 27493683
            })
        end)
        
        if success2 then
            print("✓ ใส่ชุด 4 สำเร็จด้วยวิธีเก่า")
            wait(0.3)
            
            pcall(function()
                -- สีสำหรับชุด 4
                local headR = 228.50855112075806 / 255
                local headG = 0 / 255
                local headB = 181.16014301776886 / 255
                
                game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyToRealHumanoid:FireServer({
                    ["BodyColor"] = {
                        ["HeadColor"] = Color3.new(headR, headG, headB),
                        ["TorsoColor"] = Color3.new(0, 0, 0),
                        ["LeftArmColor"] = Color3.new(0, 0, 0),
                        ["RightLegColor"] = Color3.new(0, 0, 0),
                        ["LeftLegColor"] = Color3.new(0, 0, 0),
                        ["RightArmColor"] = Color3.new(0, 0, 0)
                    }
                })
                print("✓ ตั้งค่าสีสำเร็จสำหรับชุด 4")
            end)
        else
            print("✗ ใส่ชุด 4 ไม่สำเร็จทั้งสองวิธี: " .. tostring(error2))
        end
    end
    
    return success
end

