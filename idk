                game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyToRealHumanoid:FireServer({
                    ["BodyColor"] = {
                        ["HeadColor"] = Color3.new(0.964706, 0.964706, 0.964706),
                        ["TorsoColor"] = Color3.new(0.964706, 0.964706, 0.964706),
                        ["LeftArmColor"] = Color3.new(0.964706, 0.964706, 0.964706),
                        ["RightLegColor"] = Color3.new(0.964706, 0.964706, 0.964706),
                        ["LeftLegColor"] = Color3.new(0.964706, 0.964706, 0.964706),
                        ["RightArmColor"] = Color3.new(0.964706, 0.964706, 0.964706)
                    }
                })
                print("✓ ตั้งค่าสีสำเร็จ")
            end)
        else
            print("✗ ใส่ชุดไม่สำเร็จทั้งสองวิธี: " .. tostring(error2))
        end
    end
    
    return success
end

-- ฟังก์ชันใส่ชุด 2 (สีเทาเข้ม) - เก็บส่วนประกอบครบทุกอย่าง
local function applyOutfit2()
    print("กำลังใส่ชุด 2 (สีเทาเข้ม)...")
    
    -- 1. ใส่ชุดหลัก
    local success1, error1 = pcall(function()
        game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyOutfit:FireServer({
            ["WalkAnimation"] = 0,
            ["Accessories"] = {
                {
                    ["Rotation"] = Vector3.new(0, 0, 0),
                    ["AssetId"] = 13700928975, -- หน้า
                    ["Position"] = Vector3.new(0, 0, 0),
                    ["Scale"] = Vector3.new(1, 1, 1),
                    ["IsLayered"] = false,
                    ["AccessoryType"] = Enum.AccessoryType.Face
                },
                {
                    ["Rotation"] = Vector3.new(0, 0, 0),
                    ["AssetId"] = 192557913, -- หลัง
                    ["Position"] = Vector3.new(0, 0, 0),
                    ["Scale"] = Vector3.new(1, 1, 1),
                    ["IsLayered"] = false,
                    ["AccessoryType"] = Enum.AccessoryType.Back
                }
            },
            ["RightLegColor"] = Color3.new(0.5, 0.5, 0.5), -- เทาเข้ม
            ["MoodAnimation"] = 10647852134,
            ["LeftLegColor"] = Color3.new(0.5, 0.5, 0.5),
            ["JumpAnimation"] = 0,
            ["ProportionScale"] = 0.5,
            ["BodyTypeScale"] = 0,
            ["ClimbAnimation"] = 0,
            ["LeftArmColor"] = Color3.new(0.5, 0.5, 0.5),
            ["Face"] = 7074814,
            ["Pants"] = 14540498087,
            ["RightArmColor"] = Color3.new(0.5, 0.5, 0.5),
            ["DepthScale"] = 0.8500000238418579,
            ["WidthScale"] = 0.699999988079071,
            ["SwimAnimation"] = 0,
            ["RightArm"] = 32357584,
            ["GraphicTShirt"] = 0,
            ["Head"] = 8330576,
            ["Shirt"] = 14648309042,
            ["RightLeg"] = 27493718,
            ["RunAnimation"] = 0,
            ["Torso"] = 27493604,
            ["FallAnimation"] = 0,
            ["TorsoColor"] = Color3.new(0.5, 0.5, 0.5),
            ["IdleAnimation"] = 11600321661,
            ["LeftArm"] = 32357619,
            ["HeadScale"] = 1,
            ["HeightScale"] = 1.0499999523162842,
            ["HeadColor"] = Color3.new(0.5, 0.5, 0.5),
            ["LeftLeg"] = 27493683
        })
    end)
    
    if success1 then
        print("✓ ใส่ชุดหลักสำเร็จ")
    else
        print("✗ ใส่ชุดหลักไม่สำเร็จ: " .. tostring(error1))
    end
    
    -- 2. รอเล็กน้อยแล้วตั้งค่าสี
    wait(0.3)
    
    local success2, error2 = pcall(function()
        game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyToRealHumanoid:FireServer({
            ["BodyColor"] = {
                ["HeadColor"] = Color3.new(0.5, 0.5, 0.5),
                ["TorsoColor"] = Color3.new(0.5, 0.5, 0.5),
                ["LeftArmColor"] = Color3.new(0.5, 0.5, 0.5),
                ["RightLegColor"] = Color3.new(0.5, 0.5, 0.5),
                ["LeftLegColor"] = Color3.new(0.5, 0.5, 0.5),
                ["RightArmColor"] = Color3.new(0.5, 0.5, 0.5)
            }
        })
    end)
    
    if success2 then
        print("✓ ตั้งค่าสีสำเร็จ")
    else
        print("✗ ตั้งค่าสีไม่สำเร็จ: " .. tostring(error2))
    end
    
    return success1 and success2
end

-- ฟังก์ชันใส่ชุด 3 (ทดสอบ - ใช้โค้ดที่คุณให้มาโดยตรง)
