local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("GOD MODE V4 - SUPREME", "Default")

-- [ Services ]
local Player = game:GetService("Players").LocalPlayer
local RS = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")

-- [ Global Config ]
local Config = {
    KillAura = false,
    AuraRange = 500,
    InstantSword = false, 
    InstantTrowel = false, 
    InstantBomb = false, 
    InstantRocket = false, 
    InstantSlingshot = false, 
    InstantSuperball = false, 
    InstantPaintball = false, 
    AntiFall = false,
    FollowPlayer = false,
    TargetName = "",
    LockTarget = false
}

-- [ Helper Functions ]
local function GetCharacter()
    return Player.Character or Player.CharacterAdded:Wait()
end

local function SetAntiFall(state)
    Config.AntiFall = state
    local char = GetCharacter()
    local hrp = char:WaitForChild("HumanoidRootPart")
    if state then
        if not hrp:FindFirstChild("AntiFallV") then
            local bv = Instance.new("BodyVelocity")
            bv.Name = "AntiFallV"
            bv.Velocity = Vector3.new(0, 0, 0)
            bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            bv.Parent = hrp
        end
    else
        if hrp:FindFirstChild("AntiFallV") then hrp.AntiFallV:Destroy() end
    end
end

