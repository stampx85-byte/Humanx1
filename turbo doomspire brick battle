local tabWarp = DrRayLibrary.newTab("‡∏£‡∏∞‡∏ö‡∏ö‡∏ß‡∏≤‡∏£‡πå‡∏õ/‡∏ó‡∏µ‡∏°", "rbxassetid://4483345998")
local tabFast = DrRayLibrary.newTab("‡∏¢‡∏¥‡∏á‡∏£‡∏±‡∏ß‡∏ô‡∏£‡∏Å‡πÅ‡∏ï‡∏Å", "rbxassetid://4483345998")

-- [[ Tab: ‡πÇ‡∏´‡∏°‡∏î‡πÇ‡∏´‡∏î‡∏™‡∏±‡∏™ & Kill Aura ]]
tabGod.newToggle("KILL AURA (‡∏ó‡∏±‡πà‡∏ß‡πÅ‡∏°‡∏û)", "‡πÄ‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡πâ‡∏ß‡∏Ñ‡∏ô‡∏ó‡∏±‡πâ‡∏á‡πÅ‡∏°‡∏û‡∏à‡∏∞‡πÇ‡∏î‡∏ô‡∏ü‡∏±‡∏ô", false, function(s) Config.KillAura = s end)
tabGod.newSlider("Aura Range", "‡∏£‡∏∞‡∏¢‡∏∞‡∏Å‡∏≤‡∏£‡∏Ü‡πà‡∏≤", 5000, false, function(v) Config.AuraRange = v end)
tabGod.newToggle("‡∏•‡∏≠‡∏¢‡∏ï‡∏±‡∏ß + ‡∏Å‡∏±‡∏ô‡∏Å‡∏£‡∏∞‡πÄ‡∏î‡πá‡∏ô", "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÅ‡∏•‡πâ‡∏ß‡∏ô‡∏¥‡πà‡∏á + ‡πÑ‡∏°‡πà‡∏Å‡∏£‡∏∞‡πÄ‡∏î‡πá‡∏ô", false, function(s) SetAntiFall(s) end)

-- [[ Tab: ‡∏£‡∏∞‡∏ö‡∏ö‡∏ß‡∏≤‡∏£‡πå‡∏õ & ‡∏ó‡∏µ‡∏° ]]
tabWarp.newButton("Warp Blue", "‡πÑ‡∏õ‡∏ê‡∏≤‡∏ô‡∏ô‡πâ‡∏≥‡πÄ‡∏á‡∏¥‡∏ô", function() GetCharacter().HumanoidRootPart.CFrame = CFrame.new(6.35, 49.15, 131.30) end)
tabWarp.newButton("Warp Red", "‡πÑ‡∏õ‡∏ê‡∏≤‡∏ô‡πÅ‡∏î‡∏á", function() GetCharacter().HumanoidRootPart.CFrame = CFrame.new(-130.32, 46.69, -4.03) end)
tabWarp.newButton("Warp Yellow", "‡πÑ‡∏õ‡∏ê‡∏≤‡∏ô‡πÄ‡∏´‡∏•‡∏∑‡∏≠‡∏á", function() GetCharacter().HumanoidRootPart.CFrame = CFrame.new(129.51, 69.90, 8.98) end)
tabWarp.newButton("Warp Green", "‡πÑ‡∏õ‡∏ê‡∏≤‡∏ô‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß", function() GetCharacter().HumanoidRootPart.CFrame = CFrame.new(-2.89, 30.27, -123.54) end)

--- [[ ‡∏£‡∏∞‡∏ö‡∏ö‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡πÅ‡∏ö‡∏ö‡∏•‡πâ‡∏≤‡∏á‡∏ö‡∏±‡∏Ñ (Destroy & Re-create) ]] ---
local currentDropdown = nil

local function CreatePlayerDropdown()
    -- ‡∏•‡∏ö‡∏≠‡∏±‡∏ô‡πÄ‡∏Å‡πà‡∏≤‡∏ó‡∏¥‡πâ‡∏á‡∏ñ‡πâ‡∏≤‡∏°‡∏µ (‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏•‡πâ‡∏≤‡∏á‡∏ä‡∏∑‡πà‡∏≠‡∏Ñ‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏≠‡∏Å)
    if currentDropdown then
        currentDropdown:Destroy() 
    end

    local p_list = {}
    for _, v in pairs(game.Players:GetPlayers()) do
        if v ~= Player then table.insert(p_list, v.Name) end
    end

    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÉ‡∏´‡∏°‡πà‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
    currentDropdown = tabWarp.newDropdown("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô", "‡∏Ñ‡∏ô‡∏≠‡∏≠‡∏Å‡∏à‡∏∞‡∏´‡∏≤‡∏¢‡πÑ‡∏õ ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤‡∏à‡∏∞‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏°‡∏≤", p_list, function(selected)
        Config.TargetName = selected
    end)
end

-- ‡∏õ‡∏∏‡πà‡∏°‡∏Å‡∏î‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä (Manual)
tabWarp.newButton("üîÑ ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠ (‡∏•‡πâ‡∏≤‡∏á‡∏Ñ‡∏ô‡∏≠‡∏≠‡∏Å/‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤)", "‡∏Å‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î", function()
    CreatePlayerDropdown()
end)

-- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÅ‡∏£‡∏Å‡∏ï‡∏≠‡∏ô‡∏£‡∏±‡∏ô‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå
CreatePlayerDropdown()

tabWarp.newToggle("LOCK TARGET (‡∏¢‡∏¥‡∏á‡πÉ‡∏™‡πà‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢)", "‡∏Å‡∏£‡∏∞‡∏™‡∏∏‡∏ô‡∏à‡∏∞‡∏û‡∏∏‡πà‡∏á‡πÉ‡∏™‡πà‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å", false, function(s) Config.LockTarget = s end)

tabWarp.newButton("Warp To Player", "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏µ‡∏¢‡∏ß", function()
    local target = game.Players:FindFirstChild(Config.TargetName)
    if target and target.Character then GetCharacter().HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame end
end)
tabWarp.newToggle("Follow Player", "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ï‡∏¥‡∏î‡∏ï‡∏±‡∏ß‡∏ï‡∏•‡∏≠‡∏î‡πÄ‡∏ß‡∏•‡∏≤", false, function(s) Config.FollowPlayer = s end)

-- [[ Tab: ‡∏¢‡∏¥‡∏á‡∏£‡∏±‡∏ß‡∏ô‡∏£‡∏Å‡πÅ‡∏ï‡∏Å ]]
tabFast.newToggle("‡∏î‡∏≤‡∏ö‡∏£‡∏±‡∏ß (Ultra Sword)", "‡∏ü‡∏±‡∏ô‡∏ô‡∏£‡∏Å‡πÅ‡∏ï‡∏Å", false, function(s) Config.InstantSword = s end)
tabFast.newToggle("‡∏à‡∏£‡∏ß‡∏î‡∏£‡∏±‡∏ß (Mega Rocket)", "‡∏¢‡∏¥‡∏á‡∏Å‡∏£‡∏∞‡∏à‡∏≤‡∏¢", false, function(s) Config.InstantRocket = s end)
tabFast.newToggle("‡∏õ‡∏∑‡∏ô‡∏™‡∏µ‡∏ó‡∏≠‡∏á‡∏£‡∏±‡∏ß (Paintball Gold)", "‡πÉ‡∏ä‡πâ‡∏£‡∏µ‡πÇ‡∏°‡∏ó‡πÉ‡∏´‡∏°‡πà‡∏•‡πà‡∏≤‡∏™‡∏∏‡∏î", false, function(s) Config.InstantPaintball = s end)
tabFast.newToggle("‡∏´‡∏ô‡∏±‡∏á‡∏™‡∏ï‡∏¥‡πä‡∏Å‡∏£‡∏±‡∏ß", "‡πÑ‡∏ß‡∏à‡∏±‡∏î", false, function(s) Config.InstantSlingshot = s end)
tabFast.newToggle("‡∏ö‡∏≠‡∏•‡∏£‡∏±‡∏ß", "‡πÄ‡∏î‡πâ‡∏á‡∏£‡∏±‡∏ß‡πÜ", false, function(s) Config.InstantSuperball = s end)
tabFast.newToggle("‡∏£‡∏∞‡πÄ‡∏ö‡∏¥‡∏î‡∏£‡∏±‡∏ß", "‡∏ß‡∏≤‡∏á‡∏£‡∏±‡∏ß‡πÜ", false, function(s) Config.InstantBomb = s end)

-- [[ Main Loop ]]
RunService.Heartbeat:Connect(function()
    local char = Player.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return end
    local hrp = char.HumanoidRootPart

    local target = game.Players:FindFirstChild(Config.TargetName)
    local targetHRP = (target and target.Character and target.Character:FindFirstChild("HumanoidRootPart")) or nil

    if Config.FollowPlayer and targetHRP then
        hrp.CFrame = targetHRP.CFrame * CFrame.new(0, 0, 2)
    end

    if Config.KillAura then
        for _, v in pairs(game.Players:GetPlayers()) do
            if v ~= Player and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                local dist = (hrp.Position - v.Character.HumanoidRootPart.Position).Magnitude
                if dist <= Config.AuraRange then
                    RS.BBWeapons_Remotes.Sword.Activate:FireServer()
                end
            end
        end
    end

    local shootPos = hrp.CFrame * CFrame.new(0, 0, -10)
    if Config.LockTarget and targetHRP then shootPos = targetHRP.CFrame end
    
    if Config.InstantSword then RS.BBWeapons_Remotes.Sword.Activate:FireServer() end
    
    if Config.InstantPaintball and char:FindFirstChild("PaintballGun") then
        RS.BBWeapons_Remotes.PaintballGun.Activate:InvokeServer({
            ["LaunchCFrame"] = shootPos, ["Player"] = Player, ["BrickColor"] = BrickColor.new("Gold"),
            ["UniqueIdentifier"] = "Paint_" .. tick(), ["Tool"] = char.PaintballGun, ["MousePosition"] = shootPos.p
        })
    end

    if Config.InstantRocket and char:FindFirstChild("RocketLauncher") then
        RS.BBWeapons_Remotes.RocketLauncher.Activate:InvokeServer({["LaunchCFrame"] = shootPos, ["Player"] = Player, ["UniqueIdentifier"] = tick(), ["Tool"] = char.RocketLauncher, ["MousePosition"] = shootPos.p})
    end

    if Config.InstantSlingshot and char:FindFirstChild("Slingshot") then
        RS.BBWeapons_Remotes.Slingshot.Activate:InvokeServer({["LaunchCFrame"] = shootPos, ["Player"] = Player, ["UniqueIdentifier"] = tick(), ["Tool"] = char.Slingshot, ["MousePosition"] = shootPos.p})
    end

    if Config.InstantSuperball and char:FindFirstChild("Superball") then
        RS.BBWeapons_Remotes.Superball.Activate:InvokeServer({["LaunchCFrame"] = shootPos, ["Player"] = Player, ["UniqueIdentifier"] = tick(), ["Tool"] = char.Superball, ["MousePosition"] = shootPos.p})
    end
end)

task.spawn(function()
    while true do
        task.wait(0.01)
        if Config.InstantBomb then
            local char = Player.Character
            local b = char and char:FindFirstChild("Timebomb")
            if b then
                RS.BBWeapons_Remotes.Timebomb.Activate:InvokeServer({
                    ["Player"] = Player, ["Color"] = true, ["UpdateInterval"] = 0, ["Tool"] = b, ["SpawnCFrame"] = char.HumanoidRootPart.CFrame
                })
            end
        end
    end
end)

Player.CharacterAdded:Connect(function()
    task.wait(1)
    if Config.AntiFall then SetAntiFall(true) end
end)
