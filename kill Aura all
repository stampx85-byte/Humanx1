    Instance.new("UICorner", b).CornerRadius = UDim.new(1, 0); MakeDraggable(b)
    return b
end

local MinBoardBtn = CreateBtn("üìú", UDim2.new(0.85, -50, 0.3, 0), Color3.fromRGB(0, 100, 255))
