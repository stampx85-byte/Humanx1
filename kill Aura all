local Player = game.Players.LocalPlayer
local MyName = Player.Name

-- 1. ‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÑ‡∏î‡πâ‡∏£‡∏±‡∏ö‡∏™‡∏¥‡∏ó‡∏ò‡∏¥‡πå VIP ‡πÅ‡∏•‡∏∞ Admin (‡∏ô‡∏≥ Winwie001 ‡∏≠‡∏≠‡∏Å‡πÅ‡∏•‡πâ‡∏ß)
_G.VIP_LIST = {
    ["A_ASPD"] = true,
    ["ffsww_1007"] = true,
    ["HACKER_18050"] = true,
    ["evils915"] = true,
    -- ["Winwie001"] = true, -- ‡∏ñ‡∏π‡∏Å‡∏•‡∏ö‡∏≠‡∏≠‡∏Å‡πÅ‡∏•‡πâ‡∏ß
    ["TV_BTS1"] = true
}

-- 2. ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏•‡πâ‡∏≤‡∏á‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡πÄ‡∏Å‡πà‡∏≤‡πÅ‡∏•‡∏∞‡∏£‡∏±‡∏ô‡∏ï‡∏±‡∏ß VIP
local function ForceSwitchToVIP()
    warn("üíé [SYSTEM] Upgrading to VIP Status...")
    
    -- ‡∏•‡∏ö GUI ‡πÄ‡∏Å‡πà‡∏≤‡∏≠‡∏≠‡∏Å‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
    for _, v in pairs(game.CoreGui:GetChildren()) do
        if v:IsA("ScreenGui") and v.Name ~= "RobloxGui" then
            v:Destroy()
        end
    end
    
    -- ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÄ‡∏î‡∏¥‡∏°
    _G.Running = false 
    task.wait(0.3)
    
    -- ‡∏£‡∏±‡∏ô‡∏ï‡∏±‡∏ß VIP (Kill Aura 20)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/stampx85-byte/Humanx1/refs/heads/main/Kill%20Aura%2020"))()
end

-- 3. ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÅ‡∏ö‡∏ö‡∏¢‡πà‡∏≠ (Partial Name)
local function findByPartialName(name)
    name = name:lower()
    for _, p in pairs(game.Players:GetPlayers()) do
        if p.Name:lower():sub(1, #name) == name then
            return p
        end
    end
    return nil
end

-- 4. ‡∏£‡∏∞‡∏ö‡∏ö‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡πÅ‡∏ä‡∏ó (‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö VIP ‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô)
Player.Chatted:Connect(function(msg)
    if not _G.VIP_LIST[Player.Name] then return end 
    
    local args = msg:split(" ")
    if args[1]:lower() == "!givevip" and args[2] then
        local target = findByPartialName(args[2])
        if target then
            if not _G.VIP_LIST[target.Name] then
                _G.VIP_LIST[target.Name] = true
                print("‚úÖ [VIP SYSTEM] ‡πÉ‡∏´‡πâ VIP ‡πÅ‡∏Å‡πà: " .. target.Name)
                
                if target == Player then
                    ForceSwitchToVIP()
                end
            end
        end
    end
end)

-- 5. [‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ó‡∏≥‡∏á‡∏≤‡∏ô]
if _G.VIP_LIST[MyName] then
    print("üíé ‡∏¢‡∏¥‡∏ô‡∏î‡∏µ‡∏ï‡πâ‡∏≠‡∏ô‡∏£‡∏±‡∏ö VIP: " .. MyName)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/stampx85-byte/Humanx1/refs/heads/main/Kill%20Aura%2020"))()
