    Instance.new("StringValue", Player).Name = "ScriptWord_Active"
end

local function MakeDraggable(frame)
    local dragging, dragInput, dragStart, startPos
    frame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true; dragStart = input.Position; startPos = frame.Position
            input.Changed:Connect(function() if input.UserInputState == Enum.UserInputState.End then dragging = false end end)
        end
    end)
    frame.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then dragInput = input end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - dragStart
            frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)
end

-- [Effect] ‡∏ü‡∏≠‡∏á‡∏≠‡∏≤‡∏Å‡∏≤‡∏® (‡∏Ñ‡∏á‡πÄ‡∏î‡∏¥‡∏°)
local function CreateBubbles(parent, color)
    task.spawn(function()
        while task.wait(0.4) do
            if not parent then break end
            if parent.Visible then
                local bubble = Instance.new("Frame")
                bubble.Parent = parent
                bubble.ZIndex = parent.ZIndex - 1
                bubble.BackgroundTransparency = 0.6
                bubble.BackgroundColor3 = color
                bubble.BorderSizePixel = 0
                if math.random(1, 2) == 1 then Instance.new("UICorner", bubble).CornerRadius = UDim.new(1, 0) end
                local size = math.random(4, 8)
                bubble.Size = UDim2.new(0, size, 0, size)
                bubble.Position = UDim2.new(math.random(0.1, 0.9), 0, 1, 0)
                local tween = TweenService:Create(bubble, TweenInfo.new(math.random(2, 4), Enum.EasingStyle.Linear), {
                    Position = UDim2.new(bubble.Position.X.Scale, math.random(-20, 20), -0.2, 0),
                    BackgroundTransparency = 1,
                    Rotation = math.random(-180, 180)
                })
                tween:Play()
                tween.Completed:Connect(function() bubble:Destroy() end)
            end
        end
    end)
end

-- [Feature] ‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏ï‡∏∞ (‡∏Ñ‡∏á‡πÄ‡∏î‡∏¥‡∏°)
local function CustomKick()
    local Sound = Instance.new("Sound", game.Workspace)
    local sId = KickSoundInp.Text
    if not sId:find("rbxassetid://") then sId = "rbxassetid://"..sId end
    Sound.SoundId = sId; Sound.Volume = 5; Sound:Play()
    local ScreenGuiK = Instance.new("ScreenGui", game.CoreGui)
    ScreenGuiK.IgnoreGuiInset = true
    local TextLabel = Instance.new("TextLabel", ScreenGuiK)
    TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    TextLabel.Size = UDim2.new(1, 0, 1, 0)
    TextLabel.Text = KickTextInp.Text
    TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
    TextLabel.TextScaled = true; TextLabel.ZIndex = 999999
    task.wait(3.5)
    Player:Kick("\n\n[ Humanx1 System ]\n"..KickTextInp.Text.."\n")
    ScreenGuiK:Destroy()
end

local function EnhanceUI(frame, mainColor)
    Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 12)
    local stroke = Instance.new("UIStroke", frame)
    stroke.Thickness = 2.5; stroke.Color = mainColor; stroke.Transparency = 0.2
    CreateBubbles(frame, mainColor)
    task.spawn(function()
        while frame do
            TweenService:Create(stroke, TweenInfo.new(1.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Transparency = 0.6, Thickness = 3.5}):Play()
            task.wait(1.5)
            TweenService:Create(stroke, TweenInfo.new(1.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Transparency = 0.1, Thickness = 2}):Play()
            task.wait(1.5)
        end
    end)
end

