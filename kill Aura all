-- [[ FUNCTION: GLOBAL NOTIFICATION (Top Right) ]]
local function TopRightNotify(title, text, color)
    local NotifyGui = game.CoreGui:FindFirstChild("SW_TopNotify") or Instance.new("ScreenGui", game.CoreGui)
    NotifyGui.Name = "SW_TopNotify"
    
    local container = NotifyGui:FindFirstChild("Container") or Instance.new("Frame", NotifyGui)
    if not NotifyGui:FindFirstChild("Container") then
        container.Name = "Container"
        container.Size = UDim2.new(0, 250, 0, 500)
        container.Position = UDim2.new(1, -260, 0, 20)
        container.BackgroundTransparency = 1
        local layout = Instance.new("UIListLayout", container)
        layout.Padding = UDim.new(0, 10)
        layout.HorizontalAlignment = Enum.HorizontalAlignment.Right
    end

    local nFrame = Instance.new("Frame", container)
    nFrame.Size = UDim2.new(0, 0, 0, 50) -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏à‡∏≤‡∏Å 0 ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ç‡∏¢‡∏≤‡∏¢
    nFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
    nFrame.ClipsDescendants = true
    local stroke = Instance.new("UIStroke", nFrame); stroke.Color = color or Color3.fromRGB(255, 255, 255); stroke.Thickness = 2
    Instance.new("UICorner", nFrame)

    local tLabel = Instance.new("TextLabel", nFrame)
    tLabel.Size = UDim2.new(1, -10, 0, 20); tLabel.Position = UDim2.new(0, 10, 0, 5)
    tLabel.Text = title; tLabel.TextColor3 = color; tLabel.Font = Enum.Font.FredokaOne; tLabel.TextSize = 14; tLabel.BackgroundTransparency = 1; tLabel.TextXAlignment = Enum.TextXAlignment.Left

    local dLabel = Instance.new("TextLabel", nFrame)
    dLabel.Size = UDim2.new(1, -10, 0, 20); dLabel.Position = UDim2.new(0, 10, 0, 25)
    dLabel.Text = text; dLabel.TextColor3 = Color3.new(1,1,1); dLabel.Font = Enum.Font.SourceSansBold; dLabel.TextSize = 12; dLabel.BackgroundTransparency = 1; dLabel.TextXAlignment = Enum.TextXAlignment.Left

    -- Animation In
    TweenService:Create(nFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back), {Size = UDim2.new(1, 0, 0, 50)}):Play()
    
    task.delay(4, function()
        TweenService:Create(nFrame, TweenInfo.new(0.5), {Size = UDim2.new(0, 0, 0, 50), BackgroundTransparency = 1}):Play()
        task.wait(0.5)
        nFrame:Destroy()
    end)
end

-- [[ BUTTON ANIMATION FUNCTION ]]
local function AddButtonAnim(btn, hoverCol)
    local oldCol = btn.BackgroundColor3
    local oldSize = btn.Size
    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.3), {BackgroundColor3 = hoverCol, Size = oldSize + UDim2.new(0, 4, 0, 4)}):Play()
    end)
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.3), {BackgroundColor3 = oldCol, Size = oldSize}):Play()
    end)
    btn.MouseButton1Down:Connect(function()
        btn:TweenSize(oldSize - UDim2.new(0, 5, 0, 5), "Out", "Quad", 0.1, true)
    end)
    btn.MouseButton1Up:Connect(function()
        btn:TweenSize(oldSize + UDim2.new(0, 4, 0, 4), "Out", "Quad", 0.1, true)
    end)
end

