local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("AUTO MONEY SPAMMER", "Default")

-- [ Services ]
local RS = game:GetService("ReplicatedStorage")

-- [ Config ]
local Config = {
    AutoSpam = false,
    Delay = 0.5 -- ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏£‡∏≠‡∏ö (‡∏¢‡∏¥‡πà‡∏á‡∏ô‡πâ‡∏≠‡∏¢‡∏¢‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß)
}

-- [ ‡∏£‡∏∞‡∏ö‡∏ö Auto Spam Logic ]
task.spawn(function()
    while true do
        if Config.AutoSpam then
            -- ‡∏™‡πà‡∏á Remote 100 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏Å‡∏±‡∏ô‡πÉ‡∏ô‡πÄ‡∏™‡∏µ‡πâ‡∏¢‡∏ß‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
            for i = 1, 100 do
                task.spawn(function()
                    pcall(function()
                        RS.Remotes.ClaimReward:FireServer()
                    end)
                end)
            end
        end
        task.wait(Config.Delay) -- ‡∏£‡∏≠‡πÄ‡∏ß‡∏•‡∏≤‡πÉ‡∏ô‡∏£‡∏≠‡∏ö‡∏ñ‡∏±‡∏î‡πÑ‡∏õ
    end
end)

--- UI SETUP ---
local tab = DrRayLibrary.newTab("Main Farm", "rbxassetid://4483345998")

tab.newToggle("Enable Auto x100", "‡πÄ‡∏õ‡∏¥‡∏î‡∏î‡∏π‡∏î‡πÄ‡∏á‡∏¥‡∏ô 100 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ï‡πà‡∏≠‡∏£‡∏≠‡∏ö‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", false, function(state)
    Config.AutoSpam = state
    if state then
        print("üí∞ Auto Spammer: ON")
    else
        print("üõë Auto Spammer: OFF")
    end
end)

tab.newSlider("Loop Speed", "‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏£‡∏≠‡∏ö (‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ)", 20, true, 5, function(v)
    -- ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡πà‡∏≤ Delay (‡∏´‡∏≤‡∏£‡∏î‡πâ‡∏ß‡∏¢ 10 ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÑ‡∏î‡πâ‡∏ó‡∏®‡∏ô‡∏¥‡∏¢‡∏°)
    Config.Delay = v / 10
end)

print("üöÄ Auto Spam x100 Loaded!")
