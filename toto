--[[
  ‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡∏≠‡∏¢‡πÑ‡∏õ‡∏´‡∏≤ Nooby ‡πÅ‡∏•‡∏∞ Cutie Noob
  ‡πÄ‡∏ß‡∏≠‡∏£‡πå‡∏ä‡∏±‡∏ô: Fly Hunt Nooby V6 - ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°
]]

local Player = game:GetService("Players").LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏£‡∏∞‡∏ö‡∏ö
local isActive = false
local isFollowing = false
local isKilling = false
local warpSpeed = 2000
local searchDistance = 5000
local currentTarget = nil
local followConnection = nil
local isFlying = false
local flyTween = nil
local lastSearchTime = 0
local searchCooldown = 1.5
local emergencyStop = false
local lastPosition = nil

-- ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏´‡∏≤ (‡πÄ‡∏à‡∏≤‡∏∞‡∏à‡∏á‡πÅ‡∏Ñ‡πà 2 ‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô)
local targetNames = {
    "nooby",
    "cutie noob"
}

-- UI ‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏á‡πà‡∏≤‡∏¢
local function CreateSimpleUI()
    local CoreGui = game:GetService("CoreGui")
    local oldUI = CoreGui:FindFirstChild("NoobyHunterUI")
    if oldUI then oldUI:Destroy() end
    
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "NoobyHunterUI"
    ScreenGui.Parent = CoreGui
    ScreenGui.ResetOnSpawn = false
    
    local MainFrame = Instance.new("Frame")
    MainFrame.Size = UDim2.new(0, 320, 0, 250)
    MainFrame.Position = UDim2.new(0.02, 0, 0.02, 0)
    MainFrame.BackgroundColor3 = Color3.fromRGB(20, 30, 40)
    MainFrame.BackgroundTransparency = 0.1
    MainFrame.BorderSizePixel = 0
    MainFrame.Parent = ScreenGui
    
    local UICorner = Instance.new("UICorner")
    UICorner.CornerRadius = UDim.new(0, 12)
    UICorner.Parent = MainFrame
    
    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(1, 0, 0, 45)
    Title.BackgroundColor3 = Color3.fromRGB(0, 150, 200)
    Title.Text = "üéØ NOOBY HUNTER V6"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextSize = 22
    Title.Font = Enum.Font.GothamBold
    Title.Parent = MainFrame
    
    local TitleCorner = Instance.new("UICorner")
    TitleCorner.CornerRadius = UDim.new(0, 12)
    TitleCorner.Parent = Title
    
    local ToggleBtn = Instance.new("TextButton")
    ToggleBtn.Size = UDim2.new(0.9, 0, 0, 55)
    ToggleBtn.Position = UDim2.new(0.05, 0, 0.25, 0)
    ToggleBtn.Text = "‚ñ∂Ô∏è ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡πà‡∏≤ Nooby"
    ToggleBtn.BackgroundColor3 = Color3.fromRGB(0, 180, 100)
    ToggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    ToggleBtn.TextSize = 18
    ToggleBtn.Font = Enum.Font.GothamBold
    ToggleBtn.Name = "ToggleButton"
    ToggleBtn.Parent = MainFrame
    
    local BtnCorner = Instance.new("UICorner")
    BtnCorner.CornerRadius = UDim.new(0, 10)
    BtnCorner.Parent = ToggleBtn
    
    local StatusLabel = Instance.new("TextLabel")
    StatusLabel.Size = UDim2.new(1, 0, 0, 120)
    StatusLabel.Position = UDim2.new(0, 0, 0.55, 0)
    StatusLabel.BackgroundTransparency = 1
    StatusLabel.Text = "üìä ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏û‡∏£‡πâ‡∏≠‡∏°\nüéØ ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢: Nooby & Cutie Noob\nüîÑ ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°‡πÅ‡∏•‡πâ‡∏ß\nüíÄ ‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡∏à‡∏ô‡∏ï‡∏≤‡∏¢"
    StatusLabel.TextColor3 = Color3.fromRGB(200, 230, 255)
    StatusLabel.TextSize = 14
    StatusLabel.Font = Enum.Font.Gotham
    StatusLabel.TextWrapped = true
    StatusLabel.Parent = MainFrame
    
    local KeybindLabel = Instance.new("TextLabel")
    KeybindLabel.Size = UDim2.new(1, 0, 0, 25)
    KeybindLabel.Position = UDim2.new(0, 0, 0.95, -25)
    KeybindLabel.BackgroundTransparency = 1
    KeybindLabel.Text = "F=‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î | Space=‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô | R=‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä"
    KeybindLabel.TextColor3 = Color3.fromRGB(150, 200, 255)
    KeybindLabel.TextSize = 12
    KeybindLabel.Font = Enum.Font.GothamMedium
    KeybindLabel.Parent = MainFrame
    
    return ScreenGui, ToggleBtn, StatusLabel
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤ Nooby ‡πÅ‡∏•‡∏∞ Cutie Noob ‡πÄ‡∏à‡∏≤‡∏∞‡∏à‡∏á
local function findTargetNPCs()
    local npcList = {}
    local foundCount = 0
    
    -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏ä‡∏∑‡πà‡∏≠‡∏ó‡∏µ‡πà‡πÄ‡∏à‡∏≤‡∏∞‡∏à‡∏á (‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÅ‡∏ö‡∏ö partial)
    for _, obj in ipairs(Workspace:GetChildren()) do
        if obj:IsA("Model") and obj ~= Character then
            local objName = string.lower(obj.Name)
            local isExactMatch = false
            
            for _, targetName in ipairs(targetNames) do
                if objName == targetName then
                    isExactMatch = true
                    break
                end
            end
            
            if isExactMatch then
                local hrp = obj:FindFirstChild("HumanoidRootPart")
                local humanoid = obj:FindFirstChildOfClass("Humanoid")
                
                if hrp and humanoid and humanoid.Health > 0 then
                    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÑ‡∏°‡πà‡πÉ‡∏ä‡πà‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
                    local isPlayer = false
                    for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
                        if obj == player.Character then
                            isPlayer = true
                            break
                        end
                    end
                    
                    if not isPlayer then
                        local distance = 0
                        if Character and Character:FindFirstChild("HumanoidRootPart") then
                            distance = (Character.HumanoidRootPart.Position - hrp.Position).Magnitude
                        end
                        
                        table.insert(npcList, {
                            Model = obj,
                            Name = obj.Name,
                            Position = hrp.Position,
                            Humanoid = humanoid,
                            Distance = distance
                        })
                        foundCount = foundCount + 1
                    end
                end
            end
        end
    end
    
    -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏ï‡∏¥‡∏°‡∏à‡∏≤‡∏Å Workspace.GetDescendants
    if foundCount == 0 then
        for _, obj in ipairs(Workspace:GetDescendants()) do
            if obj:IsA("Model") and obj ~= Character then
                local objName = string.lower(obj.Name)
                local isExactMatch = false
                
                for _, targetName in ipairs(targetNames) do
                    if objName == targetName then
                        isExactMatch = true
                        break
                    end
                end
                
                if isExactMatch then
                    local hrp = obj:FindFirstChild("HumanoidRootPart")
                    local humanoid = obj:FindFirstChildOfClass("Humanoid")
                    
                    if hrp and humanoid and humanoid.Health > 0 then
                        local alreadyAdded = false
                        for _, npc in ipairs(npcList) do
                            if npc.Model == obj then
                                alreadyAdded = true
                                break
                            end
                        end
                        
                        if not alreadyAdded then
                            local isPlayer = false
                            for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
                                if obj == player.Character then
                                    isPlayer = true
                                    break
                                end
                            end
                            
                            if not isPlayer then
                                local distance = 0
                                if Character and Character:FindFirstChild("HumanoidRootPart") then
                                    distance = (Character.HumanoidRootPart.Position - hrp.Position).Magnitude
                                end
                                
                                table.insert(npcList, {
                                    Model = obj,
                                    Name = obj.Name,
                                    Position = hrp.Position,
                                    Humanoid = humanoid,
                                    Distance = distance
                                })
                                foundCount = foundCount + 1
                            end
                        end
                    end
                end
            end
        end
    end
    
    return npcList
end

-- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î
local function findNearestTarget()
    if not Character or not Character:FindFirstChild("HumanoidRootPart") then
        return nil, "‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£", 0, nil
    end
    
    local currentTime = tick()
    if currentTime - lastSearchTime < searchCooldown then
        return nil, "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ñ‡∏π‡∏•‡∏î‡∏≤‡∏ß‡∏ô‡πå", 0, nil
    end
    
    lastSearchTime = currentTime
    local playerPos = Character.HumanoidRootPart.Position
    local allNPCs = findTargetNPCs()
    
    if #allNPCs == 0 then
        return nil, "‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢", 0, nil
    end
    
    local closestTarget = nil
    local closestHumanoid = nil
    local closestDistance = searchDistance
    local targetName = ""
    
    -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á‡πÅ‡∏•‡∏∞‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î
    for _, npc in ipairs(allNPCs) do
        if npc.Model and npc.Model.Parent then
            local hrp = npc.Model:FindFirstChild("HumanoidRootPart")
            if hrp and npc.Humanoid then
                local distance = (playerPos - hrp.Position).Magnitude
                if distance < closestDistance then
                    closestDistance = distance
                    closestTarget = npc.Model
                    closestHumanoid = npc.Humanoid
                    targetName = npc.Name
                end
            end
        end
    end
    
    return closestTarget, targetName, closestDistance, closestHumanoid
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÅ‡∏ö‡∏ö‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏° (‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏•‡∏á)
local function optimalWarpToTarget(target)
    if not target or not target:FindFirstChild("HumanoidRootPart") then
        return false
    end
    
    if not Character or not Character:FindFirstChild("HumanoidRootPart") then
        return false
    end
    
    local playerHrp = Character.HumanoidRootPart
    local targetHrp = target.HumanoidRootPart
    
    -- ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ö‡∏ô‡∏´‡∏±‡∏ß (‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏•‡∏∑‡∏≠ 2-3 ‡∏´‡∏ô‡πà‡∏ß‡∏¢ ‡πÅ‡∏ó‡∏ô‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÄ‡∏õ‡πá‡∏ô 10)
    local headPosition = targetHrp.Position + Vector3.new(0, 2.5, 0)  -- ‡∏•‡∏î‡∏à‡∏≤‡∏Å 10 ‡πÄ‡∏´‡∏•‡∏∑‡∏≠ 2.5
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡πÄ‡∏£‡∏≤‡∏à‡∏∞‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
    local raycastParams = RaycastParams.new()
    raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
    raycastParams.FilterDescendantsInstances = {Character, target}
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏à‡∏≤‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏´‡∏±‡∏ß‡∏•‡∏á‡∏°‡∏≤‡∏î‡πâ‡∏≤‡∏ô‡∏•‡πà‡∏≤‡∏á 5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢ (‡∏•‡∏î‡∏•‡∏á‡∏à‡∏≤‡∏Å 20)
    local raycastResult = workspace:Raycast(headPosition, Vector3.new(0, -5, 0), raycastParams)
    
    if raycastResult then
        -- ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ‡∏≠‡∏∞‡πÑ‡∏£‡∏Ç‡∏ß‡∏≤‡∏á‡∏î‡πâ‡∏≤‡∏ô‡∏•‡πà‡∏≤‡∏á (‡πÄ‡∏ä‡πà‡∏ô ‡∏û‡∏∑‡πâ‡∏ô) ‡πÉ‡∏´‡πâ‡∏≠‡∏¢‡∏π‡πà‡πÄ‡∏´‡∏ô‡∏∑‡∏≠‡∏°‡∏±‡∏ô 1 ‡∏´‡∏ô‡πà‡∏ß‡∏¢ (‡∏•‡∏î‡∏à‡∏≤‡∏Å 2)
        headPosition = Vector3.new(headPosition.X, raycastResult.Position.Y + 1, headPosition.Z)
    end
    
    -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏±‡∏ö‡πÅ‡∏•‡πâ‡∏ß
    local success = pcall(function()
        -- ‡πÉ‡∏ä‡πâ CFrame.New ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Ñ‡∏ß‡∏≤‡∏°‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î
        playerHrp.CFrame = CFrame.new(headPosition)
        
        -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
        playerHrp.Velocity = Vector3.new(0, 0, 0)
        playerHrp.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        playerHrp.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
        
        -- ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ä‡∏ô‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß
        playerHrp.CanCollide = false
        task.wait(0.05)
        playerHrp.CanCollide = true
    end)
    
    if success then
        print("[WARP] ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏´‡∏±‡∏ß " .. target.Name .. " ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à (‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á: 2.5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢)")
        return true
    else
        print("[ERROR] ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß")
        return false
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡∏ö‡∏ô‡∏´‡∏±‡∏ß‡πÅ‡∏ö‡∏ö‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°
local function optimizedFollowAndKill(target, humanoid)
    if not target or not humanoid then return end
    
    isFollowing = true
    isKilling = true
    currentTarget = target
    
    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ PlatformStand ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏•‡∏≠‡∏¢‡πÑ‡∏î‡πâ
    if Character:FindFirstChild("Humanoid") then
        Character.Humanoid.PlatformStand = true
        Character.Humanoid:ChangeState(Enum.HumanoidStateType.Physics)
    end
    
    print("[ATTACK] ‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÅ‡∏ö‡∏ö‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°: " .. target.Name)
    
    -- ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ö‡∏¥‡∏ô‡∏ñ‡πâ‡∏≤‡∏°‡∏µ
    if flyTween then
        flyTween:Cancel()
        flyTween = nil
    end
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á connection ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡∏ö‡∏ô‡∏´‡∏±‡∏ß‡πÅ‡∏ö‡∏ö‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á
    if followConnection then
        followConnection:Disconnect()
    end
    
    local stuckCheckCount = 0
    local damageCounter = 0
    
    followConnection = RunService.Heartbeat:Connect(function(deltaTime)
        if not isFollowing or not isActive or emergencyStop then
            return
        end
        
        if not target or not target.Parent or not target:FindFirstChild("HumanoidRootPart") then
            isFollowing = false
            isKilling = false
            if followConnection then
                followConnection:Disconnect()
            end
            return
        end
        
        if not Character or not Character:FindFirstChild("HumanoidRootPart") then
            return
        end
        
        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡∏°‡∏µ‡∏ä‡∏µ‡∏ß‡∏¥‡∏ï‡∏≠‡∏¢‡∏π‡πà‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
        if humanoid.Health <= 0 then
            print("[KILL] ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ï‡∏≤‡∏¢‡πÅ‡∏•‡πâ‡∏ß: " .. target.Name)
            isFollowing = false
            isKilling = false
            if followConnection then
                followConnection:Disconnect()
            end
            return
        end
        
        local targetHrp = target.HumanoidRootPart
        local playerHrp = Character.HumanoidRootPart
        
        -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡∏ö‡∏ô‡∏´‡∏±‡∏ß (‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏•‡∏∑‡∏≠ 2.5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢)
        local targetPosition = Vector3.new(
            targetHrp.Position.X,
            targetHrp.Position.Y + 2.5,  -- ‡∏•‡∏î‡∏à‡∏≤‡∏Å 8 ‡πÄ‡∏´‡∏•‡∏∑‡∏≠ 2.5
            targetHrp.Position.Z
        )
        
        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏£‡∏≤‡∏≠‡∏¢‡∏π‡πà‡∏™‡∏π‡∏á‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
        local currentY = playerHrp.Position.Y
        local targetY = targetHrp.Position.Y
        
        if currentY - targetY > 5 then  -- ‡∏ñ‡πâ‡∏≤‡∏™‡∏π‡∏á‡∏Å‡∏ß‡πà‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏°‡∏≤‡∏Å‡πÄ‡∏Å‡∏¥‡∏ô 5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢
            -- ‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏•‡∏á
            targetPosition = Vector3.new(targetPosition.X, targetY + 1, targetPosition.Z)
            print("[DEBUG] ‡∏õ‡∏£‡∏±‡∏ö‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏•‡∏á")
        end
        
        -- ‡πÉ‡∏ä‡πâ‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡πÅ‡∏ö‡∏ö Lerp ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏•‡∏∑‡πà‡∏ô‡πÑ‡∏´‡∏•
        local currentCFrame = playerHrp.CFrame
        local targetCFrame = CFrame.new(targetPosition)
        local smoothness = 0.8  -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡∏•‡∏∑‡πà‡∏ô
        
        local newCFrame = CFrame.new(
            currentCFrame.Position:Lerp(targetPosition, smoothness),
            targetHrp.Position
        )
        
        -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
        pcall(function()
            playerHrp.CFrame = newCFrame
            
            -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á
            playerHrp.Velocity = Vector3.new(0, 0, 0)
            
            -- ‡∏´‡∏°‡∏∏‡∏ô‡∏ï‡∏±‡∏ß‡πÉ‡∏´‡πâ‡∏´‡∏±‡∏ô‡∏´‡∏ô‡πâ‡∏≤‡πÑ‡∏õ‡∏ó‡∏≤‡∏á‡πÄ‡∏õ‡πâ‡∏≤
            playerHrp.CFrame = CFrame.new(playerHrp.Position, targetHrp.Position)
        end)
        
        -- ‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
        damageCounter = damageCounter + deltaTime
        if damageCounter >= 0.3 then -- ‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡∏ó‡∏∏‡∏Å 0.3 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
            -- ‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á
            humanoid:TakeDamage(20)  -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏™‡∏µ‡∏¢‡∏´‡∏≤‡∏¢
            
            -- ‡∏•‡∏≠‡∏á‡πÉ‡∏ä‡πâ RemoteEvents ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÇ‡∏à‡∏°‡∏ï‡∏µ (‡∏ñ‡πâ‡∏≤‡∏°‡∏µ)
            if Character:FindFirstChildOfClass("Tool") then
                local tool = Character:FindFirstChildOfClass("Tool")
                if tool:FindFirstChild("Handle") then
                    for _, v in pairs(tool:GetChildren()) do
                        if v:IsA("RemoteEvent") then
                            pcall(function()
                                v:FireServer(targetHrp.Position)
                            end)
                        end
                    end
                end
            end
            
            -- ‡∏•‡∏≠‡∏á‡πÉ‡∏ä‡πâ ClickDetector ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ
            if target:FindFirstChild("ClickDetector") then
                pcall(function()
                    target.ClickDetector:FireServer()
                end)
            end
            
            damageCounter = 0
        end
        
        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏ï‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà‡∏Å‡∏±‡∏ö‡∏ó‡∏µ‡πà‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
        local currentPos = playerHrp.Position
        if lastPosition then
            local distanceMoved = (currentPos - lastPosition).Magnitude
            if distanceMoved < 0.5 then  -- ‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏ô‡πâ‡∏≠‡∏¢‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ
                stuckCheckCount = stuckCheckCount + 1
                if stuckCheckCount > 10 then  -- ‡∏ï‡∏¥‡∏î‡∏°‡∏≤‡∏ô‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß
                    -- ‡∏û‡∏¢‡∏≤‡∏¢‡∏≤‡∏°‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÉ‡∏´‡∏°‡πà
                    pcall(function()
                        playerHrp.CFrame = CFrame.new(targetPosition)
                    end)
                    stuckCheckCount = 0
                end
            else
                stuckCheckCount = 0
            end
        end
        lastPosition = currentPos
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏£‡∏ß‡∏à‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà‡πÅ‡∏ö‡∏ö‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á
local function optimalExplore()
    if not Character or not Character:FindFirstChild("HumanoidRootPart") then
        return
    end
    
    -- ‡∏´‡∏¢‡∏∏‡∏î Tween ‡πÄ‡∏Å‡πà‡∏≤
    if flyTween then
        flyTween:Cancel()
        flyTween = nil
    end
    
    local hrp = Character.HumanoidRootPart
    local currentPos = hrp.Position
    
    -- ‡∏™‡∏∏‡πà‡∏°‡∏ó‡∏¥‡∏®‡∏ó‡∏≤‡∏á‡πÅ‡∏•‡∏∞‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á (‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏Å‡∏≤‡∏£‡∏™‡∏≥‡∏£‡∏ß‡∏à)
    local randomAngle = math.random() * math.pi * 2
    local randomDirection = Vector3.new(
        math.cos(randomAngle),
        0.2,  -- ‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏Å‡∏≤‡∏£‡∏™‡∏≥‡∏£‡∏ß‡∏à‡∏•‡∏á
        math.sin(randomAngle)
    )
    
    local randomDistance = math.random(200, 500)  -- ‡∏•‡∏î‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á‡∏•‡∏á
    local targetPosition = currentPos + (randomDirection * randomDistance)
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÑ‡∏°‡πà‡∏™‡∏π‡∏á‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ
    if targetPosition.Y > currentPos.Y + 20 then
        targetPosition = Vector3.new(targetPosition.X, currentPos.Y + 5, targetPosition.Z)
    end
    
    -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
    pcall(function()
        hrp.CFrame = CFrame.new(targetPosition)
        hrp.Velocity = Vector3.new(0, 0, 0)
    end)
    
    print("[EXPLORE] ‡∏™‡∏≥‡∏£‡∏ß‡∏à‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà‡πÉ‡∏´‡∏°‡πà‡∏≠‡∏¢‡πà‡∏≤‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°")
end

-- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö
local function startSystem()
    local _, toggleBtn, statusLabel = CreateSimpleUI()
    local lastTargetName = ""
    local targetFoundCount = 0
    
    -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
    local function updateStatus(text)
        statusLabel.Text = "üìä " .. text
    end
    
    -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏¢‡∏∏‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
    local function stopAllActions()
        if followConnection then
            followConnection:Disconnect()
            followConnection = nil
        end
        
        if flyTween then
            flyTween:Cancel()
            flyTween = nil
        end
        
        if Character and Character:FindFirstChild("Humanoid") then
            Character.Humanoid.PlatformStand = false
            Character.Humanoid:ChangeState(Enum.HumanoidStateType.Running)
        end
        
        if Character and Character:FindFirstChild("HumanoidRootPart") then
            Character.HumanoidRootPart.CanCollide = true
        end
        
        isFollowing = false
        isKilling = false
        isFlying = false
        currentTarget = nil
        lastPosition = nil
    end
    
    -- ‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏´‡∏•‡∏±‡∏Å
    local function mainLoop()
        while isActive do
            if emergencyStop then
                updateStatus("‚õî ‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô!\n‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏£‡∏∞‡∏ö‡∏ö...")
                stopAllActions()
                task.wait(1)
                emergencyStop = false
            end
            
            if not Character or not Character:FindFirstChild("HumanoidRootPart") then
                updateStatus("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏£‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£...")
                task.wait(1)
                Character = Player.Character or Player.CharacterAdded:Wait()
                continue
            end
            
            -- ‡∏ñ‡πâ‡∏≤‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡∏´‡∏£‡∏∑‡∏≠‡∏Ü‡πà‡∏≤‡∏≠‡∏¢‡∏π‡πà ‡∏£‡∏≠‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤‡∏à‡∏∞‡πÄ‡∏™‡∏£‡πá‡∏à
            if isFollowing or isKilling then
                task.wait(0.2)
                continue
            end
            
            -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
            local target, name, distance, humanoid = findNearestTarget()
            
            if target and humanoid then
                if name ~= lastTargetName then
                    lastTargetName = name
                    targetFoundCount = targetFoundCount + 1
                end
                
                updateStatus(string.format("üéØ ‡∏û‡∏ö: %s\nüìè ‡∏£‡∏∞‡∏¢‡∏∞: %.0f ‡∏´‡∏ô‡πà‡∏ß‡∏¢\n‚ö° ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°...\nüó∫Ô∏è ‡∏û‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î: %d ‡∏Ñ‡∏£‡∏±‡πâ‡∏á", 
                    name, distance, targetFoundCount))
                
                -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÅ‡∏ö‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°
                local success = optimalWarpToTarget(target)
                
                if success then
                    -- ‡∏£‡∏≠‡∏™‡∏±‡∏Å‡∏Ñ‡∏£‡∏π‡πà‡πÉ‡∏´‡πâ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÄ‡∏™‡∏£‡πá‡∏à
                    task.wait(0.3)
                    
                    -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÅ‡∏•‡∏∞‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡∏ö‡∏ô‡∏´‡∏±‡∏ß‡πÅ‡∏ö‡∏ö‡πÉ‡∏´‡∏°‡πà
                    if not isFollowing and target:FindFirstChild("HumanoidRootPart") and humanoid.Health > 0 then
                        optimizedFollowAndKill(target, humanoid)
                        updateStatus(string.format("‚öîÔ∏è ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÇ‡∏à‡∏°‡∏ï‡∏µ: %s\nüíÄ ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏° (2.5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢)...", name))
                    end
                    
                    -- ‡∏£‡∏≠‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏à‡∏∞‡∏ï‡∏≤‡∏¢
                    local killStart = tick()
                    while isFollowing and isActive and not emergencyStop do
                        if not target or not target.Parent or humanoid.Health <= 0 then
                            break
                        end
                        
                        -- ‡∏ñ‡πâ‡∏≤‡πÉ‡∏ä‡πâ‡πÄ‡∏ß‡∏•‡∏≤‡∏ô‡∏≤‡∏ô‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ ‡πÉ‡∏´‡πâ‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏•‡∏π‡∏õ
                        if tick() - killStart > 15 then -- ‡∏•‡∏î‡πÄ‡∏´‡∏•‡∏∑‡∏≠ 15 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡∏ï‡πà‡∏≠‡πÄ‡∏õ‡πâ‡∏≤
                            updateStatus("‚è∞ ‡πÉ‡∏ä‡πâ‡πÄ‡∏ß‡∏•‡∏≤‡∏ô‡∏≤‡∏ô‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ\n‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢...")
                            break
                        end
                        
                        task.wait(0.3)
                    end
                    
                    -- ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Ü‡πà‡∏≤‡πÄ‡∏™‡∏£‡πá‡∏à
                    if not emergencyStop then
                        updateStatus(string.format("‚úÖ ‡∏Ü‡πà‡∏≤‡πÄ‡∏™‡∏£‡πá‡∏à: %s\nüéØ ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÉ‡∏´‡∏°‡πà...", name))
                        stopAllActions()
                        task.wait(1.0)  -- ‡∏•‡∏î‡πÄ‡∏ß‡∏•‡∏≤‡∏£‡∏≠
                    end
                else
                    updateStatus("‚ùå ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à\n‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏•‡∏≠‡∏á‡πÉ‡∏´‡∏°‡πà...")
                    task.wait(0.5)
                end
            else
                updateStatus("üîç ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤ Nooby/Cutie Noob...\nüîÑ ‡∏™‡∏≥‡∏£‡∏ß‡∏à‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°")
                
                -- ‡∏™‡∏≥‡∏£‡∏ß‡∏à‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà‡πÉ‡∏´‡∏°‡πà‡πÅ‡∏ö‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°
                optimalExplore()
                
                -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏™‡∏≥‡∏£‡∏ß‡∏à‡πÄ‡∏™‡∏£‡πá‡∏à
                task.wait(2.0)  -- ‡∏•‡∏î‡πÄ‡∏ß‡∏•‡∏≤‡∏£‡∏≠
            end
        end
    end
    
    -- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î
    toggleBtn.MouseButton1Click:Connect(function()
        isActive = not isActive
        
        if isActive then
            toggleBtn.Text = "‚è∏Ô∏è ‡∏´‡∏¢‡∏∏‡∏î‡∏•‡πà‡∏≤"
            toggleBtn.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
            updateStatus("üéØ ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πà‡∏≤ Nooby...\n‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢\nüîÑ ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏° (2.5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢)")
            
            print("[SYSTEM] ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πà‡∏≤ Nooby V6 (‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°)")
            
            -- ‡πÄ‡∏£‡∏¥‡πà‡∏° Loop ‡πÉ‡∏´‡∏°‡πà‡πÉ‡∏ô Task ‡πÅ‡∏¢‡∏Å
            task.spawn(function()
                mainLoop()
            end)
        else
            toggleBtn.Text = "‚ñ∂Ô∏è ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡πà‡∏≤ Nooby"
            toggleBtn.BackgroundColor3 = Color3.fromRGB(0, 180, 100)
            updateStatus("‡∏£‡∏∞‡∏ö‡∏ö‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏≥‡∏á‡∏≤‡∏ô\n‡∏Å‡∏î‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÉ‡∏´‡∏°‡πà")
            
            -- ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
            stopAllActions()
            
            emergencyStop = false
            print("[SYSTEM] ‡∏´‡∏¢‡∏∏‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πà‡∏≤")
        end
    end)
    
    -- ‡∏Ñ‡∏µ‡∏¢‡πå‡∏•‡∏±‡∏î F
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if not gameProcessed and input.KeyCode == Enum.KeyCode.F then
            toggleBtn:Activate()
        end
    end)
    
    -- ‡∏õ‡∏∏‡πà‡∏°‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô (Space)
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if not gameProcessed and input.KeyCode == Enum.KeyCode.Space then
            emergencyStop = true
            stopAllActions()
            updateStatus("‚õî ‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô!\n‡∏Å‡∏î‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏≠‡∏µ‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á")
            print("[EMERGENCY] ‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î!")
        end
    end)
    
    -- ‡∏õ‡∏∏‡πà‡∏°‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤ (R)
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if not gameProcessed and input.KeyCode == Enum.KeyCode.R then
            lastSearchTime = 0 -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡∏π‡∏•‡∏î‡∏≤‡∏ß‡∏ô‡πå
            updateStatus("üîÑ ‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤...")
            print("[REFRESH] ‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤")
        end
    end)
end

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡πÅ‡∏•‡∏∞‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö
task.spawn(function()
    task.wait(3)
    
    print("========================================")
    print("üéØ NOOBY & CUTIE NOOB HUNTER V6")
    print("üîÑ ‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á: ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏° (2.5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢)")
    print("üéØ ‡∏£‡∏∞‡∏ö‡∏ö‡∏à‡∏∞‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°")
    print("üíÄ ‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡∏ö‡∏ô‡∏´‡∏±‡∏ß‡πÅ‡∏•‡∏∞‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡∏à‡∏ô‡∏ï‡∏≤‡∏¢ (‡πÑ‡∏°‡πà‡∏™‡∏π‡∏á‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ)")
    print("üìå ‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏° F ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö")
    print("üìå ‡∏Å‡∏î Space ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô‡∏ó‡∏±‡∏ô‡∏ó‡∏µ")
    print("üìå ‡∏Å‡∏î R ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤")
    print("========================================")
    
    -- ‡πÄ‡∏£‡∏¥‡πà‡∏° UI
    startSystem()
    
    -- ‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Nooby Hunter V6",
        Text = "‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°‡πÅ‡∏•‡πâ‡∏ß! ‡∏Å‡∏î F ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡πà‡∏≤",
        Duration = 5,
        Icon = "rbxassetid://3944680095"
    })
end)

-- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô
Player.CharacterAdded:Connect(function(newChar)
    Character = newChar
    task.wait(2)
    print("[EVENT] ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÉ‡∏´‡∏°‡πà")
    
    -- ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
    stopAllActions()
    emergencyStop = false
end)

-- ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏•‡∏≠‡∏¢‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ï‡∏≤‡∏¢
local function onHumanoidDied()
    stopAllActions()
    emergencyStop = true
    if isActive then
        print("[INFO] ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ï‡∏≤‡∏¢ ‡∏´‡∏¢‡∏∏‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡∏≠‡∏¢‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß")
    end
end

-- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠ Event
if Character:FindFirstChild("Humanoid") then
    Character.Humanoid.Died:Connect(onHumanoidDied)
end

Player.CharacterAdded:Connect(function(char)
    char:WaitForChild("Humanoid").Died:Connect(onHumanoidDied)
end)

print("[SUCCESS] ‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå Nooby Hunter V6 (‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°) ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!")
