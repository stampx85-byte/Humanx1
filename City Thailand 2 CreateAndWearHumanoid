            return true
        else
            print("‚úó ‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 9 ‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á‡πÅ‡∏ö‡∏ö: " .. tostring(error2))
            DrRayLibrary:Notify("Error", "‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 9 ‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à", 3)
            return false
        end
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
local function resetChar()
    local player = game.Players.LocalPlayer
    if player.Character then
        player.Character:BreakJoints()
        print("‚úì ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÅ‡∏•‡πâ‡∏ß")
        DrRayLibrary:Notify("Success", "‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÅ‡∏•‡πâ‡∏ß", 2)
    end
end

---------------------------------------------------------
-- Animation System (‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÑ‡∏î‡πâ - ‡πÑ‡∏°‡πà‡∏°‡∏µ UI ‡∏Ç‡∏≠‡∏á‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á)
---------------------------------------------------------

-- Animation IDs
local IdleAnimId  = "rbxassetid://91348372558295"
local WalkAnimId  = "rbxassetid://134010853417610"
local Move1AnimId = "rbxassetid://129469072457859"

-- Settings
local KEY_MOVESET1 = Enum.KeyCode.Z
local Move1Cooldown = 3
local MOVE1_START_TIME      = 6   -- seconds
local MOVE1_FIRST_DURATION  = 2   -- first press
local MOVE1_SECOND_DURATION = 6   -- second press

-- Animation state variables
local customMovementEnabled = false
local move1Ready = true
local move1Toggled = false
local isUsingMove = false

-- Function to start animation system
local function startAnimationSystem()
    if animationEnabled then return end
    
    animationEnabled = true
    print("üöÄ ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏ô‡∏¥‡πÄ‡∏°‡∏ä‡∏±‡πà‡∏ô...")
    
    local function setupCharacter(char)
        local humanoid = char:WaitForChild("Humanoid")
        animateScript = char:FindFirstChild("Animate")

        task.wait(0.2)
        if animateScript then 
            animateScript.Disabled = true 
        end

        -- Create animations
        local idle = Instance.new("Animation")
        idle.AnimationId = IdleAnimId
        idleTrack = humanoid:LoadAnimation(idle)
        idleTrack.Looped = true
        idleTrack.Priority = Enum.AnimationPriority.Movement

        local walk = Instance.new("Animation")
        walk.AnimationId = WalkAnimId
        walkTrack = humanoid:LoadAnimation(walk)
        walkTrack.Looped = true
        walkTrack.Priority = Enum.AnimationPriority.Movement

        local move1 = Instance.new("Animation")
        move1.AnimationId = Move1AnimId
        move1Track = humanoid:LoadAnimation(move1)
        move1Track.Priority = Enum.AnimationPriority.Action

        -- Reset animation states
        customMovementEnabled = true
        move1Ready = true
        move1Toggled = false
        isUsingMove = false

        -- Function to enable custom movement
        local function enableCustomMovement()
            customMovementEnabled = true
            idleTrack:Play()
            if animateScript then 
                animateScript.Disabled = true 
            end
        end

        -- Function to disable custom movement
        local function disableCustomMovement()
            customMovementEnabled = false
            idleTrack:Stop()
            walkTrack:Stop()
            if animateScript then 
                animateScript.Disabled = false 
            end
        end

        -- Function to play move1
        local function playMove1()
            if not move1Ready or isUsingMove then return end
            
            isUsingMove = true
            move1Ready = false

            task.delay(Move1Cooldown, function()
                move1Ready = true
            end)

            move1Track:Stop()
            move1Track:Play()
            game:GetService("RunService").Heartbeat:Wait()

            if not move1Toggled then
                -- FIRST PRESS
                move1Track.TimePosition = MOVE1_START_TIME
                disableCustomMovement()
                move1Toggled = true

                task.delay(MOVE1_FIRST_DURATION, function()
                    if move1Track.IsPlaying then 
                        move1Track:Stop() 
                    end
                    isUsingMove = false
                end)
            else
                -- SECOND PRESS
                move1Track.TimePosition = 0
                task.delay(0.05, function()
                    enableCustomMovement()
                    move1Toggled = false
                end)

                task.delay(MOVE1_SECOND_DURATION, function()
                    if move1Track.IsPlaying then 
                        move1Track:Stop() 
                    end
                    isUsingMove = false
                end)
            end
        end

        -- Start with custom movement
        enableCustomMovement()

        -- Connect running event
        animationConnection = humanoid.Running:Connect(function(speed)
            if not customMovementEnabled then return end
            if speed > 1 then
                if not walkTrack.IsPlaying then
                    idleTrack:Stop()
                    walkTrack:Play()
                end
            else
                if not idleTrack.IsPlaying then
                    walkTrack:Stop()
                    idleTrack:Play()
                end
            end
        end)

        -- Connect keyboard input
        game:GetService("UserInputService").InputBegan:Connect(function(input, gp)
            if not animationEnabled or gp then return end
            if input.KeyCode == KEY_MOVESET1 then
                playMove1()
            end
        end)
    end

    -- Initialize for current character
    local player = game.Players.LocalPlayer
    if player.Character then 
        setupCharacter(player.Character) 
    end
    
    -- Connect for new characters
    player.CharacterAdded:Connect(function(char)
        if animationEnabled then
            setupCharacter(char)
        end
    end)
    
    DrRayLibrary:Notify("Success", "‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏ô‡∏¥‡πÄ‡∏°‡∏ä‡∏±‡πà‡∏ô‡πÅ‡∏•‡πâ‡∏ß (‡∏Å‡∏î Z ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏ä‡πâ‡∏™‡∏Å‡∏¥‡∏•)", 2)
end

-- Function to stop animation system
local function stopAnimationSystem()
    if not animationEnabled then return end
    
    animationEnabled = false
    print("üõë ‡∏´‡∏¢‡∏∏‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏ô‡∏¥‡πÄ‡∏°‡∏ä‡∏±‡πà‡∏ô...")
    
    -- Stop all tracks
    if idleTrack then 
        idleTrack:Stop() 
    end
    if walkTrack then 
        walkTrack:Stop() 
    end
    if move1Track then 
        move1Track:Stop() 
    end
    
    -- Re-enable default animate script
    if animateScript then
        animateScript.Disabled = false
    end
    
    -- Disconnect connections
    if animationConnection then
        animationConnection:Disconnect()
        animationConnection = nil
    end
    
    DrRayLibrary:Notify("Success", "‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏ô‡∏¥‡πÄ‡∏°‡∏ä‡∏±‡πà‡∏ô‡πÅ‡∏•‡πâ‡∏ß", 2)
end

-- Function to toggle animation system
local function toggleAnimationSystem()
    if animationEnabled then
        stopAnimationSystem()
    else
        startAnimationSystem()
    end
end

