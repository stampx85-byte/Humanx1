-- ‡πÇ‡∏´‡∏•‡∏î Library DrRay UI
local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("Outfit System", "Default")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏Å‡πá‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡πÅ‡∏•‡∏∞‡∏≠‡πâ‡∏≤‡∏á‡∏≠‡∏¥‡∏á
local animationEnabled = false
local animationConnection = nil
local idleTrack, walkTrack, move1Track
local animateScript

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Tab ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ä‡∏∏‡∏î
local tab = DrRayLibrary.newTab("Outfits", "rbxassetid://10734951151")

---------------------------------------------------------
-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏Å‡∏≤‡∏£‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î (‡∏Ñ‡∏£‡∏ö 9 ‡∏ä‡∏∏‡∏î - ‡πÉ‡∏ä‡πâ BloxbizRemotes ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î)
---------------------------------------------------------

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö Remote
local function checkRemote()
    local rs = game:GetService("ReplicatedStorage")
    local hasBloxbizRemotes = rs:FindFirstChild("BloxbizRemotes")
    
    if not hasBloxbizRemotes then
        print("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö BloxbizRemotes")
        DrRayLibrary:Notify("Error", "‡πÑ‡∏°‡πà‡∏û‡∏ö BloxbizRemotes", 3)
        return false
    end
    
    local hasCatalogOnApplyOutfit = hasBloxbizRemotes:FindFirstChild("CatalogOnApplyOutfit")
    local hasCatalogOnApplyToRealHumanoid = hasBloxbizRemotes:FindFirstChild("CatalogOnApplyToRealHumanoid")
    
    if not hasCatalogOnApplyOutfit then
        print("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö CatalogOnApplyOutfit")
        DrRayLibrary:Notify("Error", "‡πÑ‡∏°‡πà‡∏û‡∏ö CatalogOnApplyOutfit", 3)
        return false
    end
    
    if not hasCatalogOnApplyToRealHumanoid then
        print("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö CatalogOnApplyToRealHumanoid")
        DrRayLibrary:Notify("Error", "‡πÑ‡∏°‡πà‡∏û‡∏ö CatalogOnApplyToRealHumanoid", 3)
        return false
    end
    
    print("‚úì ‡∏û‡∏ö BloxbizRemotes ‡πÅ‡∏•‡∏∞ RemoteEvents ‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£")
    return true
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ‡∏û‡∏∑‡πâ‡∏ô‡∏ê‡∏≤‡∏ô
local function applyBodyColor(color)
    local success, errorMsg = pcall(function()
        game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyToRealHumanoid:FireServer({
            ["BodyColor"] = {
                ["HeadColor"] = color,
                ["TorsoColor"] = color,
                ["LeftArmColor"] = color,
                ["RightLegColor"] = color,
                ["LeftLegColor"] = color,
                ["RightArmColor"] = color
            }
        })
    end)
    
    if success then
        print("‚úì ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à")
        return true
    else
        print("‚úó ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à: " .. tostring(errorMsg))
        return false
    end
end

-- ‡∏ä‡∏∏‡∏î 1 - Basic White
local function applyOutfit1()
    print("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 1 (Basic White)...")
    
    if not checkRemote() then return false end
    
    local success, errorMsg = pcall(function()
        game:GetService("ReplicatedStorage").BloxbizRemotes.CatalogOnApplyOutfit:FireServer({
            ["WalkAnimation"] = 0,
            ["Accessories"] = {
                {
                    ["Rotation"] = Vector3.new(0, 0, 0),
                    ["AssetId"] = 4085923851, -- ‡∏´‡∏ô‡πâ‡∏≤
                    ["Position"] = Vector3.new(0, 0, 0),
                    ["Scale"] = Vector3.new(1, 1, 1),
                    ["IsLayered"] = false,
                    ["AccessoryType"] = Enum.AccessoryType.Face
                },
                {
                    ["Rotation"] = Vector3.new(0, 0, 0),
                    ["AssetId"] = 16807152290, -- ‡∏´‡∏°‡∏ß‡∏Å
                    ["Position"] = Vector3.new(0, 0, 0),
                    ["Scale"] = Vector3.new(1, 1, 1),
                    ["IsLayered"] = false,
                    ["AccessoryType"] = Enum.AccessoryType.Hat
                }
            },
            ["RightLegColor"] = Color3.fromRGB(246, 246, 246),
            ["MoodAnimation"] = 10647852134,
            ["LeftLegColor"] = Color3.fromRGB(246, 246, 246),
            ["JumpAnimation"] = 0,
            ["ProportionScale"] = 0.5,
            ["BodyTypeScale"] = 0,
            ["ClimbAnimation"] = 0,
            ["LeftArmColor"] = Color3.fromRGB(246, 246, 246),
            ["Face"] = 0,
            ["Pants"] = 6095618719,
            ["RightArmColor"] = Color3.fromRGB(246, 246, 246),
            ["DepthScale"] = 0.8500000238418579,
            ["WidthScale"] = 0.699999988079071,
            ["SwimAnimation"] = 0,
            ["RightArm"] = 32357584,
            ["GraphicTShirt"] = 0,
            ["Head"] = 17388507384,
            ["Shirt"] = 3334218182,
            ["RightLeg"] = 27493718,
            ["RunAnimation"] = 0,
            ["Torso"] = 27121265,
            ["FallAnimation"] = 0,
            ["TorsoColor"] = Color3.fromRGB(246, 246, 246),
            ["IdleAnimation"] = 11600321661,
            ["LeftArm"] = 32357619,
            ["HeadScale"] = 1,
            ["HeightScale"] = 1.0499999523162842,
            ["HeadColor"] = Color3.fromRGB(246, 246, 246),
            ["LeftLeg"] = 27493683
        })
    end)
    
    if success then
        print("‚úì ‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 1 ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à")
        wait(0.3)
        applyBodyColor(Color3.fromRGB(246, 246, 246))
        return true
    else
        print("‚úó ‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 1 ‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à: " .. tostring(errorMsg))
        DrRayLibrary:Notify("Error", "‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 1 ‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à", 3)
        return false
    end
end

