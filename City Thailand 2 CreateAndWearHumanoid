-- ‡∏£‡∏∞‡∏ö‡∏ö‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡πÅ‡∏ä‡∏ó
local function setupChatCommands()
    game.Players.LocalPlayer.Chatted:Connect(function(message)
        local msg = string.lower(message)
        
        if msg == "!outfit1" then
            applyOutfit1()
        elseif msg == "!outfit2" then
            applyOutfit2()
        elseif msg == "!outfit3" then
            applyOutfit3()
        elseif msg == "!outfit4" then
            applyOutfit4()
        elseif msg == "!outfit5" then
            applyOutfit5()
        elseif msg == "!outfit6" then
            applyOutfit6()
        elseif msg == "!outfit7" then
            applyOutfit7()
        elseif msg == "!outfit8" then
            applyOutfit8()
        elseif msg == "!reset" then
            resetChar()
        elseif msg == "!help" then
            print("========================================")
            print("üéÆ Outfit System Help:")
            print("!outfit1-8 - ‡πÉ‡∏™‡πà‡∏ä‡∏∏‡∏î 1-8")
            print("!reset     - ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£")
            print("!help      - ‡πÅ‡∏™‡∏î‡∏á‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î")
            print("========================================")
        end
    end)
end

-- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏£‡∏∞‡∏ö‡∏ö
setupChatCommands()

