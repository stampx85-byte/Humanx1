    end
    
    if attackBtn then
        attackBtn.Text = "AUTO ATTACK: OFF"
        attackBtn.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
    end
    
    if flyTween then
        flyTween:Cancel()
        flyTween = nil
        isFlying = false
    end
    
    DrRayLibrary:Notification("‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö", "‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!", "OK")
end)

mainTab.newLabel("üìã ‡∏Ñ‡∏≥‡πÅ‡∏ô‡∏∞‡∏ô‡∏≥‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
mainTab.newLabel("1. ‡∏Ñ‡∏•‡∏¥‡∏Å‡πÇ‡∏´‡∏•‡∏î‡πÅ‡∏ï‡πà‡∏•‡∏∞‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡πà‡∏≠‡∏ô‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
mainTab.newLabel("2. ‡πÉ‡∏ä‡πâ Toggle ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö")
mainTab.newLabel("3. ‡∏õ‡∏£‡∏±‡∏ö‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏ï‡πà‡∏≤‡∏á‡πÜ ‡∏ï‡∏≤‡∏°‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£")
mainTab.newLabel("4. ‡∏Å‡∏î Space ‡∏´‡∏¢‡∏∏‡∏î Auto Fly ‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô")

-- ‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à
