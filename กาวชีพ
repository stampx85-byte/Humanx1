-- ==============================================
-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÅ‡∏ó‡πá‡∏ö‡πÉ‡∏´‡∏°‡πà‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö NOOBY HUNTER V6
-- ==============================================
local noobyTab = DrRayLibrary.newTab("Nooby Hunter V6", "rbxassetid://4483345998")

-- ==============================================
-- NOOBY HUNTER V6 SCRIPT (‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°)
-- ==============================================
noobyTab.newLabel("üéØ Nooby Hunter V6 - ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°")
noobyTab.newLabel("üéØ ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢: Nooby ‡πÅ‡∏•‡∏∞ Cutie Noob ‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô")
noobyTab.newLabel("üíÄ ‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡∏à‡∏ô‡∏ï‡∏≤‡∏¢ | üîÑ ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏° 2.5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏£‡∏∞‡∏ö‡∏ö Nooby Hunter
local noobyEnabled = false
local noobyToggle
local emergencyStop = false
local isActive = false
local isFollowing = false
local isKilling = false
local currentTarget = nil
local followConnection = nil
local flyTween = nil
local lastPosition = nil
local lastSearchTime = 0
local searchCooldown = 1.5
local targetFoundCount = 0
local lastTargetName = ""

-- ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏´‡∏≤ (‡πÄ‡∏à‡∏≤‡∏∞‡∏à‡∏á‡πÅ‡∏Ñ‡πà 2 ‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô)
local targetNames = {
    "nooby",
    "cutie noob"
}

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö UI
local Player, Character, RunService, Workspace, UserInputService, TweenService

-- Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö
noobyToggle = noobyTab.newToggle("‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Nooby Hunter", "‡∏•‡πà‡∏≤‡∏ï‡∏±‡∏ß Nooby ‡πÅ‡∏•‡∏∞ Cutie Noob ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", false, function(state)
    noobyEnabled = state
    isActive = state
    
    if state then
        DrRayLibrary:Notification("Nooby Hunter", "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πà‡∏≤ Nooby V6...", "OK")
    else
        stopAllActions()
        DrRayLibrary:Notification("Nooby Hunter", "‡∏´‡∏¢‡∏∏‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πà‡∏≤ Nooby", "OK")
    end
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡πÇ‡∏´‡∏•‡∏î‡∏£‡∏∞‡∏ö‡∏ö
noobyTab.newButton("‡πÇ‡∏´‡∏•‡∏î Nooby Hunter V6", "‡πÄ‡∏ï‡∏£‡∏µ‡∏¢‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πà‡∏≤ Nooby ‡πÅ‡∏•‡∏∞ Cutie Noob", function()
    Player = game:GetService("Players").LocalPlayer
    Character = Player.Character or Player.CharacterAdded:Wait()
    RunService = game:GetService("RunService")
    Workspace = game:GetService("Workspace")
    UserInputService = game:GetService("UserInputService")
    TweenService = game:GetService("TweenService")
    
    DrRayLibrary:Notification("Nooby Hunter V6", "‡∏£‡∏∞‡∏ö‡∏ö‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à! ‡∏Å‡∏î‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô", "OK")
end)

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤ Nooby ‡πÅ‡∏•‡∏∞ Cutie Noob ‡πÄ‡∏à‡∏≤‡∏∞‡∏à‡∏á
local function findTargetNPCs()
    local npcList = {}
    local foundCount = 0
    
    -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏ä‡∏∑‡πà‡∏≠‡∏ó‡∏µ‡πà‡πÄ‡∏à‡∏≤‡∏∞‡∏à‡∏á (‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÅ‡∏ö‡∏ö partial)
    for _, obj in ipairs(Workspace:GetChildren()) do
        if obj:IsA("Model") and obj ~= Character then
            local objName = string.lower(obj.Name)
            local isExactMatch = false
            
            for _, targetName in ipairs(targetNames) do
                if objName == targetName then
                    isExactMatch = true
                    break
                end
            end
            
            if isExactMatch then
                local hrp = obj:FindFirstChild("HumanoidRootPart")
                local humanoid = obj:FindFirstChildOfClass("Humanoid")
                
                if hrp and humanoid and humanoid.Health > 0 then
                    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÑ‡∏°‡πà‡πÉ‡∏ä‡πà‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
                    local isPlayer = false
                    for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
                        if obj == player.Character then
                            isPlayer = true
                            break
                        end
                    end
                    
                    if not isPlayer then
                        local distance = 0
                        if Character and Character:FindFirstChild("HumanoidRootPart") then
                            distance = (Character.HumanoidRootPart.Position - hrp.Position).Magnitude
                        end
                        
                        table.insert(npcList, {
                            Model = obj,
                            Name = obj.Name,
                            Position = hrp.Position,
                            Humanoid = humanoid,
                            Distance = distance
                        })
                        foundCount = foundCount + 1
                    end
                end
            end
        end
    end
    
    -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏ï‡∏¥‡∏°‡∏à‡∏≤‡∏Å Workspace.GetDescendants
    if foundCount == 0 then
        for _, obj in ipairs(Workspace:GetDescendants()) do
            if obj:IsA("Model") and obj ~= Character then
                local objName = string.lower(obj.Name)
                local isExactMatch = false
                
                for _, targetName in ipairs(targetNames) do
                    if objName == targetName then
                        isExactMatch = true
                        break
                    end
                end
                
                if isExactMatch then
                    local hrp = obj:FindFirstChild("HumanoidRootPart")
                    local humanoid = obj:FindFirstChildOfClass("Humanoid")
                    
                    if hrp and humanoid and humanoid.Health > 0 then
                        local alreadyAdded = false
                        for _, npc in ipairs(npcList) do
                            if npc.Model == obj then
                                alreadyAdded = true
                                break
                            end
                        end
                        
                        if not alreadyAdded then
                            local isPlayer = false
                            for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
                                if obj == player.Character then
                                    isPlayer = true
                                    break
                                end
                            end
                            
                            if not isPlayer then
                                local distance = 0
                                if Character and Character:FindFirstChild("HumanoidRootPart") then
                                    distance = (Character.HumanoidRootPart.Position - hrp.Position).Magnitude
                                end
                                
                                table.insert(npcList, {
                                    Model = obj,
                                    Name = obj.Name,
                                    Position = hrp.Position,
                                    Humanoid = humanoid,
                                    Distance = distance
                                })
                                foundCount = foundCount + 1
                            end
                        end
                    end
                end
            end
        end
    end
    
    return npcList
end

-- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î
local function findNearestTarget()
    if not Character or not Character:FindFirstChild("HumanoidRootPart") then
        return nil, "‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£", 0, nil
    end
    
    local currentTime = tick()
    if currentTime - lastSearchTime < searchCooldown then
        return nil, "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ñ‡∏π‡∏•‡∏î‡∏≤‡∏ß‡∏ô‡πå", 0, nil
    end
    
    lastSearchTime = currentTime
    local playerPos = Character.HumanoidRootPart.Position
    local allNPCs = findTargetNPCs()
    
    if #allNPCs == 0 then
        return nil, "‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢", 0, nil
    end
    
    local closestTarget = nil
    local closestHumanoid = nil
    local closestDistance = 5000
    local targetName = ""
    
    -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á‡πÅ‡∏•‡∏∞‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î
    for _, npc in ipairs(allNPCs) do
        if npc.Model and npc.Model.Parent then
            local hrp = npc.Model:FindFirstChild("HumanoidRootPart")
            if hrp and npc.Humanoid then
                local distance = (playerPos - hrp.Position).Magnitude
                if distance < closestDistance then
                    closestDistance = distance
                    closestTarget = npc.Model
                    closestHumanoid = npc.Humanoid
                    targetName = npc.Name
                end
            end
        end
    end
    
    return closestTarget, targetName, closestDistance, closestHumanoid
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÅ‡∏ö‡∏ö‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏° (‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏•‡∏á)
local function optimalWarpToTarget(target)
    if not target or not target:FindFirstChild("HumanoidRootPart") then
        return false
    end
    
    if not Character or not Character:FindFirstChild("HumanoidRootPart") then
        return false
    end
    
    local playerHrp = Character.HumanoidRootPart
    local targetHrp = target.HumanoidRootPart
    
    -- ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ö‡∏ô‡∏´‡∏±‡∏ß (‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏•‡∏∑‡∏≠ 2-3 ‡∏´‡∏ô‡πà‡∏ß‡∏¢ ‡πÅ‡∏ó‡∏ô‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÄ‡∏õ‡πá‡∏ô 10)
    local headPosition = targetHrp.Position + Vector3.new(0, 2.5, 0)  -- ‡∏•‡∏î‡∏à‡∏≤‡∏Å 10 ‡πÄ‡∏´‡∏•‡∏∑‡∏≠ 2.5
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡πÄ‡∏£‡∏≤‡∏à‡∏∞‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
    local raycastParams = RaycastParams.new()
    raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
    raycastParams.FilterDescendantsInstances = {Character, target}
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏à‡∏≤‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏´‡∏±‡∏ß‡∏•‡∏á‡∏°‡∏≤‡∏î‡πâ‡∏≤‡∏ô‡∏•‡πà‡∏≤‡∏á 5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢ (‡∏•‡∏î‡∏•‡∏á‡∏à‡∏≤‡∏Å 20)
    local raycastResult = workspace:Raycast(headPosition, Vector3.new(0, -5, 0), raycastParams)
    
    if raycastResult then
        -- ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ‡∏≠‡∏∞‡πÑ‡∏£‡∏Ç‡∏ß‡∏≤‡∏á‡∏î‡πâ‡∏≤‡∏ô‡∏•‡πà‡∏≤‡∏á (‡πÄ‡∏ä‡πà‡∏ô ‡∏û‡∏∑‡πâ‡∏ô) ‡πÉ‡∏´‡πâ‡∏≠‡∏¢‡∏π‡πà‡πÄ‡∏´‡∏ô‡∏∑‡∏≠‡∏°‡∏±‡∏ô 1 ‡∏´‡∏ô‡πà‡∏ß‡∏¢ (‡∏•‡∏î‡∏à‡∏≤‡∏Å 2)
        headPosition = Vector3.new(headPosition.X, raycastResult.Position.Y + 1, headPosition.Z)
    end
    
    -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏±‡∏ö‡πÅ‡∏•‡πâ‡∏ß
    local success = pcall(function()
        -- ‡πÉ‡∏ä‡πâ CFrame.New ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Ñ‡∏ß‡∏≤‡∏°‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î
        playerHrp.CFrame = CFrame.new(headPosition)
        
        -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
        playerHrp.Velocity = Vector3.new(0, 0, 0)
        playerHrp.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        playerHrp.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
        
        -- ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ä‡∏ô‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß
        playerHrp.CanCollide = false
        task.wait(0.05)
        playerHrp.CanCollide = true
    end)
    
    if success then
        print("[NOOBY HUNTER] ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏´‡∏±‡∏ß " .. target.Name .. " ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à (‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á: 2.5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢)")
        return true
    else
        print("[NOOBY HUNTER] ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß")
        return false
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡∏ö‡∏ô‡∏´‡∏±‡∏ß‡πÅ‡∏ö‡∏ö‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°
local function optimizedFollowAndKill(target, humanoid)
    if not target or not humanoid then return end
    
    isFollowing = true
    isKilling = true
    currentTarget = target
    
    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ PlatformStand ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏•‡∏≠‡∏¢‡πÑ‡∏î‡πâ
    if Character:FindFirstChild("Humanoid") then
        Character.Humanoid.PlatformStand = true
        Character.Humanoid:ChangeState(Enum.HumanoidStateType.Physics)
    end
    
    print("[NOOBY HUNTER] ‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÅ‡∏ö‡∏ö‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°: " .. target.Name)
    
    -- ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ö‡∏¥‡∏ô‡∏ñ‡πâ‡∏≤‡∏°‡∏µ
    if flyTween then
        flyTween:Cancel()
        flyTween = nil
    end
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á connection ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡∏ö‡∏ô‡∏´‡∏±‡∏ß‡πÅ‡∏ö‡∏ö‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á
    if followConnection then
        followConnection:Disconnect()
    end
    
    local stuckCheckCount = 0
    local damageCounter = 0
    
    followConnection = RunService.Heartbeat:Connect(function(deltaTime)
        if not isFollowing or not isActive or emergencyStop then
            return
        end
        
        if not target or not target.Parent or not target:FindFirstChild("HumanoidRootPart") then
            isFollowing = false
            isKilling = false
            if followConnection then
                followConnection:Disconnect()
            end
            return
        end
        
        if not Character or not Character:FindFirstChild("HumanoidRootPart") then
            return
        end
        
        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡∏°‡∏µ‡∏ä‡∏µ‡∏ß‡∏¥‡∏ï‡∏≠‡∏¢‡∏π‡πà‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
        if humanoid.Health <= 0 then
            print("[NOOBY HUNTER] ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ï‡∏≤‡∏¢‡πÅ‡∏•‡πâ‡∏ß: " .. target.Name)
            isFollowing = false
            isKilling = false
            if followConnection then
                followConnection:Disconnect()
            end
            return
        end
        
        local targetHrp = target.HumanoidRootPart
        local playerHrp = Character.HumanoidRootPart
        
        -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡∏ö‡∏ô‡∏´‡∏±‡∏ß (‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏•‡∏∑‡∏≠ 2.5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢)
        local targetPosition = Vector3.new(
            targetHrp.Position.X,
            targetHrp.Position.Y + 2.5,  -- ‡∏•‡∏î‡∏à‡∏≤‡∏Å 8 ‡πÄ‡∏´‡∏•‡∏∑‡∏≠ 2.5
            targetHrp.Position.Z
        )
        
        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏£‡∏≤‡∏≠‡∏¢‡∏π‡πà‡∏™‡∏π‡∏á‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
        local currentY = playerHrp.Position.Y
        local targetY = targetHrp.Position.Y
        
        if currentY - targetY > 5 then  -- ‡∏ñ‡πâ‡∏≤‡∏™‡∏π‡∏á‡∏Å‡∏ß‡πà‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏°‡∏≤‡∏Å‡πÄ‡∏Å‡∏¥‡∏ô 5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢
            -- ‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏•‡∏á
            targetPosition = Vector3.new(targetPosition.X, targetY + 1, targetPosition.Z)
            print("[NOOBY HUNTER] ‡∏õ‡∏£‡∏±‡∏ö‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏•‡∏á")
        end
        
        -- ‡πÉ‡∏ä‡πâ‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡πÅ‡∏ö‡∏ö Lerp ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏•‡∏∑‡πà‡∏ô‡πÑ‡∏´‡∏•
        local currentCFrame = playerHrp.CFrame
        local targetCFrame = CFrame.new(targetPosition)
        local smoothness = 0.8  -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡∏•‡∏∑‡πà‡∏ô
        
        local newCFrame = CFrame.new(
            currentCFrame.Position:Lerp(targetPosition, smoothness),
            targetHrp.Position
        )
        
        -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
        pcall(function()
            playerHrp.CFrame = newCFrame
            
            -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á
            playerHrp.Velocity = Vector3.new(0, 0, 0)
            
            -- ‡∏´‡∏°‡∏∏‡∏ô‡∏ï‡∏±‡∏ß‡πÉ‡∏´‡πâ‡∏´‡∏±‡∏ô‡∏´‡∏ô‡πâ‡∏≤‡πÑ‡∏õ‡∏ó‡∏≤‡∏á‡πÄ‡∏õ‡πâ‡∏≤
            playerHrp.CFrame = CFrame.new(playerHrp.Position, targetHrp.Position)
        end)
        
        -- ‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢ (‡∏ñ‡∏∑‡∏≠‡∏´‡∏°‡∏±‡∏î‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥)
        damageCounter = damageCounter + deltaTime
        if damageCounter >= 0.3 then -- ‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡∏ó‡∏∏‡∏Å 0.3 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
            -- ‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á
            humanoid:TakeDamage(20)  -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏™‡∏µ‡∏¢‡∏´‡∏≤‡∏¢
            
            -- ‡∏•‡∏≠‡∏á‡πÉ‡∏ä‡πâ RemoteEvents ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÇ‡∏à‡∏°‡∏ï‡∏µ (‡∏ñ‡πâ‡∏≤‡∏°‡∏µ)
            if Character:FindFirstChildOfClass("Tool") then
                local tool = Character:FindFirstChildOfClass("Tool")
                if tool:FindFirstChild("Handle") then
                    for _, v in pairs(tool:GetChildren()) do
                        if v:IsA("RemoteEvent") then
                            pcall(function()
                                v:FireServer(targetHrp.Position)
                            end)
                        end
                    end
                end
            end
            
            -- ‡∏•‡∏≠‡∏á‡πÉ‡∏ä‡πâ ClickDetector ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ
            if target:FindFirstChild("ClickDetector") then
                pcall(function()
                    target.ClickDetector:FireServer()
                end)
            end
            
            damageCounter = 0
        end
        
        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏ï‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà‡∏Å‡∏±‡∏ö‡∏ó‡∏µ‡πà‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
        local currentPos = playerHrp.Position
        if lastPosition then
            local distanceMoved = (currentPos - lastPosition).Magnitude
            if distanceMoved < 0.5 then  -- ‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏ô‡πâ‡∏≠‡∏¢‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ
                stuckCheckCount = stuckCheckCount + 1
                if stuckCheckCount > 10 then  -- ‡∏ï‡∏¥‡∏î‡∏°‡∏≤‡∏ô‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß
                    -- ‡∏û‡∏¢‡∏≤‡∏¢‡∏≤‡∏°‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÉ‡∏´‡∏°‡πà
                    pcall(function()
                        playerHrp.CFrame = CFrame.new(targetPosition)
                    end)
                    stuckCheckCount = 0
                end
            else
                stuckCheckCount = 0
            end
        end
        lastPosition = currentPos
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏£‡∏ß‡∏à‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà‡πÅ‡∏ö‡∏ö‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á
local function optimalExplore()
    if not Character or not Character:FindFirstChild("HumanoidRootPart") then
        return
    end
    
    -- ‡∏´‡∏¢‡∏∏‡∏î Tween ‡πÄ‡∏Å‡πà‡∏≤
    if flyTween then
        flyTween:Cancel()
        flyTween = nil
    end
    
    local hrp = Character.HumanoidRootPart
    local currentPos = hrp.Position
    
    -- ‡∏™‡∏∏‡πà‡∏°‡∏ó‡∏¥‡∏®‡∏ó‡∏≤‡∏á‡πÅ‡∏•‡∏∞‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á (‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏Å‡∏≤‡∏£‡∏™‡∏≥‡∏£‡∏ß‡∏à)
    local randomAngle = math.random() * math.pi * 2
    local randomDirection = Vector3.new(
        math.cos(randomAngle),
        0.2,  -- ‡∏•‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏Å‡∏≤‡∏£‡∏™‡∏≥‡∏£‡∏ß‡∏à‡∏•‡∏á
        math.sin(randomAngle)
    )
    
    local randomDistance = math.random(200, 500)  -- ‡∏•‡∏î‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á‡∏•‡∏á
    local targetPosition = currentPos + (randomDirection * randomDistance)
    
    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÑ‡∏°‡πà‡∏™‡∏π‡∏á‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ
    if targetPosition.Y > currentPos.Y + 20 then
        targetPosition = Vector3.new(targetPosition.X, currentPos.Y + 5, targetPosition.Z)
    end
    
    -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
    pcall(function()
        hrp.CFrame = CFrame.new(targetPosition)
        hrp.Velocity = Vector3.new(0, 0, 0)
    end)
    
    print("[NOOBY HUNTER] ‡∏™‡∏≥‡∏£‡∏ß‡∏à‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà‡πÉ‡∏´‡∏°‡πà‡∏≠‡∏¢‡πà‡∏≤‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°")
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏¢‡∏∏‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
local function stopAllActions()
    if followConnection then
        followConnection:Disconnect()
        followConnection = nil
    end
    
    if flyTween then
        flyTween:Cancel()
        flyTween = nil
    end
    
    if Character and Character:FindFirstChild("Humanoid") then
        Character.Humanoid.PlatformStand = false
        Character.Humanoid:ChangeState(Enum.HumanoidStateType.Running)
    end
    
    if Character and Character:FindFirstChild("HumanoidRootPart") then
        Character.HumanoidRootPart.CanCollide = true
    end
    
    isFollowing = false
    isKilling = false
    isActive = false
    currentTarget = nil
    lastPosition = nil
    emergencyStop = false
end

-- ‡∏•‡∏π‡∏õ‡∏´‡∏•‡∏±‡∏Å‡∏Ç‡∏≠‡∏á Nooby Hunter
local function noobyMainLoop()
    while isActive do
        if emergencyStop then
            DrRayLibrary:Notification("Nooby Hunter", "‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô! ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏£‡∏∞‡∏ö‡∏ö...", "OK")
            stopAllActions()
            task.wait(1)
            emergencyStop = false
        end
        
        if not Character or not Character:FindFirstChild("HumanoidRootPart") then
            task.wait(1)
            Character = Player.Character or Player.CharacterAdded:Wait()
            continue
        end
        
        -- ‡∏ñ‡πâ‡∏≤‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡∏´‡∏£‡∏∑‡∏≠‡∏Ü‡πà‡∏≤‡∏≠‡∏¢‡∏π‡πà ‡∏£‡∏≠‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤‡∏à‡∏∞‡πÄ‡∏™‡∏£‡πá‡∏à
        if isFollowing or isKilling then
            task.wait(0.2)
            continue
        end
        
        -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
        local target, name, distance, humanoid = findNearestTarget()
        
        if target and humanoid then
            if name ~= lastTargetName then
                lastTargetName = name
                targetFoundCount = targetFoundCount + 1
            end
            
            DrRayLibrary:Notification("Nooby Hunter", 
                string.format("‡∏û‡∏ö: %s\n‡∏£‡∏∞‡∏¢‡∏∞: %.0f ‡∏´‡∏ô‡πà‡∏ß‡∏¢", name, distance), "OK")
            
            -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÅ‡∏ö‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°
            local success = optimalWarpToTarget(target)
            
            if success then
                -- ‡∏£‡∏≠‡∏™‡∏±‡∏Å‡∏Ñ‡∏£‡∏π‡πà‡πÉ‡∏´‡πâ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÄ‡∏™‡∏£‡πá‡∏à
                task.wait(0.3)
                
                -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡πÅ‡∏•‡∏∞‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡∏ö‡∏ô‡∏´‡∏±‡∏ß‡πÅ‡∏ö‡∏ö‡πÉ‡∏´‡∏°‡πà
                if not isFollowing and target:FindFirstChild("HumanoidRootPart") and humanoid.Health > 0 then
                    optimizedFollowAndKill(target, humanoid)
                    DrRayLibrary:Notification("Nooby Hunter", 
                        string.format("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÇ‡∏à‡∏°‡∏ï‡∏µ: %s\n‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏° (2.5 ‡∏´‡∏ô‡πà‡∏ß‡∏¢)...", name), "OK")
                end
                
                -- ‡∏£‡∏≠‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏à‡∏∞‡∏ï‡∏≤‡∏¢
                local killStart = tick()
                while isFollowing and isActive and not emergencyStop do
                    if not target or not target.Parent or humanoid.Health <= 0 then
                        break
                    end
                    
                    -- ‡∏ñ‡πâ‡∏≤‡πÉ‡∏ä‡πâ‡πÄ‡∏ß‡∏•‡∏≤‡∏ô‡∏≤‡∏ô‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ ‡πÉ‡∏´‡πâ‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏•‡∏π‡∏õ
                    if tick() - killStart > 15 then -- ‡∏•‡∏î‡πÄ‡∏´‡∏•‡∏∑‡∏≠ 15 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡∏ï‡πà‡∏≠‡πÄ‡∏õ‡πâ‡∏≤
                        DrRayLibrary:Notification("Nooby Hunter", "‡πÉ‡∏ä‡πâ‡πÄ‡∏ß‡∏•‡∏≤‡∏ô‡∏≤‡∏ô‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢...", "OK")
                        break
                    end
                    
                    task.wait(0.3)
                end
                
                -- ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Ü‡πà‡∏≤‡πÄ‡∏™‡∏£‡πá‡∏à
                if not emergencyStop then
                    DrRayLibrary:Notification("Nooby Hunter", 
                        string.format("‡∏Ü‡πà‡∏≤‡πÄ‡∏™‡∏£‡πá‡∏à: %s\n‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÉ‡∏´‡∏°‡πà...", name), "OK")
                    stopAllActions()
                    task.wait(1.0)  -- ‡∏•‡∏î‡πÄ‡∏ß‡∏•‡∏≤‡∏£‡∏≠
                end
            else
                DrRayLibrary:Notification("Nooby Hunter", "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏•‡∏≠‡∏á‡πÉ‡∏´‡∏°‡πà...", "OK")
                task.wait(0.5)
            end
        else
            -- ‡∏™‡∏≥‡∏£‡∏ß‡∏à‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà‡πÉ‡∏´‡∏°‡πà‡πÅ‡∏ö‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°
            optimalExplore()
            
            -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏™‡∏≥‡∏£‡∏ß‡∏à‡πÄ‡∏™‡∏£‡πá‡∏à
            task.wait(2.0)  -- ‡∏•‡∏î‡πÄ‡∏ß‡∏•‡∏≤‡∏£‡∏≠
        end
    end
end

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö Nooby Hunter
noobyTab.newButton("‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡πà‡∏≤ Nooby", "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πà‡∏≤ Nooby ‡πÅ‡∏•‡∏∞ Cutie Noob ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", function()
    if not Player then
        DrRayLibrary:Notification("Nooby Hunter", "‡πÇ‡∏õ‡∏£‡∏î‡πÇ‡∏´‡∏•‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡πà‡∏≠‡∏ô!", "ERROR")
        return
    end
    
    isActive = true
    noobyEnabled = true
    noobyToggle.toggleBtn.Boolean.Value = true
    
    task.spawn(function()
        noobyMainLoop()
    end)
    
    DrRayLibrary:Notification("Nooby Hunter V6", "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πà‡∏≤ Nooby ‡πÅ‡∏•‡πâ‡∏ß!", "OK")
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô
noobyTab.newButton("‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô", "‡∏´‡∏¢‡∏∏‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏ó‡∏±‡∏ô‡∏ó‡∏µ", function()
    emergencyStop = true
    stopAllActions()
    noobyEnabled = false
    isActive = false
    noobyToggle.toggleBtn.Boolean.Value = false
    DrRayLibrary:Notification("Nooby Hunter", "‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î!", "OK")
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤
noobyTab.newButton("‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤", "‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡∏π‡∏•‡∏î‡∏≤‡∏ß‡∏ô‡πå‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤", function()
    lastSearchTime = 0
    DrRayLibrary:Notification("Nooby Hunter", "‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÅ‡∏•‡πâ‡∏ß!", "OK")
end)

-- Keybind ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô
noobyTab.newKeybind("‡∏Ñ‡∏µ‡∏¢‡πå‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô", "‡∏Å‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô", Enum.KeyCode.Space, function()
    emergencyStop = true
    stopAllActions()
    noobyEnabled = false
    isActive = false
    noobyToggle.toggleBtn.Boolean.Value = false
    DrRayLibrary:Notification("Nooby Hunter", "‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô‡∏î‡πâ‡∏ß‡∏¢‡∏Ñ‡∏µ‡∏¢‡πå!", "OK")
end)

-- Keybind ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö
noobyTab.newKeybind("‡∏Ñ‡∏µ‡∏¢‡πå‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î", "‡∏Å‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö", Enum.KeyCode.F, function()
    noobyEnabled = not noobyEnabled
    isActive = noobyEnabled
    noobyToggle.toggleBtn.Boolean.Value = noobyEnabled
    
    if noobyEnabled then
        DrRayLibrary:Notification("Nooby Hunter", "‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πà‡∏≤ Nooby!", "OK")
        task.spawn(function()
            noobyMainLoop()
        end)
    else
        stopAllActions()
        DrRayLibrary:Notification("Nooby Hunter", "‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πà‡∏≤ Nooby!", "OK")
    end
end)

-- Label ‡πÅ‡∏™‡∏î‡∏á‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥
noobyTab.newLabel("üìä ‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥‡∏Å‡∏≤‡∏£‡∏•‡πà‡∏≤")
noobyTab.newLabel("üéØ ‡∏û‡∏ö‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢: 0 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á")

-- Slider ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß (‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÉ‡∏ô‡∏≠‡∏ô‡∏≤‡∏Ñ‡∏ï)
noobyTab.newSlider("‡∏£‡∏±‡∏®‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤", "‡∏õ‡∏£‡∏±‡∏ö‡∏£‡∏∞‡∏¢‡∏∞‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢", 5000, true, function(value)
    DrRayLibrary:Notification("Nooby Hunter", "‡∏£‡∏±‡∏®‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤: " .. value, "OK")
end)

-- ==============================================
-- AUTO ATTACK SCRIPT (‡πÄ‡∏î‡∏¥‡∏°)
-- ==============================================
local attackEnabled = false
local attackToggle
local attackBtn

attackTab.newLabel("‚öîÔ∏è Auto Attack System")

attackToggle = attackTab.newToggle("‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Auto Attack", "‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", false, function(state)
    attackEnabled = state
    if attackBtn then
        attackBtn.Text = attackEnabled and "AUTO ATTACK: ON" or "AUTO ATTACK: OFF"
        attackBtn.BackgroundColor3 = attackEnabled and Color3.fromRGB(0, 150, 0) or Color3.fromRGB(150, 0, 0)
    end
end)

-- ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå Auto Attack
attackTab.newButton("‡πÇ‡∏´‡∏•‡∏î Auto Attack", "‡πÇ‡∏´‡∏•‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", function()
    local Player = game.Players.LocalPlayer
    local Character = Player.Character or Player.CharacterAdded:Wait()
    local RS = game:GetService("ReplicatedStorage")

    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á UI ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Auto Attack
    if game:GetService("CoreGui"):FindFirstChild("UltimateFix") then 
        game:GetService("CoreGui").UltimateFix:Destroy() 
    end
    
    local ScreenGui = Instance.new("ScreenGui", game:GetService("CoreGui"))
    ScreenGui.Name = "UltimateFix"
    ScreenGui.ResetOnSpawn = false
    
    attackBtn = Instance.new("TextButton", ScreenGui)
    attackBtn.Size = UDim2.new(0, 150, 0, 50)
    attackBtn.Position = UDim2.new(0.5, -75, 0.1, 0)
    attackBtn.Text = "AUTO ATTACK: OFF"
    attackBtn.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
    attackBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    attackBtn.TextSize = 14
    attackBtn.Font = Enum.Font.GothamBold
    
    local UICorner = Instance.new("UICorner")
    UICorner.CornerRadius = UDim.new(0, 8)
    UICorner.Parent = attackBtn
    
    attackBtn.MouseButton1Click:Connect(function()
        attackEnabled = not attackEnabled
        attackBtn.Text = attackEnabled and "AUTO ATTACK: ON" or "AUTO ATTACK: OFF"
        attackBtn.BackgroundColor3 = attackEnabled and Color3.fromRGB(0, 150, 0) or Color3.fromRGB(150, 0, 0)
        -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï toggle ‡πÉ‡∏ô DrRay UI
        attackToggle.toggleBtn.Boolean.Value = attackEnabled
    end)

    -- ‡∏•‡∏π‡∏õ Auto Attack
    task.spawn(function()
        while task.wait() do
            if attackEnabled then
                pcall(function()
                    local module_upvr = require(RS:WaitForChild("Module"):WaitForChild("Hitbox"))
                    
                    local Remote = nil
                    for _, v in pairs(Player.Backpack:GetDescendants()) do
                        if v.Name == "Combat_Event" then 
                            Remote = v 
                            break 
                        end
                    end
                    
                    if not Remote and Player.Character then
                        Remote = Player.Character:FindFirstChild("Combat_Event", true)
                    end

                    if Remote and Player.Character then
                        local Root = Player.Character.HumanoidRootPart
                        local Hum = Player.Character.Humanoid
                        
                        local TargetArea = module_upvr:Overlap({
                            MaxTarget = 100;
                            Humanoid = Hum;
                            CF = Root.CFrame;
                            Size = Vector3.new(200, 200, 200);
                        })

                        if #TargetArea > 0 then
                            Remote:FireServer(TargetArea)
                        end
                    end
                end)
            end
        end
    end)
    
    DrRayLibrary:Notification("Auto Attack", "‡∏£‡∏∞‡∏ö‡∏ö‡πÇ‡∏à‡∏°‡∏ï‡∏µ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!", "OK")
end)

attackTab.newSlider("‡∏£‡∏±‡∏®‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡πÇ‡∏à‡∏°‡∏ï‡∏µ", "‡∏õ‡∏£‡∏±‡∏ö‡∏Ç‡∏ô‡∏≤‡∏î‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà‡πÇ‡∏à‡∏°‡∏ï‡∏µ", 200, true, function(value)
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ç‡∏ô‡∏≤‡∏î‡∏Å‡∏≤‡∏£‡πÇ‡∏à‡∏°‡∏ï‡∏µ (‡∏Ñ‡∏∏‡∏ì‡∏ï‡πâ‡∏≠‡∏á‡∏õ‡∏£‡∏±‡∏ö‡πÇ‡∏Ñ‡πâ‡∏î‡πÉ‡∏ô‡∏•‡∏π‡∏õ‡πÉ‡∏´‡πâ‡πÉ‡∏ä‡πâ‡∏Ñ‡πà‡∏≤‡∏ô‡∏µ‡πâ)
    DrRayLibrary:Notification("‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡πà‡∏≤", "‡∏£‡∏±‡∏®‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡πÇ‡∏à‡∏°‡∏ï‡∏µ: " .. value, "OK")
end)

attackTab.newLabel("‚ö†Ô∏è ‡∏Ñ‡∏•‡∏¥‡∏Å‡πÇ‡∏´‡∏•‡∏î Auto Attack ‡∏Å‡πà‡∏≠‡∏ô‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")

-- ==============================================
-- AUTO FLY SCRIPT (‡πÄ‡∏î‡∏¥‡∏°)
-- ==============================================
local flyEnabled = false
local flyToggle
local flyTween = nil
local isFlying = false

flyTab.newLabel("ü™Ω Auto Fly System")

flyToggle = flyTab.newToggle("‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Auto Fly", "‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡∏≠‡∏¢‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", false, function(state)
    flyEnabled = state
    if not state and flyTween then
        flyTween:Cancel()
        flyTween = nil
        isFlying = false
    end
end)

-- ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå Auto Fly
flyTab.newButton("‡πÇ‡∏´‡∏•‡∏î Auto Fly", "‡πÇ‡∏´‡∏•‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡∏≠‡∏¢‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", function()
    local Player = game:GetService("Players").LocalPlayer
    local Character = Player.Character or Player.CharacterAdded:Wait()
    local RunService = game:GetService("RunService")
    local Workspace = game:GetService("Workspace")
    local TweenService = game:GetService("TweenService")
    
    -- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏£‡∏∞‡∏ö‡∏ö
    local flySpeed = 80
    local searchDistance = 5000
    
    -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤ NPC ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
    local function findAllNPCs()
        local npcList = {}
        
        for _, obj in ipairs(Workspace:GetDescendants()) do
            if obj:IsA("Model") and obj ~= Character then
                local hrp = obj:FindFirstChild("HumanoidRootPart")
                local humanoid = obj:FindFirstChildOfClass("Humanoid")
                
                if hrp and humanoid and humanoid.Health > 0 then
                    local isPlayer = false
                    for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
                        if obj == player.Character then
                            isPlayer = true
                            break
                        end
                    end
                    
                    if not isPlayer then
                        table.insert(npcList, {
                            Model = obj,
                            Name = obj.Name,
                            Position = hrp.Position,
                            Distance = 0
                        })
                    end
                end
            end
        end
        
        return npcList
    end
    
    -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î
    local function findNearestMonster()
        if not Character or not Character:FindFirstChild("HumanoidRootPart") then
            return nil, "‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£"
        end
        
        local playerPos = Character.HumanoidRootPart.Position
        local allNPCs = findAllNPCs()
        
        local closestMonster = nil
        local closestDistance = searchDistance
        local monsterName = ""
        
        for _, npc in ipairs(allNPCs) do
            if npc.Model and npc.Model.Parent then
                local hrp = npc.Model:FindFirstChild("HumanoidRootPart")
                if hrp then
                    local distance = (playerPos - hrp.Position).Magnitude
                    local npcName = string.lower(npc.Name)
                    
                    local isSlime = string.find(npcName, "slime")
                    local isKing = string.find(npcName, "king")
                    
                    if (isSlime or isKing) and distance < closestDistance then
                        closestDistance = distance
                        closestMonster = npc.Model
                        monsterName = npc.Name
                    end
                end
            end
        end
        
        if not closestMonster then
            for _, npc in ipairs(allNPCs) do
                if npc.Model and npc.Model.Parent then
                    local hrp = npc.Model:FindFirstChild("HumanoidRootPart")
                    if hrp then
                        local distance = (playerPos - hrp.Position).Magnitude
                        if distance < closestDistance then
                            closestDistance = distance
                            closestMonster = npc.Model
                            monsterName = npc.Name
                        end
                    end
                end
            end
        end
        
        return closestMonster, closestDistance, monsterName
    end
    
    -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡∏≠‡∏¢‡πÑ‡∏õ‡∏´‡∏≤‡∏°‡∏≠‡∏ô‡∏™‡πÄ‡∏ï‡∏≠‡∏£‡πå
    local function flyToTarget(target)
        if not target or not target:FindFirstChild("HumanoidRootPart") then
            return false
        end
        
        if not Character or not Character:FindFirstChild("HumanoidRootPart") then
            return false
        end
        
        if flyTween then
            flyTween:Cancel()
            flyTween = nil
        end
        
        local playerHrp = Character.HumanoidRootPart
        local targetHrp = target.HumanoidRootPart
        
        local targetPosition = targetHrp.Position + Vector3.new(0, 15, 0)
        local distance = (playerHrp.Position - targetPosition).Magnitude
        local travelTime = distance / flySpeed
        
        travelTime = math.clamp(travelTime, 1, 10)
        
        local tweenInfo = TweenInfo.new(
            travelTime,
            Enum.EasingStyle.Quad,
            Enum.EasingDirection.Out,
            0,
            false,
            0
        )
        
        flyTween = TweenService:Create(
            playerHrp,
            tweenInfo,
            {CFrame = CFrame.new(targetPosition)}
        )
        
        flyTween:Play()
        isFlying = true
        
        flyTween.Completed:Connect(function()
            isFlying = false
            flyTween = nil
        end)
        
        return true
    end
    
    -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡∏≠‡∏¢‡∏™‡∏≥‡∏£‡∏ß‡∏à‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà
    local function flyExplore()
        if not Character or not Character:FindFirstChild("HumanoidRootPart") then
            return
        end
        
        if flyTween then
            flyTween:Cancel()
            flyTween = nil
        end
        
        local hrp = Character.HumanoidRootPart
        local currentPos = hrp.Position
        
        local randomDirection = Vector3.new(
            math.random(-1, 1),
            0,
            math.random(-1, 1)
        ).Unit
        
        local randomDistance = math.random(300, 800)
        local targetPosition = currentPos + (randomDirection * randomDistance)
        
        local distance = (currentPos - targetPosition).Magnitude
        local travelTime = distance / (flySpeed * 0.5)
        
        travelTime = math.clamp(travelTime, 2, 8)
        
        local tweenInfo = TweenInfo.new(
            travelTime,
            Enum.EasingStyle.Quad,
            Enum.EasingDirection.Out
        )
        
        flyTween = TweenService:Create(
            hrp,
            tweenInfo,
            {CFrame = CFrame.new(targetPosition)}
        )
        
        flyTween:Play()
        isFlying = true
        
        flyTween.Completed:Connect(function()
            isFlying = false
            flyTween = nil
        end)
    end
    
    -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö Auto Fly
    task.spawn(function()
        while task.wait(0.5) do
            if flyEnabled then
                if not Character or not Character:FindFirstChild("HumanoidRootPart") then
                    task.wait(1)
                    Character = Player.Character or Player.CharacterAdded:Wait()
                    continue
                end
                
                if isFlying then
                    task.wait(0.5)
                    continue
                end
                
                local target, distance, name = findNearestMonster()
                
                if target then
                    local success = flyToTarget(target)
                    if success then
                        local waitTime = math.min(distance / flySpeed, 10)
                        task.wait(waitTime + 1)
                    else
                        task.wait(1)
                    end
                else
                    flyExplore()
                    task.wait(5)
                end
            end
        end
    end)
    
    -- ‡∏õ‡∏∏‡πà‡∏°‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô (Space)
    game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
        if not gameProcessed and input.KeyCode == Enum.KeyCode.Space and flyEnabled then
            if flyTween then
                flyTween:Cancel()
                flyTween = nil
                isFlying = false
            end
        end
    end)
    
    DrRayLibrary:Notification("Auto Fly", "‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡∏≠‡∏¢‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!", "OK")
    DrRayLibrary:Notification("‡∏Ñ‡∏≥‡πÅ‡∏ô‡∏∞‡∏ô‡∏≥", "‡∏Å‡∏î Space ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô", "OK")
end)

flyTab.newSlider("‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏Å‡∏≤‡∏£‡∏•‡∏≠‡∏¢", "‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏Å‡∏≤‡∏£‡∏•‡∏≠‡∏¢", 80, true, function(value)
    flySpeed = value
    DrRayLibrary:Notification("‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡πà‡∏≤", "‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏Å‡∏≤‡∏£‡∏•‡∏≠‡∏¢: " .. value, "OK")
end)

flyTab.newKeybind("‡∏Ñ‡∏µ‡∏¢‡πå‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô", "‡∏Å‡∏î‡∏Ñ‡∏µ‡∏¢‡πå‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏•‡∏≠‡∏¢", Enum.KeyCode.Space, function(key)
    if flyTween then
        flyTween:Cancel()
        flyTween = nil
        isFlying = false
    end
end)

flyTab.newLabel("‚ö†Ô∏è ‡∏Ñ‡∏•‡∏¥‡∏Å‡πÇ‡∏´‡∏•‡∏î Auto Fly ‡∏Å‡πà‡∏≠‡∏ô‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
flyTab.newLabel("üéØ ‡∏£‡∏∞‡∏ö‡∏ö‡∏à‡∏∞‡∏•‡∏≠‡∏¢‡∏´‡∏≤ Slime ‡πÅ‡∏•‡∏∞ King ‡∏Å‡πà‡∏≠‡∏ô")
flyTab.newLabel("üõë ‡∏Å‡∏î Space ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô")

-- ==============================================
-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á‡∏£‡∏∞‡∏ö‡∏ö‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏Å‡∏±‡∏ô
-- ==============================================
local mainTab = DrRayLibrary.newTab("Main Control", "rbxassetid://4483345998")

mainTab.newLabel("üéÆ ‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î")

mainTab.newButton("‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î", "‡πÄ‡∏õ‡∏¥‡∏î‡∏ó‡∏±‡πâ‡∏á Auto Attack ‡πÅ‡∏•‡∏∞ Auto Fly", function()
    attackEnabled = true
    flyEnabled = true
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï toggle ‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á
    if attackToggle then
        attackToggle.toggleBtn.Boolean.Value = true
    end
    if flyToggle then
        flyToggle.toggleBtn.Boolean.Value = true
    end
    
    if attackBtn then
        attackBtn.Text = "AUTO ATTACK: ON"
        attackBtn.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
    end
    
    DrRayLibrary:Notification("‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö", "‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!", "OK")
end)

mainTab.newButton("‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î", "‡∏õ‡∏¥‡∏î‡∏ó‡∏±‡πâ‡∏á Auto Attack ‡πÅ‡∏•‡∏∞ Auto Fly", function()
    attackEnabled = false
    flyEnabled = false
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï toggle ‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á
    if attackToggle then
        attackToggle.toggleBtn.Boolean.Value = false
    end
